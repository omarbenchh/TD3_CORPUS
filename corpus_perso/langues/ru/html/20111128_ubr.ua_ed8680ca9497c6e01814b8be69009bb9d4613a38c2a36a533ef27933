                <script type="text/javascript">
                    $(function () {
                        $(document).keypress(function (event) {
                            if (event.ctrlKey && (event.keyCode == '13' || event.keyCode == '10')) {
                                copySelection();
                            }
                        });
                    });

                    function copySelection() {
                        if (window.getSelection) {
                            selected = window.getSelection();
                        }
                        else if (document.getSelection) {
                            selected = document.getSelection();
                        }
                        else if (document.selection) {
                            selected = document.selection.createRange().text;
                        }
                        if ((selected != '') && (confirm('Редактору будут отправлены адрес страницы и выделенный фрагмент текста:  \n<<\n' + selected + ' \n>>\n Отправить данный фрагмент редактору?')));
                        {
                            var strError = String(selected);
                            $.post("/ajax/sendErrorReport", {strError: strError, newsId: 113584},
                                function (data, status) {
                                    if (status == 'failure') {
                                        alert('Произошла ошибка');
                                    }
                                    else if (status == 'success') {
                                        _gaq.push(['_trackEvent', 'TextBag', 'Send', 'TextInteraction', ,true]);
                                        alert('Спасибо! Письмо в редакцию было отправлено!');
                                    }
                                }, 'JSON');
                        }
                    }
                </script>


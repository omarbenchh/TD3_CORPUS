function ocw(url) {
   var left = Math.floor( (screen.width - 700) / 2);
   var top = Math.floor( (screen.height - 500) / 2);
   var winParms = "top=" + top + ",left=" + left + ",height=500,width=700";
   var win = window.open(url, 'ocw_win', winParms);
   if (parseInt(navigator.appVersion) >= 4) { win.window.focus(); }
   return win;
}

var initGraphs = false;
jQuery(window).scroll(function (event) {
    var scrollPos= jQuery(window).scrollTop();
	if(scrollPos>500 && initGraphs==false){
		initGraphs=true;
		middleWindow.run();
	}
});
</script>


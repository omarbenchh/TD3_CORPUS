<script type="text/javascript">
function open_window(link,w,h) //opens new window
 {
  var win = "width="+w+",height="+h+",menubar=no,location=no,resizable=yes,scrollbars=no";
  newWin = window.open(link,"newWin",win);
  newWin.focus();
 }
</script>



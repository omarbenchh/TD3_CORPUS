<script type="text/javascript">

$(document).ready(function(){


var obj = $('#sticky'); 
try{
var offset = obj.offset(); 
var topOffset = offset.top; //цифра, верх (там 501, иди 707)
var leftOffset = offset.left; 
var marginTop = obj.css("marginTop"); //цсс-свойство
var marginLeft = obj.css("marginLeft"); 
var smescheniye = 10 - topOffset;
} catch (e) {};


$(window).scroll(function() {
var hideobj = $('#hidesticky');
var hideoffset = hideobj.offset();
var hidetopOffset = hideoffset.top - 400; //Когда низ касается пряталки, но думаем, что верх

var scrollTop = $(window).scrollTop();

	if (scrollTop >= topOffset){
		obj.css({
			marginTop: smescheniye,
//			marginLeft: leftOffset,
			marginLeft: 0,
			position: 'fixed'
		});
	}

	if (scrollTop < topOffset){
		obj.css({
			marginTop: marginTop,
			marginLeft: marginLeft,
			position: 'relative'
		});
	}

	if (scrollTop >= hidetopOffset){
		obj.css({
			position: 'relative'
		});
	}

});

});

</script>


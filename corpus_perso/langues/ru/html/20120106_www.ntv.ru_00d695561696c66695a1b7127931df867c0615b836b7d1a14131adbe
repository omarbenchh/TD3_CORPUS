<script type="text/javascript">
// <![CDATA[

var serverTimeZ = 1453066279;
var localDate = new Date();

function setLocalTime() {
  localDate.setTime(serverTimeZ*1000);
  var moscowDate = new Date(localDate.getUTCFullYear(), localDate.getUTCMonth(), localDate.getUTCDate(),  
                            localDate.getUTCHours(), localDate.getUTCMinutes(), localDate.getUTCSeconds());
  moscowDate.setUTCMinutes(localDate.getUTCMinutes() + 240);
  var arcDate = "/"+moscowDate.getFullYear()+"/"+addZero(moscowDate.getMonth()+1)+"/"+addZero(moscowDate.getDate())+"/";
  $("#localTime").parent().attr("href",arcDate);
  var delta = parseInt($('.city_selected').attr('zo'));
  localDate = new Date(localDate.getUTCFullYear(), localDate.getUTCMonth(), localDate.getUTCDate(),  
                       localDate.getUTCHours(), localDate.getUTCMinutes(), localDate.getUTCSeconds());
  localDate.setUTCMinutes(localDate.getUTCMinutes() + delta*60); 
  $("#localTime").html(localDate.getUTCDate() + '.'+ addZero(localDate.getUTCMonth()+1) + ', ' + addZero(localDate.getHours()) + ":" + addZero(localDate.getMinutes()));
  setWeatherImage(localDate); 
}

function setWeatherImage(localDate){
 var hours =  localDate.getHours();
 if($("#pogoda").length > 0) {
  var sunset = $("#pogoda").attr("sunset");
  var sunrise = $("#pogoda").attr("sunrise");
  var st =  $("#pogoda i").attr('class');
  if(hours < sunrise || hours > sunset){
    var st =  $("#pogoda i").attr('class');
    var num =  st.substr(-1);    
      if("456".search(num) +1 > 0 ){
        $("#pogoda i").attr('class', st+"m");
      }
    }
  }
}

$(function() {

 $('#topsoc').load('/esi/like.jsp', function (){});
    
    
 $("#tab_soc").show();
  printUserGreeting();
  setLocalTime();

  $("#localTime").everyTime("20s", function() {
     localDate.setTime(localDate.getTime()+20*1000);
     $(this).html(localDate.getUTCDate() + '.'+ addZero(localDate.getUTCMonth()+1) + ', ' + addZero(localDate.getHours()) + ":" + addZero(localDate.getMinutes()));
  });
 
});

function addZero(value) {
   if(value < 10) return "0"+value;
   return value;
}

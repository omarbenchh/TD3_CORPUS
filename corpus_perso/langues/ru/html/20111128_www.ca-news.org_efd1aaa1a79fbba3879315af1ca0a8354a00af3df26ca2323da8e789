$(document).ready(function() {
	$('#addcommform').ajaxForm(function(data) {
		document.getElementById("comm-add").innerHTML=data;
	});
	
  	$('#autocarousel').infiniteCarousel({
		displayThumbnails:0,
		displayTime : 6000 
	});
  	$('#autocarousel').css('visibility','visible');
  	
	$("ul.sf-menu").superfish();
	$(".chmainimg").hover(function() {
		i=$(this).attr("id").split("-");
		$(".chmaininfo").hide();
		$(".chmaininfo"+i[1]).show();
		$(".chmainimg").removeClass("cmaintblhover");
		$(this).addClass("cmaintblhover");
	}, function(){
	});
	$(".mainbg").hover(function() {
		i=$(this).attr("id").split("-");
		$(".chmainnewsimg").hide();
		$("#chmainnewsimg-"+i[1]).show();
		$(".mainbg").removeClass("mainbghover");
		$(this).addClass("mainbghover");
	}, function(){
	});
	$(".proletter").click(function(){
		if($(this).hasClass("proletterfalse")) return false;
		var letter=$(this).attr("id").split("-");
		$(".pchars").hide();
		$(".pchar"+letter[1]).show();
		$(".proletter").removeClass("proletterselect");
		$("#proletter-"+letter[1]).addClass("proletterselect");
		return false;
	});
});

      function mon() {
	      document.getElementById('detailspopup').style.display = 'block';
      }
      function moff() {
	      document.getElementById('detailspopup').style.display = 'none';
      }
      function slidediv(linktosite,imagetoban,id){
        hideadv("detailspopup");
        document.getElementById("imgpopup").innerHTML="<a href='"+linktosite+"' target='_blank'><img src='"+imagetoban+"' alt='' \/><\/a>";
        divdesc=document.getElementById("detailspopup");
        var offset = $("#"+id).offset();
        advleft=offset.left-100;
        advtop=offset.top-30;
        if(advleft<202)advleft=202;
        divdesc.style.left=advleft+"px";
        divdesc.style.top=offset.top-30+"px";
      }
      function showadv(linktosite,imagetoban,id){
        slidediv(linktosite,imagetoban,id);
        $("#detailspopup").slideDown('fast');
      }
      function hideadv(divid){
        $("#"+divid).slideUp('fast');
      }
/* \popup link */

function show_hide(what){
        if(what=='popular'){
          $('#most-popular-list').css('display', 'block');
          $('#most-commented-list').css('display', 'none');
          $('#top-list-read').css('border-bottom', '3px solid #ff0000');
          $('#top-list-comm').css('border-bottom', '3px solid #0F4D92');
		  $('#top-list-read').css('color', '#ff0000');
          $('#top-list-comm').css('color', '#0F4D92');
        }
        else if(what=='commented'){
          $('#most-popular-list').css('display', 'none');
          $('#most-commented-list').css('display', 'block');
          $('#top-list-read').css('border-bottom', '3px solid #0F4D92');
          $('#top-list-comm').css('border-bottom', '3px solid #ff0000');
		  $('#top-list-read').css('color', '#0F4D92');
          $('#top-list-comm').css('color', '#ff0000');
        }
        else if(what=='sections'){
          divinform=document.getElementById("sections");
          if($('#sections').css('display')=='none'){
            divtest=document.getElementById("sections-btn");
            divinform.style.left=divtest.offsetLeft+"px";
            divinform.style.top=divtest.offsetTop+30+"px";
            divinform.style.display = 'block';
          }
          else if($('#sections').css('display') == 'block'){
            divinform.style.display = 'none';
          }
          
        }
        else if(what=="clock"){
          
        }
        else if(what=="smile"){
          $('#smiles').toggle();
        }
          
      }
</script>
<script type="text/javascript">
$(document).ready(function() {
    $("#topnav li").prepend("<span></span>"); //Throws an empty span tag right before the a tag
    
    $("#topnav li").each(function() { //For each list item...
        var linkText = $(this).find("a").html(); //Find the text inside of the a tag
        $(this).find("span").show().html(linkText); //Add the text in the span tag
    });
    
    $("#topnav .noactive").hover(function() {    //On hover...
        $(this).find("span").stop().animate({
            marginTop: "-35" //Find the span tag and move it up 40 pixels
        }, 250);
    } , function() { //On hover out...
        $(this).find("span").stop().animate({
            marginTop: "0" //Move the span back to its original state (0px)
        }, 250);
    });       
});

$(function() {

    var marquee = $("#marquee"); 
    marquee.css({"overflow": "hidden", "width": "100%", "padding": "4px 0", "background": "#EBF3FF"});

    // wrap "My Text" with a span (IE doesn't like divs inline-block)
   // marquee.wrapInner("<span>");
    marquee.find("span").css({ "width": "12%", "display": "inline-block", "text-align":"center", "margin":"0 auto" }); 
    marquee.append(marquee.find("span").clone()); // now there are two spans with "My Text"

    marquee.wrapInner("<div>");
    marquee.find("div").css("width", "750%");

    var reset = function() {
        $(this).css("margin-left", "0%");
		$(this).animate({ "margin-left": "-250%" }, 30000, 'linear', reset);
		
    };
    reset.call(marquee.find("div"));

});
</script>
<script type="text/javascript">
 
$(document).ready(function(){
	$("#showHideContent").click(function () {
		if ($(".content").is(":hidden")) { $(".content").css({"display":"table-row"});} 
		else {$(".content").css({"display":"none"});}
		return false;
	});
	});
</script>

      var ad_refresh;
      $(document).ready(function () {
          function play(){
              var $target = $('.doska_informer .item:first-child');
              $target.slideUp('slow', function() {
                  $target.appendTo('.doska_informer').show();
              });
          }
          $('.doska_informer').hover(function () {
              clearInterval(ad_refresh);
          }, function () {
              ad_refresh = setInterval(function () {
                  play();
              }, 2000);
          });
          ad_refresh = setInterval(function () {
              play();
          }, 2000);
      });
  </script>


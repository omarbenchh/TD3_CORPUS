  <script>
    function toggleautoblocks(name){
      if(name==1){
        $(".auto_block .auto_parts,.auto_block .more2").hide();$(".auto_block .auto_auto,.auto_block .more1").show();
        $(".auto_blocktabs .auto_tab").removeClass('auto_tab_sel');
        $(".auto_blocktabs .auto_tab_auto").addClass('auto_tab_sel');
      }
      else{
        $(".auto_block .auto_parts,.auto_block .more2").show();$(".auto_block .auto_auto,.auto_block .more1").hide();
        $(".auto_blocktabs .auto_tab").removeClass('auto_tab_sel');
        $(".auto_blocktabs .auto_tab_parts").addClass('auto_tab_sel');
      }
    }
  </script>

<script type="text/javascript">
$(document).ready(function() {
	$("#crosstop-enter").click(function(){ $("#crosstop-enter-form").toggle(); $("#crosstop-enter").toggleClass("crosstop-lnkselect"); return false; });
	$(window).scroll(function() {
		if ($(this).scrollTop() > 80){  
		  //$(".crosstop-logo").show(200);
			$(".crosstop").css("height","28px");
			$(".crosstop-inner").css("line-height","28px");
			$(".crosstop-search").css("top","1px");
			$("#showHideLastAnswer").css("margin","3px 0");
			$(".crosstop-auth").css("padding","3px 8px");
			

 		}else{
			//$(".crosstop-logo").hide(200); 
			$(".crosstop").css("height","40px");
			$(".crosstop-inner").css("line-height","40px");
			$(".crosstop-search").css("top","7px");
			$("#showHideLastAnswer").css("margin","10px 0");
			$(".crosstop-auth").css("padding","5px 8px");
			 
		}
	});  
	$("#crosstop-dropmenu").click(function(){
		var active = $(this).attr('data-active');
		if(active == 1){$(".crosstop-submenu").hide();$(this).attr('data-active', '0'); }
		else{$(".crosstop-submenu").show(); $(this).attr('data-active', '1'); }
	});
	$(".crosstop-submenu").mouseup(function(){return false});
	$("#crosstop-dropmenu").mouseup(function(){return false});
	$(document).mouseup(function(){$(".crosstop-submenu").hide(); $("#crosstop-dropmenu").attr('data-active', '');});
});
</script>


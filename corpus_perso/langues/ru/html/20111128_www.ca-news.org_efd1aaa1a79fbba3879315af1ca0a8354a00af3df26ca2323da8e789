<script type="text/javascript">
 
function insert(smile){
                tinymce.activeEditor.execCommand('mceInsertContent',false," "+smile+" ");
        }
function quote_comment(quote, comment){
	$('label[for="discus-input-text"]').hide();  
	var text=document.getElementById("discus-text-"+comment).innerHTML;
	tinymce.activeEditor.execCommand('mceInsertContent',false," "+"<cite class='mceNonEditable'>"+quote+"<br>"+text+"</cite><p></p>"); 
	$('#_comm_answer').val(comment);
	var top = $(".disqus-title").offset().top;
    $('body').animate( { scrollTop: top }, 2000 );
}
function quote_name(name, answer){
	$('label[for="discus-input-text"]').hide(); 
	tinymce.activeEditor.execCommand('mceInsertContent',false," "+"<cite class='mceNonEditable'><b>"+name+"</b></cite><p></p>"); 
	$('#_comm_answer').val(answer);
	var top = $(".disqus-title").offset().top;
    $('body').animate( { scrollTop: top }, 2000 );
}
 
  function insertAtCaret(objt, text) {
        obj=document.getElementById("comms");
  if(document.selection) {
   obj.focus();
   var orig = obj.value.replace(/\r\n/g, "\n");
   var range = document.selection.createRange();

   if(range.parentElement() != obj) {
    return false;
   }

   range.text = text;

   var actual = tmp = obj.value.replace(/\r\n/g, "\n");

   for(var diff = 0; diff < orig.length; diff++) {
    if(orig.charAt(diff) != actual.charAt(diff)) break;
   }

   for(var index = 0, start = 0;
    tmp.match(text)
     && (tmp = tmp.replace(text, ""))
     && index <= diff;
    index = start + text.length
   ) {
    start = actual.indexOf(text, index);
   }
  } else if(obj.selectionStart) {
   var start = obj.selectionStart;
   var end   = obj.selectionEnd;

   obj.value = obj.value.substr(0, start)
    + text
    + obj.value.substr(end, obj.value.length);
  }

  if(start != null) {
   setCaretTo(obj, start + text.length);
  } else {
   obj.value += text;
  }
 }

 function setCaretTo(obj, pos) {
  if(obj.createTextRange) {
   var range = obj.createTextRange();
   range.move('character', pos);
   range.select();
  } else if(obj.selectionStart) {
   obj.focus();
   obj.setSelectionRange(pos, pos);
  }
 }

        function xhs(id) {
            t=document.getElementById(id);
                if(t.style.display=='none') t.style.display='';
                else t.style.display='none'
            return false;
        }
$(document).ready(function() {  
	$('#noauth').click( function(event){  
		event.preventDefault();  
		$('#overlay').fadeIn(400, function(){ 
				$('#auth_comm').css('display', 'block').animate({opacity: 1, top: '30%'}, 200); 
		});
	});
	
	$('#modal_close, #overlay').click( function(){ 
		$('#auth_comm').animate({opacity: 0, top: '45%'}, 200, function(){  
			$(this).css('display', 'none');  
			$('#overlay').fadeOut(400);  
		});
	});
	 
}); 
function rating(_cid, _value)	{    
	var _sid =2;
	var _nid =840391;
	$("#_discus_rating_"+_cid).empty();
	 
	if (_sid!="") { 
		if (_nid!="") { 
			if (_cid!="") { 
			 	if (_value!="") {  
					$.ajax({
						type: "POST",
						url: "/aki130-discus/rating.php",
						data: {"sid":_sid ,"nid":_nid, "cid":_cid, "value":_value},
						success: function(data) {
							//location.reload();
							$("#_discus_rating_"+_cid).empty();
							if(data!="") { 
								$("#_discus_rating_"+_cid).append(data);
													}
						}
					});
				} 
			}
		}   
	}  
} 
function showHideEdit(_id) {
	if ($("#_edit_block_"+_id).is(":hidden")) { $("#_edit_block_"+_id).show("slow");} 
	else {$("#_edit_block_"+_id).hide("slow");}
}

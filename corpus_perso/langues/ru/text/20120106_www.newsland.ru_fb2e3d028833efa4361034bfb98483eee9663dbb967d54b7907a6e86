    <script type="text/javascript">
        $(document).ready(function(){
            $('.advert-tofix').each(function(){
                $(this).before('<div class="advert-tofix-holder"></div>');
            });

        });
        $(window).scroll(function(){
            $('.advert-tofix').each(function(){
                var el = $(this);
                var elHolder = el.prev('.advert-tofix-holder');
                var advertHeight = parseInt(el.outerHeight());
                var advertOffsetTop = elHolder.offset().top;
                var maxOffsetTop = $('#pageContent').offset().top + $('#pageContent').height() - advertHeight;
                var scrollValue = $(window).scrollTop();
                if(scrollValue>advertOffsetTop){
                    elHolder.height(advertHeight);
                    if(scrollValue>=maxOffsetTop){
                        el.removeClass('advert-fixed').addClass('advert-fixed-max');
                    }else{
                        el.removeClass('advert-fixed-max').addClass('advert-fixed');
                    }
                }else{
                    elHolder.height(0);
                    el.removeClass('advert-fixed').removeClass('advert-fixed-max');
                }
            });
        });
    </script>

        

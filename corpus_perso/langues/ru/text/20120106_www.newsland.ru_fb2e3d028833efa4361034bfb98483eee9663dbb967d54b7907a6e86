    function esc(s){
        for(var i=0,l=s.length,r='';i<l;i++){
            var c = s.charCodeAt(i);r += String.fromCharCode(c>1039&&c<1104 ? c-848 : c==1105 ? 184 : c==1025 ? 168 : c);
        }
        return r;
    }

				<script>console.log('/home/engine.new_git/website/views/scripts/index2013/header.phtml');</script>
<script>
	var usd = 0.0;
	var usd_diff = 0.0;
	var usd_html = '';
	var eur = 0.0;
	var eur_diff = 0.0;
	var eur_html = '';
	
	function exchange(){
		$.getJSON('http://www.vorle.ru/exchange')
			.success(function(data){
				//
				// USD 
				$('#usd_val').html(data['USD000UTSTOM']);
				
				if (usd == 0.0){
					usd = data['USD000UTSTOM'];
				}else if(usd != data['USD000UTSTOM']){
					usd_diff = Math.round((usd - data['USD000UTSTOM']) * 10000) / 10000;
					usd = data['USD000UTSTOM'];
				}
				//if (usd_diff == 0) usd_html = '<span style="color:#333;"><strong>0</strong></span>';
				if (usd_diff < 0) usd_html = '<span style="color:#FF0000;"><strong>'+usd_diff+'</strong></span>';
				if (usd_diff > 0) usd_html = '<span style="color:#008000;"><strong>+'+usd_diff+'</strong></span>';
				
				$('#usd_diff').html(usd_html);
				//
				// EUR
				$('#eur_val').html(data['EUR_RUB__TOM']);
				
				if (eur == 0.0){
					eur = data['EUR_RUB__TOM'];
				}else if(eur != data['EUR_RUB__TOM']){
					eur_diff = Math.round((eur - data['EUR_RUB__TOM']) * 10000) / 10000;
					eur = data['EUR_RUB__TOM'];
				}
				//if (eur_diff == 0) eur_html = '<span style="color:#333;"><strong>0</strong></span>';
				if (eur_diff < 0) eur_html = '<span style="color:#FF0000;"><strong>'+eur_diff+'</strong></span>';
				if (eur_diff > 0) eur_html = '<span style="color:#008000;"><strong>+'+eur_diff+'</strong></span>';
				
				$('#eur_diff').html(eur_html);
			})
			.error(function(jqXHR, textStatus, errorThrown) {
				console.log('error: '+textStatus);
				console.log("incoming Text: " + jqXHR.responseText);
			});
		setTimeout(exchange, 1000);
	}
	$(document).ready(function(){
		//exchange();
	});
</script>


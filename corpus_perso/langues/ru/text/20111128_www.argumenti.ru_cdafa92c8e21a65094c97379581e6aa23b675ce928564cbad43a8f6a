		  function loaderBlockComments(){
			var oDivLoading = $('#ajaxBlockComments div.loading');
			if (oDivLoading.length == 0) {
			  $('#ajaxBlockComments').prepend('<div class="loading"></div>');
			}
			$('#ajaxBlockComments div.loading').ajaxStart(function(){
				$(this).show();
			}).ajaxStop(function(){ 
				$(this).hide();
			}).ajaxError(function() {
			    alert('err connect');
			   $(this).text('Ошибка в соединении.'); $(this).hide();
			});
		  }		
		  
          function loadajaxBlockComments(){
			  var sResource = '';
			  if (location.href.indexOf('afisha') > -1){
			  	sResource = '&resource=afisha';
			  }else if (location.href.indexOf('events') > -1){
			  	sResource = '&resource=news';
			  }else if (location.href.indexOf('online') > -1){
			  	sResource = '&resource=online';
			  }else if (location.href.indexOf('opros') > -1){
			  	sResource = '&resource=opros';
			  }
            $('#ajaxBlockComments').load('/ajaxrequest/getblock/comments?targetId=27500946' + sResource);
          }

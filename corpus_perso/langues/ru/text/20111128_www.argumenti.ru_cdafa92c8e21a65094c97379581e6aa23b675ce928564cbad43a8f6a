			//Убираем битые ссылки
			// начать повторы с интервалом 2 сек
			var timerId = setInterval(function() {
				$('img[src="/132123"]').parent().parent().next().remove();
			 	$('img[src="/132123"]').parent().parent().remove();
			 	//console.log( $('img') );
			}, 1000);

			// через 5 сек остановить повторы
			setTimeout(function() {
				clearInterval(timerId);
			}, 5000);

  function refreshnewsfeed(a) {
   if(a!=currFeedType){
     currFeedType = a;
     $jQ(".lentaselector").removeClass("lentared");
     $jQ(".feed"+a).addClass("lentared");
     $jQ("#newsfeedblock").fadeIn("slow");
     JsHttpRequest.query(

<script>
if(typeof window.orientation === 'undefined'){
var move_to_top = false;
$jQ(document).scroll(function(){
    if ($jQ(window).scrollTop() > 100){
        if (move_to_top==false) {
            console.log('in');
            $jQ('#move-to-top').fadeIn('fast');
            move_to_top = true;
        }
    }else{
        if (move_to_top==true){
            console.log('out');
            $jQ('#move-to-top').fadeOut('fast');
            move_to_top = false;
        }
    }
}); 

$jQ('#move-to-top').click(function(){
    $jQ('html,body').animate({scrollTop: 0}, 1000);
});
}
</script>

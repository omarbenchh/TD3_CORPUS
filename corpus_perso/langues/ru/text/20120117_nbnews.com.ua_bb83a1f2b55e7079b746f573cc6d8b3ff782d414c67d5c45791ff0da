      <script language="javascript">
       var tid = 0;
       var onl = 0;
       var days = new Array('Воскресенье','Понедельник','Вторник','Среда','Четверг','Пятница','Суббота');
       var months = new Array('января','февраля','марта','апреля','мая','июня','июля','августа','сентября','октября','ноября','декабря');
       function _date(){ 
         var t = new Date();
         var _hours = t.getHours();if(_hours.toString().length==1){_hours='0'+_hours;} 
         var _mins = t.getMinutes();if(_mins.toString().length==1){_mins='0'+_mins;}
         document.getElementById('sdate').innerHTML='<nobr>'+days[t.getDay()]+', '+t.getDate()+' '+months[t.getMonth()]+' '+t.getFullYear()+
         ' <font class="timestr"> | '+_hours+':'+_mins+'</font>&nbsp;&nbsp;</nobr>';
         tid=window.setTimeout("_date()",1000)
              }
       tid=window.setTimeout("_date()",10);
      </script>

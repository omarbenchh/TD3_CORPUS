            var options = {
                scrollBy: 200,
                speed: 200,
                easing: 'easeOutQuart',
                scrollBar: 'scrollbar',
                dynamicHandle: 1,
                dragHandle: 1,
                clickBar: 1,
                mouseDragging: 1,
                touchDragging: 1,
                releaseSwing: 1
            };
            scroller1 = new Sly('#disqus_container_1', options);
            // Initiate frame
            scroller1.init();

            var toggleAnimationLoop = (function () {
                var loopId;
                var speed = 500;

                return function () {
                    if (loopId) {
                        loopId = clearTimeout(loopId);
                        scroller1.set(options);
                    } else {
                        var i = 0;
                        scroller1.set('easing', 'linear');
                        scroller1.set('speed', speed);
                        (function loop() {
                            i++;
                            loopId = setTimeout(loop, speed);
                            scroller1[i % 2 === 0 ? 'toStart' : 'toEnd']();
                        }());
                    }
                };
            })();
        }(jQuery));
    </script>

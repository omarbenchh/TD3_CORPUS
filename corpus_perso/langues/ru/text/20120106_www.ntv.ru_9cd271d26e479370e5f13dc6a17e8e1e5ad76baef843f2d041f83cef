    shuffle(banners);
    
    
    
    if(banners.length > 0) {
      var ids = "";
      for(i=0; i< 4 && i<banners.length; i++) {

        el = banners[i];
        if(ids != "") {
          ids = ids + ".";
        }
        ids = ids + $(el).attr("key");
        $("#tn_list").append(el);
      }
      $.ajax({ 
        url: "http://stat.ntv.ru/services/banners/show.jsp",
        data : { "ids" : ids},
        type : "post",
        crossDomain : true
      });


    }
  });
  // ]]>
  </script>
  


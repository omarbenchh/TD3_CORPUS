<div id="acContainer"></div>

<script type="text/javascript" src="http://img2.ntv.ru/js/jquery.editinplace.js"></script> 
<script type="text/javascript" src="http://img2.ntv.ru/js/messages.js"></script>
<script>


function isElementInViewport (el) {
    if (el instanceof jQuery) {
        el = el[0];
    }
    var rect = el.getBoundingClientRect();
    return (
        rect.top >= 0 &&
        rect.left >= 0 &&
        rect.bottom <= (window.innerHeight || document.documentElement.clientHeight) && /*or $(window).height() */
        rect.right <= (window.innerWidth || document.documentElement.clientWidth) /*or $(window).width() */
    );
}

function fireIfElementVisible (el, callback) {
    return function () {
        if ( isElementInViewport(el) ) {
            callback();
        }
    }
}

var str = window.location.href;
var ancor = str.split("#m");
var messageLoaded = false; 
// no ancor for current message
if(typeof ancor[1] == "undefined") {
    var handler = fireIfElementVisible ($("#acContainer"), loadMessages);
    //jQuery
    $(window).bind('DOMContentLoaded load resize scroll', handler); 
} else {
    jQuery(document).ready(function() {
        loadMessages();
    });	
}

function loadMessages() {
        if(messageLoaded) return;
        messageLoaded = true;
	$.ajax({
	    url: "/private/messagesList.jsp",
	    data: "key=JDJWJRJMKKJGJTJR-39B4503000E68A325F59D74416A5511B797A4123",
	    type: "GET",
	    async:   false,
	    success: function(result) {
	 	   $("#acContainer").html(result);
	 	   if( typeof ancor[1] != "undefined" ) {
	 	        $("#message_container2").show();
	 	        $("#openAllMsg").hide();
	 	        $.scrollTo("#m"+ancor[1],1000); 
	 	   }    
	    },
	}); 
}
</script>


    <script type="text/javascript">
    // <![CDATA[
        
    var tmp7 = 90;    
    
    shuffle = function(o){ 
      for(var j, x, i = o.length; i; j = parseInt(Math.random() * i), x = o[--i], o[i] = o[j], o[j] = x);
      return o;
    };
    
    function menuTopScroll() {
        //var tmp7 = $('#flamur1').height();
        //var p = $( "#topmenu");
        //var position = p.position(); 
        //tmp7 = position.top;
    }

    jQuery(document).ready(function() {
        $(window).bind('scroll', function() {
            
            if ($(window).scrollTop() > tmp7) {
	            
                $('#topmenuout').addClass('fixed');
              //  $("#topmenuout").offset({top:150});
                $('#newtop').addClass('fixed');
                
            } else {
                $('#topmenuout').removeClass('fixed');
                $('#newtop').removeClass('fixed');
            }

            if ($(window).scrollTop() > 200) {
                $('#topmenuout').addClass('scrollLogo');
                $('#topenter').hide();
            } else {
                $('#topmenuout').removeClass('scrollLogo');
                $('#topenter').show();
			      }

        });
        
      // get cook
      var defCity = null;
      
      var ccValue = getCookieValue("bcast");
      if(ccValue != undefined) {
         ccValEl = ccValue.split(".");
         if(ccValEl.length == 3) {
            ccId = parseInt(ccValEl[1]);
            el = $("#bc"+ccId);
            if(el.length > 0) {
                defCity = ccId;
                el.addClass("city_selected");
            }
         }
      } 
      if(defCity == null) {
          $("#bc1").addClass("city_selected");
      }

      var current_city = $('.city_selected').attr('cvalue');
      var current_zone = $('.city_selected').attr('zvalue');
    
      $('#city a div').html("<span>"+current_city+"<b></b></span>");
      $('#city a small').text(current_zone);
    
      $('.bc_item_name').click(function() {
         var val = parseInt($(this).attr("value"));
         $.post("/users/setcity/"+val+"/",function() {
              document.location.reload();
         });
      });

$('#city a').click(function() {
  $('#tn_list').slideToggle('slow', function() {
    $('#citylist').toggle();

  });
});

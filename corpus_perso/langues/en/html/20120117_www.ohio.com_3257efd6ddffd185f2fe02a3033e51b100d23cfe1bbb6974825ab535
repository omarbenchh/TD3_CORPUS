<script type='text/javascript'>
$(document).ready(function() {
	// Start Spinner loading call to wait for images to load
	$('#spinner-listings-loading').bind('ajaxSend', function() {
        $(this).show();
    }).bind('ajaxStop', function() {
        $(this).hide();
    }).bind('ajaxError', function() {
        $(this).hide();
    });
	// Start call to assign spinner to loaded item
	if ($('#realestate-listing-holder').length > 100) {
		//$('#spinner').show();
		$('#spinner-listings-loading').css('display', 'block');
	} else {
		//$('#spinner').hide();
		$('#spinner-listings-loading').css('display', 'none');
	}
	//$('.realestate-blue-flat-button').click(function() {
        //$('#spinner').show();
    //});
	// Shuffle all given listings in the array
	function ShuffleListings(o) {
		for(var j, x, i = o.length; i; j = parseInt(Math.random() * i), x = o[--i], o[i] = o[j], o[j] = x);
		return o;
	};
	// Define the array of listings to shuffle
	var realEstateListingsImages = $('.realestate-list-item-ohio');
	// Call the action to shuffle the listings
	ShuffleListings(realEstateListingsImages);	
	// jQuery to dump out new values to element with ID of 'realestate-listing-holder'
	$(function() {
		for (var i=0;i<realEstateListingsImages.length;i++) {
		$('#realestate-listing-holder').append(realEstateListingsImages[i]);
		}
		$('#realestate-listing-holder').css('display', 'block');
	});
});
</script>

      
    
    

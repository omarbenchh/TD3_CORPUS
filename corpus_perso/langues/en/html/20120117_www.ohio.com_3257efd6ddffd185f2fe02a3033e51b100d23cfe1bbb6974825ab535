<script type='text/javascript'>
jQuery('#special-section-promo-link').on('click', function () {
	if( /iPhone|iPad|iPod/i.test(navigator.userAgent) ) {
		// similar behavior as clicking on a link
		window.location.href = "https://itunes.apple.com/us/app/akron-beacon-journal/id849076494?mt=8";
	} else if ( /Android/i.test(navigator.userAgent) ) {
		// similar behavior as clicking on a link
		window.location.href = "https://play.google.com/store/apps/details?id=com.akronbeaconjournal.android.prod";
	} else {
		// similar behavior as clicking on a link
		window.location.href = "http://ohio.com/mobile";
	}
});
</script>

      
    
    
              

        <style>
  #slide-menu-trigger-area.fixed {
      position: fixed;
      width: 100%;
      top: 0;
      left: 0;
      z-index: 100;
  }
  
  #slide-panel-main.fixed {
      height:100%!important;
      position: fixed;
      top: 0;
  }
</style>

<script type='text/javascript'>
  //STICKY NAV
$(document).ready(function () {  
  var top = $('#slide-menu-trigger-area').offset().top - parseFloat($('#slide-menu-trigger-area').css('marginTop').replace(/auto/, 100));
  $(window).scroll(function (event) {
    // what the y position of the scroll is
    var y = $(this).scrollTop();

    // whether that's below the form
    if (y >= top) {
      // if so, ad the fixed class
      $('#slide-menu-trigger-area').addClass('fixed');
      $('#slide-panel-main').addClass('fixed');
    } else {
      // otherwise remove it
      $('#slide-menu-trigger-area').removeClass('fixed');
      $('#slide-panel-main').removeClass('fixed');
    }
  });
});
</script>

      
  

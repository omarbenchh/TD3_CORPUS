<script type="text/javascript">
  jQuery('document').ready(function() {
    jQuery('#searchForm').submit(function() {
      var scp = jQuery('#Select1').val();      
      if(scp==='Local Directory') {
        var keyword = jQuery('#SearchText').val();
        window.location="http://akronohio.planetdiscover.com/sp?keywords=" + keyword + "&skin=&aff=1109&Submit2=Go";        
        return false;
      }
    });   
  });
</script>

      
  
                                                                  </div>
                        </div><!-- END SEARCH INSERT CENTER -->
                    </div><!-- END SEARCH BAR WRAPPER -->
                </li>
                <li><a href="http://ohio.com"><i class="fa fa-home" style="margin-right:5px;font-size:17px;"></i>Home</a></li>

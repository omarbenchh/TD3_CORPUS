        <!-- <link type="text/css" rel="stylesheet" href="http://api.youneeq.ca/content/popper/style.css">

<script type="text/javascript" src="http://api.youneeq.ca/scripts/json2.js"></script>
<script type="text/javascript" src="http://api.youneeq.ca/scripts/detect_timezone.js"></script>
<script type="text/javascript" src="http://api.youneeq.ca/scripts/popper/tinybox.js"></script>
<script type="text/javascript" src="http://api.youneeq.ca/scripts/popper/script.js"></script>

<div id="youneeq_holder" class="youneeq_holder story-page-story-list" style="margin:10px auto;">
   <span id="miniteaserhead">RECOMMENDED FOR YOU</span>
  <div id="youneeq_list" class="four_stories linklist" style="padding-left:10px;"></div>
   <a href="#" class="recommended-modal" style="margin-left: auto; display: block; width: 121px"><img src="http://api.youneeq.ca/Content/images/powered-by-y.gif"></a><br class="clear" />
</div>
<script type="text/javascript">
$(document).ready(function () {
		$.getScript('http://api.youneeq.ca/app/yqmin',function() {
			Yq.onready(my_yq_init);
		});

   function on_yq_suggest(suggestions) {
      //check if there are suggested results, if so populate 'youneeq' div
      if (suggestions && suggestions.suggest && suggestions.suggest.node) {
         var nodeids = suggestions.suggest.node;
         var stories = '';

         //add links, and apply sidebar styles - this could also be handled with a list
         for (var i = 0; i < nodeids.length; i++) {
            var a_id = nodeids[i].id, a_title = nodeids[i].title, a_url = nodeids[i].url, a_img = nodeids[i].image;
            
            stories += '<div class="headline_story yq-article" data-yq-id="' + a_id + '"><a href="' + a_url
                     + '" class="yq-title"><img class="paid-dot" border="0" width="17" height="12" '
                     + 'src="/img/akron/paidDot.png" />' + a_title + '</a>' + (a_img
                     ? '<img style="border: 0; margin-right: 5px;" src="/img/akron/photo.png" />' : '')
                     + '</div><hr class="story_divider" />';
         }
         $('#youneeq_list').append(stories);
         
         $('#youneeq_holder .yq-article').each(function() {
                var yq_id = '' + $(this).data('yqId'), yq_title = Yq.titleTrim($('.yq-title', this).text()),
                yq_url = $('.yq-title', this).attr('href');
            $('a', this).on('mousedown.yq', function() {
               Yq.yq_panel_click(yq_url, yq_title, yq_id);
            });
         });
      }
   }
   
   function my_yq_init() {
      var title = '';
      var content_id = '';
      var categories = [];
      var description = '';
      var image_url = '';
      var pub_date = '';
      var canon_href = '';

      try {
         //populate title field, used with panel results option, with open graph title
         title = $('meta[property="og:title"]').attr('content');
         //the uuid after 'article_' in the url, if not correct then replace with appropriate code
         content_id = window.location.pathname.substr(window.location.pathname.lastIndexOf(".") - 1);
         image_url = $('meta[property="og:image"]').attr('content');
         description = $('meta[property="og:description"]').attr('content');
         pub_date = $('span.updated').attr('title');
		 if (!pub_date) {
		  pub_date = $('span.date').text();
		  pub_date = pub_date ? pub_date.replace(
			/Published:\s*([a-z0-9]+)\s+([0-9]+),\s+([0-9]+)/i, function(match, month, day, year) {
			  switch (month.substr(0, 3).toLowerCase())
			  {
				case 'dec': return year + '-12-' + day;
				case 'nov': return year + '-11-' + day;
				case 'oct': return year + '-10-' + day;
				case 'sep': return year + '-09-' + day;
				case 'aug': return year + '-08-' + day;
				case 'jul': return year + '-07-' + day;
				case 'jun': return year + '-06-' + day;
				case 'may': return year + '-05-' + day;
				case 'apr': return year + '-04-' + day;
				case 'mar': return year + '-03-' + day;
				case 'feb': return year + '-02-' + day;
				default: return year + '-01-' + day;
			  }
			}) : '2000-01-01';
		}
         canon_href = $('meta[property="og:url"]').attr('content');
         //this will build an array of the values between forward slashes in the path portion of the url, and then remove the last item
         categories = window.location.pathname.substr(1, window.location.pathname.length - 1).split('/');
         categories.pop();
      } catch(e) { console.log(e.description); title = ''; };
      title = content_id && categories ? title : '';
	  
      //content or not content page logic
      if (title.length > 0) { //if 'condition that's only true on content pages' then do this...                          
         Yq.observe({
            'domain': window.location.hostname, //get domain name dynamically
            'observe': [{
               'type': 'node',
               'name': content_id,
               'title': title, //required for 'is_panel_builder'
               'categories': categories,
               'description': description,
               'image': image_url,
               'create_date': pub_date
            }],
            //fetches 5 recommendations that can come from any site running the observe observable, with no category restrictions and returning content_id, url and title
            'suggest': [{'type': 'node', 'count': 5, 'categories': [], 'is_panel_detailed': 'true', 'isAllClientDomains': 'true'}],
            'alt_href': canon_href
         }, on_yq_suggest);
      }
      else { //if identifier not found (i.e. non-content page) do this...
         Yq.observe({
            'domain': window.location.hostname, //get domain name dynamically
            //fetches 5 recommendations that can come from any site running the observe observable, with no category restrictions and returning content_id, url and title
            'suggest': [{'type': 'node', 'count': 5, 'categories': [], 'is_panel_detailed': 'true', 'isAllClientDomains': 'true'}]
         }, on_yq_suggest);
      }
   }
}); 
</script> -->

      
    
    

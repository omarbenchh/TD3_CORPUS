<script type="text/javascript">
jQuery(document).ready (function() {
	var triggers = jQuery('#blogs-promo-wrapper ul.triggers li');
	var images = jQuery('.our-blogs-promo ul.images li');
	var lastElem = triggers.length-1;
	//var lastElem = triggers.height-1;
	var blogsPromo = jQuery('.our-blogs-promo ul.images');
	var imgHeight = images.height();
	//var imgWidth = images.width();
	var target;

	triggers.first().addClass('selected');
	//blogsPromo.css('width', imgWidth*(lastElem+1) +'px');
	blogsPromo.css('height', imgHeight*(lastElem+1) +'px');

	function sliderResponse(target) {
		//blogsPromo.stop(true, false).animate({opacity: 1}, {queue: false, duration: 'fast'});
		blogsPromo.stop(true, false).animate({'top':'-'+ imgHeight*target +'px'}, 1000, 'easeInOutExpo');
		//blogsPromo.stop(true,false).animate({'left':'-'+ imgWidth*target +'px'}, 1000, 'easeInOutBack');
		//blogsPromo.stop(true,false).fadeIn(600).animate({'top':'-'+ imgHeight*target +'px'}, 1000, 'easeInOutBack');
		triggers.removeClass('selected').eq(target).addClass('selected');
	}

	triggers.click(function() {
		if ( !jQuery(this).hasClass('selected') ) {
			target = jQuery(this).index();
			sliderResponse(target);
			resetTiming();
		}
	});
	jQuery('#blogs-promo-wrapper .next').click(function() {
		target = jQuery('#blogs-promo-wrapper ul.triggers li.selected').index();
		target === lastElem ? target = 0 : target = target+1;
		sliderResponse(target);
		resetTiming();
	});
	jQuery('#blogs-promo-wrapper .prev').click(function() {
		target = jQuery('#blogs-promo-wrapper ul.triggers li.selected').index();
		lastElem = triggers.length-1;
		target === 0 ? target = lastElem : target = target-1;
		sliderResponse(target);
		resetTiming();
	});
	function sliderTiming() {
		target = jQuery('#blogs-promo-wrapper ul.triggers li.selected').index();
		target === lastElem ? target = 0 : target = target+1;
		sliderResponse(target);
	}
	var timingRun = setInterval(function() { sliderTiming(); },6000);
	jQuery('#blogs-promo-wrapper').hover( function () {
		clearInterval(timingRun)
    },
	function resetTiming() {
		clearInterval(timingRun);
		timingRun = setInterval(function() { sliderTiming(); },6000);
	});
});
</script>

      
    
    

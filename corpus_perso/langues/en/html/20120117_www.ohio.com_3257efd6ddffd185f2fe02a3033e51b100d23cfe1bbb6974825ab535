<script type="text/javascript">
jQuery(document).ready (function() {
	// VIDEO PROMO GRAY BAR SCRIPT AND TIMING STARTS HERE
	jQuery("#video-promo-text-top").animate({
		backgroundColor: '#dedede',
		opacity: '1'
	}, 1300);
	jQuery("#video-promo-play-button").animate({
		opacity: '1'
	}, 1300);
	// VIDEO PROMO GRAY BAR SCRIPT AND TIMING ENDS HERE
	// VIDEO PROMO BAR TEXT SCRIPT AND TIMING STARTS HERE
	setTimeout( function () {
	function fadeInSequence($one) {
		//default to the first three list items
		if ($one == null) $one = jQuery('.video-promo-text-section > li:lt(1)');

		//fade in the 3 found
		$one.show(1600, function () {
			//fade out the 3 after fadein and start
			$(this).fadeOut(2000).delay(400);
		}).promise().done(function () {
			if ($one.last().nextAll(':lt(1)').length) {
				//Same function, next 3 items
				fadeInSequence($one.last().nextAll(':lt(1)'));
			} else {
				//Same function, from the beginning if none are left
				//fadeInSequence();
				jQuery('.video-promo-text-section').css("display", "none");
				jQuery('#animate-text-stop').show(1300);
			}
		});
	}

	jQuery(function () {
		//start the infinite looping
		fadeInSequence();
	});
	}, 2000);
	// VIDEO PROMO BAR TEXT SCRIPT AND TIMING ENDS HERE
	// VIDEO PROMO SAMPLE VIDEOS ROTATION SCRIPT AND TIMING STARTS HERE
	var triggers = jQuery('#video-promo-wrapper ul.triggers li');
	var images = jQuery('.our-video-promo ul.images li');
	var lastElem = triggers.length-1;
	//var lastElem = triggers.height-1;
	var videoPromo = jQuery('.our-video-promo ul.images');
	var imgHeight = images.height();
	//var imgWidth = images.width();
	var target;

	triggers.first().addClass('selected');
	//videoPromo.css('width', imgWidth*(lastElem+1) +'px');
	videoPromo.css('height', imgHeight*(lastElem+1) +'px');

	function sliderResponse(target) {
		//videoPromo.stop(true, false).animate({opacity: 1}, {queue: false, duration: 'fast'});
		videoPromo.stop(true, false).animate({'top':'-'+ imgHeight*target +'px'}, 1500, 'easeInOutExpo');
		//videoPromo.stop(true,false).animate({'left':'-'+ imgWidth*target +'px'}, 1000, 'easeInOutBack');
		//videoPromo.stop(true,false).fadeIn(600).animate({'top':'-'+ imgHeight*target +'px'}, 1000, 'easeInOutBack');
		triggers.removeClass('selected').eq(target).addClass('selected');
	}

	triggers.click(function() {
		if ( !$(this).hasClass('selected') ) {
			target = $(this).index();
			sliderResponse(target);
			resetTiming();
		}
	});
	jQuery('#video-promo-wrapper .next').click(function() {
		target = jQuery('#video-promo-wrapper ul.triggers li.selected').index();
		target === lastElem ? target = 0 : target = target+1;
		sliderResponse(target);
		resetTiming();
	});
	jQuery('#video-promo-wrapper .prev').click(function() {
		target = jQuery('#video-promo-wrapper ul.triggers li.selected').index();
		lastElem = triggers.length-1;
		target === 0 ? target = lastElem : target = target-1;
		sliderResponse(target);
		resetTiming();
	});
	function sliderTiming() {
		target = jQuery('#video-promo-wrapper ul.triggers li.selected').index();
		target === lastElem ? target = 0 : target = target+1;
		sliderResponse(target);
	}
	var timingRun = setInterval(function() { sliderTiming(); },6000);
	jQuery('#video-promo-wrapper').hover( function () {
		clearInterval(timingRun)
    },
	function resetTiming() {
		clearInterval(timingRun);
		timingRun = setInterval(function() { sliderTiming(); },6000);
	});
	// VIDEO PROMO SAMPLE VIDEOS ROTATION SCRIPT AND TIMING ENDS HERE
});
</script>

      
  
                                                                                                                          </div><!-- END INSIDE CONTENT -->
                                </div><!-- END STORY RIGHT COLUMN RENDER -->
                        </div>
                    </div><!-- END CENTER STORY COLUMN -->
                    </div><!-- END STORY TWO COLUMN WRAPPER -->
                                

<script>


function election(saveData) {
    if (saveData == null) {
            alert("DATA IS UNDEFINED!");  // displays every time
    }
    //alert("Success is " + saveData);  // 'Success is undefined'
}

    var stp = '';

    function funcresults()
            {


                     
					  var election_callback='e'; 



						
						 
                $.ajax({                   
          

                    url: 'http://ed-nm.ibnlive.in.com/electiondata/electionjson/assemblyelections/civicpoll/gujarat_mnc.json',
                    type: 'GET',
                    dataType: 'jsonp',
                    jsonp: 'jsonp',
                    jsonpCallback: election_callback,
                    crossDomain: true,
					cache: true ,
				    success: function(datasl) {
                        if(datasl){
                     

                      $('#elecn').html(datasl);
					 
					
					  
                     clearInterval(stp);
                      stp = setTimeout('funcresults();', 15000);
                  }
                    },
                    error:function(){
                       //console.log('something went wrong!');
                    }
                });
						
						
 }
       

   
</script> 


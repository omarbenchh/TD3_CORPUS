             <script type="text/javascript">
function validater()
{


if(document.submitx.email.value=='')
{
alert('Please Enter Email  ');
document.submitx.email.focus();
return false;
}

 if(document.submitx.email.value!=""||document.submitx.email.value!=" ")
	{	
		var str=document.submitx.email.value;
	  	var filter=/^.+@.+\..{2,3}$/;
		if(!filter.test(str))
		{
		alert('Please Enter the Valid E-mail  ');
		document.submitx.email.focus();
		return false;
		}
}

	
	
    
	


        $('form').on('submit', function (e) {

          e.preventDefault();

          $.ajax({
            type: 'post',
            url: 'ajaxnewsletter.php',
            data: $('form').serialize(),
            success: function (response) {
				$('#itext2').show();
				$('#itext2').html(response);
             $('#itext').hide();

            }
          });

        });


	
	
return true;

}
</script>

<script>
var countre=1;
$(window).scroll(function(event) {
    var sere=$(".footer").offset().top < $(window).scrollTop() + $(window).outerHeight();
	
	if(sere=='1'){
	
	if(countre==1){
	$('#left_nav').css('margin-top','-560px');
	$('.fixed_header').css('z-index','10');
	}
	countre+1;
	}else{
	

	$('#left_nav').css('margin-top','0px');
	
	$('.fixed_header').css('z-index','4');

	}
	
});

</script>

    var y7modules = y7modules || [];
    y7modules.push({
        id: "y7module569c0715f12d2",
        module: "carousel",
        config: {
            highlightItem: false,
            useSwipe: false,
            infiniteScroll: false
        },

        init: function (Y) {
            this.config.id = this.id;
            this.config.contentBox = Y.one("#"+this.id);
            var carousel = new Y.Carousel(this.config);
            carousel.render();
        }
    });
</script> 


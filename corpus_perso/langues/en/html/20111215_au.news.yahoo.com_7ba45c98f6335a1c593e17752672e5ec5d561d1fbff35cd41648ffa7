            highlightItem: false,
            useSwipe: false,
            infiniteScroll: false
        },

        init: function (Y) {
            this.config.id = this.id;
            this.config.contentBox = Y.one("#"+this.id);
            var carousel = new Y.Carousel(this.config);
            carousel.render();
        }
    });
</script> 

<!-- property: aunews_wan | module: multipass-carousel | flavour: CAROUSEL-BIG | health: ok -->
	
	
	
	
	
	
	
	
	


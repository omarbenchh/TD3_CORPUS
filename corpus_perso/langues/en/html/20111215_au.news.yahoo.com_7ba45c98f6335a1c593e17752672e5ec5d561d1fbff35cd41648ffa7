        DARLA_CONF.waterfall = 0;
        DARLA_CONF.onPosMsg = function (msg_name, data)  {
            var ele = document.getElementById("box-" + data).parentNode,
            pos = $sf.host.get(data),
            posWidth = pos.conf.w,
            posHeight = pos.conf.h,
            posName = pos.conf.id,
            cls = (posName == "NT1") ? "" : " ad-expand";
            function hasClass(el, cls) {
                                    if (cls == "") {
                                            return false;
                                    }
                return el.className && new RegExp("(\s*|^)" + cls + "(\s|$)").test(el.className);
            }
            if(msg_name == "collapse") {
                if (hasClass(ele,cls)) {
                    var reg = new RegExp("(\s|^)"+ cls +"(\s|$)");
                    ele.className=ele.className.replace(cls, "");
                }
            }
            if(msg_name == "exp-ovr" || msg_name == "exp-push") {
                if (!hasClass(ele,cls)) {
                    ele.className += cls;
                }
            }
            if (pos == "NT1" && posWidth == "970") {
                ele.className=ele.className.replace("NT1", "MAST");
            }
        };
        if (window.DARLA) { 
            if (DARLA.config(DARLA_CONF) == "OK") {
                DARLA.render();
            }
        }

  $(document).ready(function() {
    var gcse, s,
    cx = gcseConfig.cx || false,
    pre = gcseConfig.pre || false,
    container = gcseConfig.container || '#site-search',
    resultsContainer = gcseConfig.resultsContainer || '.gcse-results',
    isResultPage = $('body.search-results').length && $(resultsContainer).append(gcseResults) || false,
    drawLightForm = gcseConfig.drawLightForm || false,
    gcseResultsUrl = gcseConfig.resultsUrl || '/search-results',
    gcseResults = '<div class="gcse-search"></div>',
    gcseBox = '<div class="gcse-searchbox-only" data-resultsUrl="' + gcseResultsUrl + '"> </div>';

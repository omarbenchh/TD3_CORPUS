    		    googletag.defineOutOfPageSlot('/5479/ctv.ctvnews/home/stories','div-gpt-ad-outofpage-1-oop').addService(googletag.pubads()).addService(googletag.companionAds());
          
                                                                                                                                                      
                                                                                                                                                          

        Requests = {
            QueryString: function (item) {
                var svalue = location.search.match(new RegExp("[\?\&]" + item + "=([^\&]*)(\&?)", "i"));
                return svalue ? svalue[1] : svalue;
            }
        }

        var destCode = Requests.QueryString('destinationCode');
        if (destCode) { axisAuthApi_pageCfg.destinationCode = destCode; }


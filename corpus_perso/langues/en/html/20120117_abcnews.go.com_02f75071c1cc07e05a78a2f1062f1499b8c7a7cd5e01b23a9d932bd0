s_omni.pageURL = ""; //content url
s_omni.pageType = ""; //page type
s_omni.server = window.document.location.hostname; //reporting domain
s_omni.prop4 = "story"; //content type
s_omni.prop1 = "abcn"; //site name
s_omni.channel = "abcn:news"; //level1
s_omni.prop5 = "abcn:news"; //level2
s_omni.prop6 = "abcn:news::"; //level3
s_omni.prop13 = "";
s_omni.prop16 = "";
s_omni.prop18 = "15369979:blog_man in stable condition after $44 million hospital bill";
s_omni.prop24 = ""; //top story
s_omni.prop25 = ""; //top video
s_omni.prop26 = ""; //top slideshow
s_omni.prop37 = "";
s_omni.hier1 = "abcn:news"; //directory structure
s_omni.eVar20 = s_omni.prop13; //columnist
s_omni.eVar16 = s_omni.pageName; //content name conversion
s_omni.eVar17 = s_omni.prop4+":"+s_omni.channel;var arecookiesEnabled = navigator.cookieEnabled;
var tryLocalStorage = function() {
try {
localStorage.setItem("testLS", "test");
localStorage.removeItem("testLS");
return true;
} catch(e) {
return false;
}
}
var thirtyMinutesLater = new Date();
thirtyMinutesLater.setMinutes(thirtyMinutesLater.getMinutes() + 30);
var setTrackID = function() {
// set setTime for protection
setTimeout(function() {
if (s_omni.prop14) {
var prevPageTrack = s_omni.prop14;
if (tryLocalStorage) {
memcachejs.set("abcnd-prev-page", prevPageTrack, 1800);
//localStorage.setItem('abcnd-prev-page', prevPageTrack);
} else if (arecookiesEnabled) {
createCookie('abcnd-prev-page',prevPageTrack,1);
}
} else {
// clear
if (tryLocalStorage) {
memcachejs.remove("abcnd-prev-page");
} else if (arecookiesEnabled) {
document.cookie = 'abcnd-prev-page=; expires='+thirtyMinutesLater.toGMTString()+';';
}
}
}, 1000);
}
$(document).ready(function(){
$(document).on("click", 'a[name^=lpos]', function(e) {
var n = $(this).attr("name");
if (tryLocalStorage) {
memcachejs.set("abcnd-link-track", n, 1800);
} else if (arecookiesEnabled) {
createCookie('abcnd-link-track',n,1);
}
});
});
var linkTrack = false;
if (tryLocalStorage) {
linkTrack = memcachejs.get("abcnd-link-track");
} else if (arecookiesEnabled) {
linkTrack = readCookie('abcnd-link-track');
}
if (linkTrack) {
var lpos = linkTrack.split("lpos=");
if (lpos.length == 2) {
if(typeof s_omni != "undefined"){
// lpos[1] is the name= property in the anchor tag.
s_omni.prop9 = lpos[1];
}
}
}
// do this again for prop12, even if click tracking doesn't occur (back button, forward button)
if (tryLocalStorage) {
memcachejs.remove("abcnd-link-track");
// if previous page is set, add to to prop12
if (memcachejs.get('abcnd-prev-page')) {
s_omni.prop12 = memcachejs.get('abcnd-prev-page')
memcachejs.remove("abcnd-prev-page");
}
} else if (arecookiesEnabled) {
document.cookie = 'abcnd-link-track=; expires='+thirtyMinutesLater.toGMTString()+';';
// if previous page is set, add to to prop12
if (readCookie('abcnd-prev-page')) {
s_omni.prop12 = readCookie('abcnd-prev-page');
document.cookie = 'abcnd-prev-page=; expires='+thirtyMinutesLater.toGMTString()+';';
}
}
// set the current page in ls/cookie format
setTrackID();
function slideLoadJS(u, c) {
var d = document, t = 'script',
o = d.createElement(t),
s = d.getElementsByTagName(t)[0];
o.src = '//' + u;
if (c) { o.addEventListener('load', function (e) { c(null, e); }, false); }
s.parentNode.insertBefore(o, s);
}
window.bkInit = function(s_omni_override) {

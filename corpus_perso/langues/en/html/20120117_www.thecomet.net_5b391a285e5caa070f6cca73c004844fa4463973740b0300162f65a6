		<script type="text/javascript">
			function loadCommentsDynamic(json) {
				if( json.loginStatus.email ) {
					$('#frm-email-input').val( json.loginStatus.email );
				}
				if (json.loginStatus.loggedIn) {
					$('.loggedout').empty();
					$('#username').text( json.loginStatus.userName );
				} else {
					$('.loggedin').empty();
				}
			}
		</script>
	 <!-- End Message Comment Form -->  <div id="article-bottom">
    
                  
    
  
    
  
    
    
        
	<div>
  
  
                  
    
  
    
  
    
    
        
        
                        <!-- scripts are in if checks so no js will fire if variables arent present... which means its safe public facing -->

  <div class="TOBEMOVED"></div>
  
  <script type="text/javascript">

  
       $( ".TOBEMOVED" ).append( $( "<div id='contentclick18919'></div>" ) );
    (function() {
      var data = {
          pub_id: "1573",w_id: "18919",pw: "ce8704f335a340", cbust: (new Date()).getTime()
      };
      if (typeof widgetCheck18919 === 'undefined'){
          var u="";
          for(var key in data){u+=key+"="+data[key]+"&";}
          u=u.substring(0,u.length-1);		
          var a = document.createElement("script");
          a.type= 'text/javascript';
          a.src = "https://api.contentclick.co.uk/pub_serve.php?" + u;
          a.async = true;		
          document.getElementById("contentclick18919").appendChild(a);
          window.widgetCheck18919 = "set";
      }
    })();
  
    

  /**
   * If it's in utility belt, then don't move with jQuery
   *
   * Utility Belt : article == "bottom"
   * Bottom Slot : article == "article"
   *
   **/


  
  if ($('#article-comments').length){
  
    var movecontentclick = $('.TOBEMOVED');
    $('.TOBEMOVED').detach;
    $('#article-comments').before(movecontentclick);
  
  }else{
  
    var movecontentclick = $('.TOBEMOVED');
    $('.TOBEMOVED').detach;
    $('.article-share.bottom').after(movecontentclick);
  
  }

  
</script>


      
      
      
    
    
        
        
                        

  

      
      
      
  

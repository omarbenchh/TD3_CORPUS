<script language="JavaScript">

if(typeof newsletter_more_options_open != 'undefined' && newsletter_more_options_open){
	jQuery(document).ready(function(){
		jQuery('#more-options').css('display', 'block');
		jQuery('#signup-form .buttons button').text('Fewer options');

                                <script>
                                    function newWin() {

                                        var trimmed = document.getElementById('q').value;
                                        var domains = document.getElementById('domains').value;

                                        var val = 0;
                                        var selection = document.searchform.sitesearch;
                                        for (i = 0; i < selection.length; i++)
                                        {

                                            if (selection [i].checked == true)
                                                val = selection [i].value;

                                        }

                                        var weburl = "http://www.google.com/search?q=" + trimmed + "&domains=" + domains + "&sitesearch=" + val + "";
                                        var new_window = window.open(weburl);/* ,"","width=200,height=200"); */

                                    }
                                </script>

$(function () { $("[data-toggle = 'tooltip']").tooltip(); });
function openModel(src,nTitle){
	$('#myModal').modal('show');
	$('#myModalLabel').html(nTitle);
	$('#subFrame').attr('src', src);
	$('#subFrame').attr('width','100%');
	$('#subFrame').attr('height','100%');
}

<script>
$(window).scroll(function() {
	var pos_high1 = $('.outbrainDiv').position().top;	
	var wtop1 = $(window).scrollTop();
	if (wtop1 >= pos_high1){
		$('.fixBtm').hide();
	}
	else{
		$('.fixBtm').show();
	}
});
</script>

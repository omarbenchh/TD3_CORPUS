<script>
// load more stories on window scroll
$(document).ready(function(){
var lnav =$(window).height();
$('#left_nav ul').css('height','auto');
var hdr = $('.sub_nav').position().top;
$(window).scroll(function() {

var wintop = $(window).scrollTop();

if (wintop >= hdr)
{
$('.sticky_header').addClass('fixed_header');
$('.sticky_header').slideDown();
}
else
{
$('.sticky_header').removeClass('fixed_header');
$('.sticky_header').slideUp(50);
}
});


	$('.pnck').on('click', function(e){
	$('#left_nav').slideToggle();
	$('#left_nav h2').css("display", "none"); 
	//e.stopPropagation();
	$('.sub_nav').css("z-index", "10"); 
	});

	function pncktoggle(){
	$('#left_nav').slideToggle();
	
	}	
		

});

	(function($){
                     $(document).ready(function(){ 
					 //console.log("widget loaded");
                         var matchcount=0;
                         var scorescontainer = $('ul#topscoreslist');     
                         var totalscores =$('ul#topscoreslist li').length;
						 //console.log("total score:"+totalscores);
                         if(totalscores > 1){
                             $('a.scoresnav').removeClass('fad_arrow');
                         }
                        // scorescontainer.css({'width':parseInt((100*totalscores))+'%'});
                        // scorescontainer.find('li').css({'width':(100/totalscores)+'%','float':'left'});
                        // $('a.scoresnav').on('click',function(e){
							 $("body").on("click", "a.scoresnav", function(e){
							 var totalscores =$('ul#topscoreslist li').length;
                             e.preventDefault();
                             var mythis =$(this);
                             var dir = mythis.data('dir');
							 //console.log(totalscores);
                             matchcount = ( dir === 'next' ) ? matchcount+1 : matchcount-1; 
							 //console.log(matchcount);
                             if( matchcount <=0 || matchcount >= totalscores){ matchcount=0;}

                             var marg = matchcount*100;
							 //console.log("marg"+marg);
							 
							
							 
                             $('ul#topscoreslist').animate({
                                           'margin-left': '-'+marg+'%'
                                   },500);
                         });

                   });
                   })(jQuery);				   
                                        
                                        
                    </script>
</script> 




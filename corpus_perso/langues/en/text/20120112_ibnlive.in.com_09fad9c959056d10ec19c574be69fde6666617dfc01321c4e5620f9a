 <script>
var contener= $('.article_expnav .ainner_nav ul');
	var li_width= contener.find('li').width();
	var liwidth=li_width+40;
	var li_length= contener.find('li').length;
	var count=0;
	$('.sld_arrow').on('click', function(){
	var dir=$(this).data('type')
	if(dir=='right'){
		count=count+1;
		}	
		else{
		count=count-1;
		}
		if(count<0 || count>=li_length-3){
		count=0;
		}
		contener.animate({
		marginLeft:'-'+liwidth*count 
		})
	});
</script>

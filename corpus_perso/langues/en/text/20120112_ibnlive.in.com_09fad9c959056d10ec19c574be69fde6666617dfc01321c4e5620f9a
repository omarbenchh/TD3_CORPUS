<script type="text/javascript">
function validate()
{



var vall=document.srch.topic.value;



if(document.srch.topic.value=="")
{
alert("Please Enter your search term");
document.srch.topic.focus();
return false;
}

 var iChars = "@#$%^*()+=[]\\;./{}|\":<>";
        for (var i = 0; i < document.srch.topic.value.length; i++) {
                if (iChars.indexOf(document.srch.topic.value.charAt(i)) != -1) {
                alert ("The box has special characters. \nThese are not allowed.\n");
                return false;
        }
                }
	

window.location.href="/search.php?topic="+vall;




}


</script>
	<script type = "text/javascript" src="http://ns.ibnlive.in.com/ibnkhabar/js/autosuggest.js"></script>
  <script type="text/javascript">
function getSearchjss(words){
		if(typeof(words)==="undefined") {
			return false;
		}
		else{
			var words = words.trim();
		}
		if(words=="" || words.length<3){
		return false;
		}else{
		$(".fp_searchbox #searchbutton").hide();
		$(".fp_searchbox #closebutton").show();
		$.get("/autosuggest.php",{query:words},function(data){
			$(".autosearchbox").html(data);
                        if(data.length >0){
                            $("div.autosearchbox").show();
                        }else{
                            $("div.autosearchbox").hide();
                        }
		});
                 
		
		}
	}

</script>
<style>

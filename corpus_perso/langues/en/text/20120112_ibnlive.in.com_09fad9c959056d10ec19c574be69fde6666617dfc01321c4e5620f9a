 <script>
$(window).scroll(function() {
var pos_high = $('.mfsection').position().top;
//alert(pos_high);
var wtop = $(window).scrollTop();
if (wtop >= pos_high){
$('.article_expnav').addClass('tslider_box').slideDown();
}
else{
$('.article_expnav').slideUp();
$('.article_expnav').removeClass('tslider_box');
}
 });
</script>

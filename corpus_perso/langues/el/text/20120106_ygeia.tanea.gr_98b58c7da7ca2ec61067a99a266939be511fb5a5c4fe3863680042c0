</div><script>
function validateBodyMass2() {
	var weight = document.getElementById('weight').value
	var height = document.getElementById('height').value 

	if ((isNaN(weight)) || (weight=='') || (weight<1) || (weight>250)) {
		document.getElementById('mass_result').innerHTML='Το βάρος σας δεν είναι έγκυρο';
	}
	else if (isNaN(height) || height<10 || height>250) {
		document.getElementById('mass_result').innerHTML='Το ύψος σας δεν είναι έγκυρο';
	}
	else {
			document.mass_frm.submit();
	}
}
</script>

var xxx_serverMethodsUrl = 'ajax/Ajaxproxy.asp';
function getVideo(num) {
	var params;
	//$('comloader').style.visibility='visible';
	params = 'c=VideosRight&m=getMainVideo&vd='+num;
	var myAjax = new Ajax.Request(xxx_serverMethodsUrl, {method:'get',parameters:params,onComplete:feedResult});
}
function feedResult(response) {
	if (response.status==200) {
		//$('comloader').style.visibility='hidden';
		$('home_video_title').innerHTML = response.responseText
		response.responseText.evalScripts();
	}
	else{
		alert('Παρουσιάστηκε ένα σφάλμα, παρακαλώ δοκιμάστε ξανά');
	}
};
</script>

					

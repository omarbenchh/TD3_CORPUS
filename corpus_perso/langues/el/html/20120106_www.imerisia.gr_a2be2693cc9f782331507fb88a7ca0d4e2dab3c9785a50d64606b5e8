    $(document).ready(function() {
    	var artH = $('#arttext').height();
    	$(window).scroll(function() {
    		if($(this).scrollTop() > artH/3) {
    			$('#js-popupart').addClass('is-visible');
        			if($(this).scrollTop() > (artH + $(window).height() - 185)) {
        		        $('#js-popupart').removeClass('is-visible');
        		} 
    		} else {
    			$('#js-popupart').removeClass('is-visible');
    		}
    	});
        $('.js-close').on('click', function(e) {
            if ($('#js-popupart').hasClass('is-visible')) {
                $('#js-popupart').remove();
            }
    		e.preventDefault();
        });
    });
</script>


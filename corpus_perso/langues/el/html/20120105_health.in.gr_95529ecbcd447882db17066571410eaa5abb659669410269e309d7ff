      Forms.SerializeControls = function(controls, fn) {
      var Result = "";

      var values = Forms.GetControlValues(controls);

      for (var i in values) {
      if (typeof (values[i]) != 'function') {
      if (fn) {
      if (fn(i, values[i])) {
      Result += i + "=" + values[i] + "&";
      }
      }
      else {
      Result += i + "=" + values[i] + "&";
      }
      }
      }

      
      Result = Result.substring(0, Result.length - 1);

      return Result;

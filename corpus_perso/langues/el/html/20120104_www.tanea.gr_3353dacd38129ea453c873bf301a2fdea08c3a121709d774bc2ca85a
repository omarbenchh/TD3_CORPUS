    <h2>
        <a href="/news/articlelist/" title="όλα τα τελευταία νέα" class="more" id="newsroomlink">
            όλες οι τελευταίες <span class=\"section\" id=\"eidiseis\">ειδήσεις</span></a></h2>
    <script type="text/javascript">
        $(function () {
            $('#newsroom').newsroom({
                UrlPrefix: '/WebServices/ArticleManager.asmx/GetLastArticleBoxROI?catId=',
                LinkPostText: ' τελευταίες <span class=\"section\" id=\"eidiseis\">ειδήσεις</span>',
                CategoriesInfo: function () {
                    var n = 'news/', a = '/articlelist/';
                    return {
                        '-1': { l: 'όλες οι', u: 'news' + a },
                        '4': { l: 'Πολιτική', u: n + 'politics' + a },
                        '10': { l: 'Ελλάδα', u: n + 'greece' + a },
                        '16': { l: 'Κόσμος', u: n + 'world' + a },
                        '18': { l: 'Life &amp; Style', u: 'life-style' + a },
                        '28': { l: 'Επιστήμη &amp; Τεχνολογία', u: n + 'sience-technology' + a },

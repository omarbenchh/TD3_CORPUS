		.success(function(data) {
			if (typeof data.id !== 'undefined') {
				if (typeof data.comments !== 'undefined') {
					fbCommentsCounter = data.comments;
				} else {
					fbCommentsCounter = 0;
				}
				WP.$('#opiCountFb').html('(' + fbCommentsCounter + ')');
			}
		});
	} else {
		if (typeof fbCommentsCounter !== 'undefined') {
			fbCommentsCounter += modifier;
			WP.$('#opiCountFb').html('(' + fbCommentsCounter + ')');
		}
	}
}
WP.$(function(){
			WP.$('#opWyborT1, #opWyborT2').click(function(){
						var heightOfTab1 = WP.$('#opWyborD1').height() || 0;
			var heightOfTab2 = WP.$('#opWyborD2').height() || 0;
			var heightOfViewport = WP.$(window).height();
			var targetHeight = Math.min(Math.max(heightOfTab1, heightOfTab2), heightOfViewport);
			WP.$('#opWyborD1, #opWyborD2').css('min-height', targetHeight);
						var cookieValue = WP.$(this).attr('rel');
			WP.mcookie.set('opiTab', cookieValue);
		});
		
					WP.$('#opiCountWp').html('(1)');
			
					loadOpiCountFb();
	
		var opWybor = new WP.tabs('opWybor');
	opWybor.press((WP.mcookie.get('opiTab') == 'fb') ? 2 : 1);
});
//-->
</script>
		</div>
	</div>


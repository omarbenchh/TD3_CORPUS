    jQuery(document).ready(function () {
        if (!(Inpl && Inpl.Mobile && Inpl.Mobile.Detect && Inpl.Mobile.Detect.isMobile)) {
            return;
        }
        var hideInfo = jQuery.cookie('hideRedirectInfo') ? true : false;

        if (!hideInfo) {
            jQuery(document.body).addClass('isMobileRedirect');

            jQuery('.redirect-close').click(function() {
                var domain = location.host.match(/\.[^\.]+\.[^\.]+$/)[0];       // 2 ostatnie czlony

                jQuery(document.body).removeClass('isMobileRedirect');
                jQuery.cookie('hideRedirectInfo', true, { expires: 3650, path: '/', domain: domain });
                return false;
            });

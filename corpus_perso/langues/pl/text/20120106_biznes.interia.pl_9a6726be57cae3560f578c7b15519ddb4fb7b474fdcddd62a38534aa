    function nsLoadAndEval(src)
  {

    if (! (window.XMLHttpRequest)) {
      nsLoadAndEvalByDom(src);
      return;
    }

    if(navigator.userAgent.indexOf('MSIE 10.0') != -1) {
      nsLoadAndEvalByDom(src);
      return;
    }

    var xhr = new XMLHttpRequest();
    if("withCredentials" in xhr) {
        xhr.open("GET", src, true);

    } else {
        nsLoadAndEvalByDom(src);
        return;
    }


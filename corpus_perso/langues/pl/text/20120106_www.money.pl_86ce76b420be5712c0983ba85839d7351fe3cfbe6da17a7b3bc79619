var powiadomEnabled=true;
function showPowiadomienia(div)
{
	if(!powiadomEnabled)
	{
		alert('Sprawd¼ swoj± skrzynkê. Wys³ali¶my Ci maila wraz z linkiem do aktywacji konta.');
		return;
	}
	var div=document.getElementById(div);
	if(div)
		div.style.display='block';
}

function hidePowiadomienia(div)
{
	var div=document.getElementById(div);
	if(div)
		div.style.display='none';	
}

function checkEmail(email)
{
	jQuery.ajax({
              url: '/ajax/moje/check_email.php',
              type: 'POST',

              data: 'email='+escape(email),
              error: function(){
              },
              success: function(html){
                 if(html=='-1')
                 {
                     alert('niepoprawny');
                 }
                 if(html==0)
                 {
                     hidePowiadomienia('box_powiadom_1')
                     showPowiadomienia('box_powiadom_2')
                 }
                 if(html==1)
                 {
                     hidePowiadomienia('box_powiadom_1')
                     showPowiadomienia('box_powiadom_3')      
                 	 var div=document.getElementById('email3');
                 	 if(div)
                 		div.value=email;                            
                 }
              
              }
       });
}


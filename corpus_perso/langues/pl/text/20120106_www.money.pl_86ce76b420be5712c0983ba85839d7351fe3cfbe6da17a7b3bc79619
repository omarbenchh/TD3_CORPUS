		alert('Musisz zaznaczyæ wszystkie wymagane pola'); 
		return false;
	}
	jQuery('#powiadom_spinner').html('<img src="http://static1.money.pl/i/spinner.gif">')
	form.register.disabled=true;
	jQuery.ajax({
        url: '/ajax/moje/register.php',
        type: 'POST',

        data: 'email='+escape(form.email.value)+'&id='+1000108,
        error: function(){
        	form.register.disabled=false;
        	jQuery('#powiadom_spinner').html('')        	
        	alert('Dodanie konta nie powiod³o siê. Spróbuj u¿yc innego adresu email, lub skontaktuj siê z administratorem');
        },
        success: function(html){
        	jQuery('#powiadom_spinner').html('')
            if(html=='1')
            {
                hidePowiadomienia('box_powiadom_3')
                showPowiadomienia('box_powiadom_4')
                powiadomEnabled=false;
                return;

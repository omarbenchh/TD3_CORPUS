  <script>
    
      jQuery("#form-polec").submit(function() {
          var wyrazenie = /^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
          if(wyrazenie.test(jQuery(this).find("#email").val())) {
              jQuery(this).find("span.blad").removeClass("widoczny");
              jQuery(this).find("#email").removeClass("blad");
              return true;
          } else {
              jQuery(this).find("#email").addClass("blad");
              jQuery(this).find("span.blad").addClass("widoczny");
              return false;
          }
      });

      jQuery("#narzedzia .facebook").hover(
        function() {
          jQuery(this).toggleClass("hover");
          jQuery(this).click();
        },
        function() {
          jQuery(this).toggleClass("hover");
      });

      var Czcionka = {
          aktualnie : 3,
          
          zmien : function () {
              jQuery("#material").attr("data-rozmiar", this.aktualnie);
          },
          
          powieksz : function () {
              if (this.aktualnie < 6) this.aktualnie++;
              this.zmien();
              return false;
          },

          pomniejsz : function () {
              if (this.aktualnie > 0) this.aktualnie--;
              this.zmien();
              return false;
          }
      }
      
      function Pozycjonuj() {
          var poz = jQuery("#material").offset(),
              scrollTop = jQuery(window).scrollTop(),
              przesuniecie = jQuery("#pasek-media-plus").outerHeight(true) + 30,
              doWysokosci = jQuery("#material footer").offset().top - 25,
              narzedzia = jQuery("#narzedzia");
              
          if(poz.top <= scrollTop + przesuniecie) {
              if(scrollTop + przesuniecie + narzedzia.outerHeight(true) < doWysokosci) {
                  narzedzia.css({
                      "top": przesuniecie + 20,
                      "position": "fixed",
                      "left": poz.left - narzedzia.outerWidth(true),
                  });
              } else {
                  narzedzia.css({
                      "top": jQuery('#material footer').offset().top - poz.top - narzedzia.outerHeight(true) + 20,
                      "position": "absolute",
                      "left": -narzedzia.outerWidth(true),
                  });
              }
          } else {
              narzedzia.css({
                  "top": 35,
                  "position": "absolute",
                  "left": -narzedzia.outerWidth(true),
              });
          }
      }
      
      jQuery(window).on("scroll resize load", function(){
          Pozycjonuj();
      });
      liczbySpolecznosciRegionalne();
    
  </script>

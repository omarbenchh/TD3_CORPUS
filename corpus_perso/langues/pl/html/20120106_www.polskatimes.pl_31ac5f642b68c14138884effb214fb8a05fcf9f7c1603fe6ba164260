<script type="text/javascript">
(function () {
    /*
     * funkcja dodaje widgety ogloszen
     */
    (function () {
        function wstaw_widgety() {
            szerokosc_okna = jQuery(window).width();
            var czy_jest_zewnetrzny = jQuery("#widget_zewnetrzny").html();
            var czy_jest_lodzki = jQuery("#widget-lodzki").html();

            if (czy_jest_zewnetrzny == "" || czy_jest_lodzki == "") {
                if (szerokosc_okna >= 480) {
                    var div_widget_lodzki = document.getElementById("widget-lodzki");
                    var div_widget_zewnetrzny = document.getElementById("widget_zewnetrzny");
                    if (div_widget_lodzki == null && div_widget_zewnetrzny != null) {
                        jQuery('#widget_zewnetrzny').append('<iframe src="http://ogloszenia.gratka.pl/widget_zewn.html?id_widget=widget_&id_gazeta=&referer=" scrolling="no" allowTransparency="true" frameborder=0></iframe>');
                        return true;
                    }
                    return false;
                }
            }
        }

        var resizeTimeout;
        var czy_wstawic_widget = true;

        if (!wstaw_widgety()) {
            jQuery(window).resize(function() {
                clearTimeout(resizeTimeout);
                resizeTimeout = setTimeout(function() {
                    if (czy_wstawic_widget) {
                        czy_wstawic_widget = !wstaw_widgety();
                    }
                }, 200);
            });
        }
    })();

    var klikniecieMenu = function () {
        return false;
    }
    jQuery('.menu1Poziom').bind('click', klikniecieMenu);
    GR('pPolskatimesMenuInicjalizuj', function () {
        jQuery('.menu1Poziom').unbind('click', klikniecieMenu);
    });
})();
(function(tr){
    var nazwa;
    GR('$$', 'menu1Poziom', $('menu-glowne'), '', function(e) {
        nazwa = e.innerHTML.trim().replace(/<\/?b>/ig, '').toLowerCase();
        if (e.parentNode.id == 'dodaj-ogloszenie') {
            nazwa = 'dodaj-ogloszenie';
        } else if (e.parentNode.id == 'kategoria-platne') {
            nazwa = 'piano';
        }
        tr.push({elements: [e], href: '/onclick/menu/menu_belka-' + nazwa});
        jQuery(e.parentNode).find('.listaKat li a').each(function() {
            var nazwa_podkategorii = this.innerHTML.trim().replace(/<\/?b>/ig, '').toLowerCase();
            tr.push({elements: [this], href: '/onclick/menu/podmenu-' + nazwa + '-podkategoria-' + nazwa_podkategorii});
        });

        var licznik = 1;
        jQuery(e.parentNode).find('.wybranyArt a').each(function (){
            tr.push({elements: [this], href: '/onclick/menu/podmenu-' + nazwa + '-link-' + licznik++});
        });
        jQuery(e.parentNode).find('.zobaczTakze a').each(function (){
            tr.push({elements: [this], href: '/onclick/menu/podmenu-' + nazwa + '-link-' + licznik++});
        });
        jQuery(e.parentNode).find('.kolInne .komentowane a').each(function (){
            tr.push({elements: [this], href: '/onclick/menu/podmenu-' + nazwa + '-link-' + licznik++});
        });
    });
    licznik = 1;
    tr.push({elements: jQuery("#h-ogl-dom"), href: '/onclick/ogloszenia/gratkadom/naglowek'});
    tr.push({elements: jQuery("#h-ogl-moto"), href: '/onclick/ogloszenia/gratkamoto/naglowek'});
    tr.push({elements: jQuery("#h-ogl-ale"), href: '/onclick/ogloszenia/alegratka/naglowek'});
    tr.push({elements: jQuery("#h-ogl-praca"), href: '/onclick/ogloszenia/gratkapraca/naglowek'});
    tr.push({elements: jQuery("#img-ogl-dom"), href: '/onclick/ogloszenia/gratkadom/glowny'});
    tr.push({elements: jQuery("#img-ogl-moto"), href: '/onclick/ogloszenia/gratkamoto/glowny'});
})(oG_OSA.tracking);
</script>


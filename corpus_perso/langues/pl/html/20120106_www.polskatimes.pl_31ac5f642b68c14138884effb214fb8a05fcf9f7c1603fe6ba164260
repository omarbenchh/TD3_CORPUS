    	    szerokosc_okna = jQuery(window).width();
    	    if (szerokosc_okna >= 480) {
    	        jQuery('.like').append('<iframe src="http://www.facebook.com/plugins/like.php?href=http%3A%2F%2Fwww.facebook.com%2FPolskatimes&amp;layout=button_count&amp;show_faces=false&amp;width=150&amp;action=like&amp;colorscheme=light&amp;height=22" style="border: none; overflow: hidden; width: 150px; height: 22px" scrolling="no" frameborder="0" allowTransparency="true"></iframe>');
    	      	return true;
    	    }
    	    return false;
  	  	}
        var resizeTimeout;
        var czy_wstawic_lajka = true;

        if (!wstaw_lajka()) {
            jQuery(window).resize(function() {
                clearTimeout(resizeTimeout);
                resizeTimeout = setTimeout(function() {
                    if (czy_wstawic_lajka) {
                        czy_wstawic_lajka = !wstaw_lajka();
                    }
                }, 200);
            });
        }

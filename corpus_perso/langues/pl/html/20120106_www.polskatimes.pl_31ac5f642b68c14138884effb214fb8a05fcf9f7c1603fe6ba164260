<script type="text/javascript">
    (function ($) {

        function dopasujTloLoga() {
            var adWinieta = $('#ad-gora1:visible'),
                logo = $('#naglowek-link-logo > img'),
                dzial = $('#naglowek-link-logo + span'),
                tloLogo = $('#logo-serwisu'),
                logoOffset = logo.offset(),
                logoWidth = logo.width();

            if (adWinieta.length === 0) { return false; }

            tloLogo.css({
                'top': logoOffset.top - 5,
                'left': logoOffset.left - 5,
                'height': logo.height() + 10,
                'width': dzial.length > 0 ? logoWidth + dzial.width() + 20 : logoWidth + 10
            });
            tloLogo.show();
            return true;
        }

        GR('wstawReklame', {
            nazwa : 'gora1',
            rodzic: '#ad-gora1',
            onServerEmission: function () {
                $('#polska').addClass('obrandowany');
                $('#fb-slider').addClass('zWinieta');

                setTimeout(function () {
                    dopasujTloLoga();
                }, 1000);
            }
        });

        $(window).resize(function () {
            $('#logo-serwisu').hide();
            dopasujTloLoga();
        });
        GR('pPolskatimesLoginWarstwaInicjalizuj');

    })(jQuery);
</script>


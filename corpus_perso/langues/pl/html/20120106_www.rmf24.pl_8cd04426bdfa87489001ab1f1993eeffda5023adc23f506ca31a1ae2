                <script>
        jQuery( document ).ready(function(){
            var contener= jQuery('ul.reportMenuList').outerWidth(true);;
            var more= jQuery('.wiecej:visible').outerWidth(true);
            var firstMore=jQuery('li.more').first().outerWidth(true);
            var sum=0;
            for (var i = 0; i < 5; i++) {
                        sum= sum + jQuery('ul.reportMenuList li.i'+i).outerWidth(true);
            }
           sum=sum+more+firstMore;
           while( sum < contener && jQuery('li.element').length < jQuery('ul.reportMenuList li.element').length + jQuery('ul.reportMenuList li.more').length){
                jQuery('li.more').first().removeClass('more').addClass('element');
                firstMore=jQuery('li.more').first().outerWidth(true);
                sum=sum+firstMore;
               
                if(jQuery('li.element').length == jQuery('ul.reportMenuList li.element').length + jQuery('ul.reportMenuList li.more').length){
                    jQuery('li .wiecej').hide();   
                }
                
            }
            jQuery(".wiecej").click(function(){
                jQuery("li.more").toggle('fast');
                jQuery("a.wiecej").toggle();
            });
        });
        </script>

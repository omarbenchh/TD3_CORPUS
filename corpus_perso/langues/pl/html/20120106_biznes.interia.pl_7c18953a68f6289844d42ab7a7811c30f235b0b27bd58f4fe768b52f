    jQuery(document).ready(function () {
        if (!(Inpl && Inpl.Mobile && Inpl.Mobile.Detect && Inpl.Mobile.Detect.isMobile)) {
            return;
        }
        var hideInfo = jQuery.cookie('hideRedirectInfo') ? true : false;

        if (!hideInfo) {

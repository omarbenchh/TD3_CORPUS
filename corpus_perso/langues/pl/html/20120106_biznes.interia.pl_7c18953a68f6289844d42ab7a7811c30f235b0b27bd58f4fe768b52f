	js.src = "//connect.facebook.net/" + localization + "/all" + (debug ? "/debug" : "") + ".js";
	ref.parentNode.insertBefore(js, ref);
}(document, 'script', 'facebook-jssdk', 'pl_PL', false));
//-->
</script>
    </body></html>
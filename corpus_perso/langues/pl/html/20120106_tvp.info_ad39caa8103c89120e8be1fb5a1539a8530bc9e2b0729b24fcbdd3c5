<script type="text/javascript">
if($('.simple-sticky-div')){
var $window = $(window), $stickyEl = $('.simple-sticky-div'), elTop = $stickyEl.offset().top, $copyrightEl = $("#main-footer");
console.log( "elTop: " +  elTop );
$(window).on('load',function(){
	elTop = $stickyEl.offset().top;
	window.setTimeout(function(){elTop = $stickyEl.offset().top;},2000);
$window.scroll(function() {
	$stickyEl.toggleClass('sticky', $window.scrollTop() > elTop);
	if (($window.scrollTop() > elTop) && ($window.scrollTop() + $stickyEl.height() + 53 > $copyrightEl.offset().top)) {
		$stickyEl.addClass('sticky_bottom').css({top: ($('body').height() - 53 - $stickyEl.outerHeight() - $copyrightEl.outerHeight()) + "px"});
	} else { 
		$stickyEl.removeClass('sticky_bottom').css({top:"53px"});
	}
});
});
}
</script>

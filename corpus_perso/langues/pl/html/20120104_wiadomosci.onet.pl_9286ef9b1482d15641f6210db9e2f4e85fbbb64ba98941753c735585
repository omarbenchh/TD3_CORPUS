	<script>
	var OF = OF || {}; 
	OF.AsyncLoader = {
		/*
		 * Dynamiczne ładowanie plików JS
		 * @param {string} src Url pliku JS
		 * @param {function} callback funkcja, która będzie wykonana po załadowaniu pliku 
		 */
		jsLoaderFiles: {},
		loadJS: function(src, callback) {		
			function runCallbacks(callbacks) {
				while(callbacks.length) {
					(callbacks.shift())();
				}
			}
			var jsLoaderFile = OF.AsyncLoader.jsLoaderFiles[src];
			//Gdy plik jest w tablicy i jest załadowany odpalamy callbacka
			if(jsLoaderFile && jsLoaderFile.loaded) {
				callback();
			} else if(jsLoaderFile) { //W przeciwnym wypadku dla ładowanego pliku dodajemy kolejnego callbacka
				jsLoaderFile.callbacks.push(callback);
			} else { //Tablica pusta, przygotowywujemy plik do załadowania
				jsLoaderFile = {
					loaded:false,
					callbacks: [callback]
				};
				OF.AsyncLoader.jsLoaderFiles[src] = jsLoaderFile;
				/** @name script */
				var script = document.createElement('script');
				script.type = 'text/javascript';
				script.src = src;
				if (script.readyState){
					script.onreadystatechange = function() {
						if (script.readyState == "loaded" || script.readyState == "complete") {
							script.onreadystatechange = null;
							jsLoaderFile.loaded = true;
							runCallbacks(jsLoaderFile.callbacks);
						}
					};
				} else {
					script.onload = function() {
						jsLoaderFile.loaded = true;
						runCallbacks(jsLoaderFile.callbacks);
					};
				}
				document.getElementsByTagName('head')[0].appendChild(script);
			}
		},
		loadStyleSheet: function( path ) {
			var head = document.getElementsByTagName( 'head' )[0], // reference to document.head for appending/ removing link nodes
				link = document.createElement( 'link' );           // create the link node
			link.setAttribute( 'href', path );
			link.setAttribute( 'rel', 'stylesheet' );
			link.setAttribute( 'type', 'text/css' );
			head.appendChild( link );  // insert the link node into the DOM and start loading the style sheet
			return link; // return the link node;
		 }
	};
	</script>





				

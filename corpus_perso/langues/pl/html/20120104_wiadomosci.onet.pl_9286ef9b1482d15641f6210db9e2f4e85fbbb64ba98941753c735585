        <a name="image_78131b32-13c4-4dae-958a-997730f60dd3" style="position:relative; top:-100px; display: block"></a>
    
    
     
    
    <div id="mainPhoto" class="mainPhotoImgContentSourceContener mainPhotoContentSourcAndAuthor" itemprop="image" itemscope itemtype="http://schema.org/ImageObject"
         
        
            data-run-module="embeddedapp/main.share_photo_social"
            data-uuid-photo="78131b32-13c4-4dae-958a-997730f60dd3"
        
         >
        
        
        
        

        
            
            
            
            
			

            
                
                
                
                
					
						
					
                    <img
                        data-item-uuid="78131b32-13c4-4dae-958a-997730f60dd3"
                        itemprop="url"  
                        class="mainPhotoImg"
                        src="http://ocdn.eu/images/pulscms/MDU7MDQsMCwyYywzZTgsMjMyOzA2LDMxNCwxYmM7MGMsMTQwYjFjZmU3ZjBhYzUyZWRjMDEwZDcwOTc4ZTg0YmUsMSwxLDYsMA__/a5cbe1e5fb4335b66e01b9d5cdfd59bf.jpg"

            $(document).ready(function()
            {
                //issue #1655
                                
                $.get("http://www.dziennikwschodni.pl/forum/index.php?app=apidw&module=user&section=getuser",{})
                .done(function(indata)
                {
                    var data = JSON.parse(indata);
                    if(data.id != 0)
                    {
                        $("#user-data").html(data.name);
                        $("#user-avatar").attr("src", data.avatar);
                    }
                });
                $("#comment-form").on("click","#comment-button",function()
                {
                    $(this).addClass("comment-disable").attr("disabled", "true");
                    var parent = $("#comment-form");
                    var obj = {link: false, sign: false, sxo_id: false, category: false, content: false, title: false};
                    $.each(parent.children("input"), function(i,v)
                    {
                        obj[$(v).attr("name")] = $(v).val();
                    });
                    obj.content = $("#comment-text").val();
                        $.ajax({
                            type: "POST",
                            url: "http://www.dziennikwschodni.pl/forum/?app=apidw&module=comment&section=newcomment",
                            xhrFields: { withCredentials:true },
                            data: obj,
                        }).done(function(data)
                        {
                            if(data !="link_no_log")
                            {
                                $("#new-comment").html('<div class="clear"></div><div class="komentarz-czekaj">Twój komentarz zostanie za chwilę opublikowany</div>');
                                //location.reload();
                            }
                            else
                            {
                                $("#comment-text-error").html('<div class="form-error alert-dismissible ml15">Aby umieścić link w komentarzu musisz być zalogowany</div>');
                                setTimeout(function() {
                                    $("#comment-text-error").empty();
                                    $("#comment-button").removeAttr("disabled");                           
                                    $("#comment-button").removeClass("comment-disable");                           
                                }, 3000);                                 
                            }
                        });             
                });
                $("#articleZoomText a").click(function()
                {
                    $("#articleZoomText .current").removeClass("current");
                    $(this).addClass("current");
                    if ($(this).hasClass( "A1" )) {
                        $(".news_single_content, .news_single_lead").css({
                            fontSize: "14px",
                            lineHeight: "20px"
                        });
                    } else if ($(this).hasClass( "A2" )) {
                        $(".news_single_content, .news_single_lead").css({
                            fontSize: "16px",
                            lineHeight: "24px"
                        });
                    } else if ($(this).hasClass( "A3" )) {
                        $(".news_single_content, .news_single_lead").css({
                            fontSize: "18px",
                            lineHeight: "28px"
                        });
                    };
                    return false;
                });
                /*issue #1798*/
                $(".art-18-year-yes").click(function(){
                    $isRemember = $("#art-18-year-remember").prop('checked');
                    $(".art-18-year-container").hide();
                    if($isRemember)
                    {
                        var d = new Date();
                        d.setTime(d.getTime() + (24*60*60*1000));
                        document.cookie ="is_adult=1; expires= "+d.toGMTString();
                    }
                        
                });
                $(".art-18-year-no").click(function(){
                    window.location.replace("http://www.dziennikwschodni.pl");
                });                
            });
        function socialLeftBox() {
            var poz = $(".single_news_page2").offset(),
                scrollTop = $(window).scrollTop(),
                offsety = 0,
                height = $(".single_news_page .tags_content").offset(),
                height = height.top + $(".single_news_page .tags_content").outerHeight(true) - 0;
                
            if(poz.top <= scrollTop + offsety) {
                if(scrollTop + offsety + $(".social-box-left").outerHeight(true) < height) {
                    $(".social-box-left").css("top", scrollTop + offsety - poz.top + 0);
                }
            } else {
                $(".social-box-left").css("top", 0);
            }
        }
        $(window).on("scroll resize load", function(){
          socialLeftBox();
        });
           
        </script>        
        

<script>
	(function() {
	
		var currentIndex=0;
		
		$('.next-ceneo').click(function() {
			currentIndex+=1;
			if (currentIndex!=3) {
				$('.box-ceneo-outer').css({
					"top": currentIndex*(-344)+"px"
				})
			} else {
				$('.box-ceneo-outer').css({
					"top": 0
				});
				currentIndex=0;
		 	};
		  return false;
		});
		
	})();
	
</script>

            $("#comment-form").on("keypress","#comment-text",function()
            {
                var val = $(this).val();
                var target = $("#comment-text-error");
                if(val.length > 5) {
                    target.html("");
                    if ($("#comment-button").attr("disabled")) {
                        $("#comment-button").removeAttr("disabled");
                    };
                } else {
                    target.html('<div class="form-error alert-dismissible ml15">Treść komentarza musi mieć przynajmniej 6 znaków</div>');
                    if (!$("#comment-button").attr("disabled")) {
                        $("#comment-button").attr("disabled", "false");
                    };
                }
            });

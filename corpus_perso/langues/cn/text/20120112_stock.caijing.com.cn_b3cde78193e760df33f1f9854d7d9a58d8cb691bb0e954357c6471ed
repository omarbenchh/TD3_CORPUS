body {_background-image: url(about:blank); _background-attachment: fixed;}
.backToTop{width:60px; height:60px;background:url(http://ws1.cdn.caijing.com.cn/2012-08-28/112086217.gif) 0 0 no-repeat;position:fixed;top:410px;_position: absolute;_top:expression(documentElement.scrollTop+215 + "px");cursor:pointer; margin-left:510px; left:50%;}

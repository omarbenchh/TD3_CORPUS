function form9_submit() {
    var form9_uname = $("form9_uname").value;
    var form9_pass  = $("form9_pass").value;
    if(form9_uname != "" && form9_pass != "") {
        $("d_uname").value=form9_uname;
        $("d_pass").value=form9_pass;
        $('form1').submit();
    } else {
        alert("请输入用户名和密码！");
        $("form9_uname").focus();
        $("form9").reset();
    }
}

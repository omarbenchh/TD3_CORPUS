var rand = {};
rand.get = function (begin,end){
 return Math.floor(Math.random()*(end-begin))+begin;
}
Array.prototype.remove=function(dx)
  {
    if(isNaN(dx)||dx>this.length){return false;}
    for(var i=0,n=0;i<this.length;i++)
    {
        if(this[i]!=this[dx])
        {
            this[n++]=this[i]
        }
        else{
            el = this[i];
        }
    }
    this.length-=1
    return el;
  }


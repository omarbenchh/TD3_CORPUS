myDate = new Date();
if(myDate.getHours()>11||myDate.getHours()<8){
    for (var i = 0; i<6; i++) {
	   var seed = rand.get(0,all_pic.length);
	   var item = all_pic.remove(seed);
	   show_item.push(item);

    }
}
else{
    for (var i = 0; i<6; i++) {
       show_item.push(all_pic.shift());
    }
}
	if(show_item.length>0){
		html="";
		for (var i = 0; i < show_item.length; i++) {
			html +='<div style="width:115px; height:auto; float:left;"><div style="border:1px solid #ccc;padding:5px; width:95px;"  align="center"><a href="'+show_item[i].url+'" target="_blank"><img src="'+show_item[i].pic+'"  border="0"></a></div><div style="margin-top:8px; height:40px" align="center">'+show_item[i].title+'</div></div>';

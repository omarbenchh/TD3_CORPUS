<script language="javascript" type ="text/javascript">
function reset()
{
    document.form1.username.value="";
    document.form1.password.value="";
    document.form1.comment.value="";
}
function register()
{
	var query=document.location.search.substring(1);
	var pos=query.indexOf("=");
	var value=query.substring(pos+1);
         var origurl=document.location.href; 
	if(value=="false"){
	   var pos1=origurl.indexOf("?");
	   origurl=origurl.substring(0,pos1);
            alert("亲，您的用户名不存在，请点击注册按钮先行注册论坛账号，谢谢");
            window.navigate(origurl);
	}else if(value=="true"){
	   var pos1=origurl.indexOf("?");
	   origurl=origurl.substring(0,pos1);
            alert("亲，评论发表成功，如果您想要查看评论可以点击查看全部评论按钮");
            window.navigate(origurl);
	}else if(value=="wrong"){
	   var pos1=origurl.indexOf("?");
	   origurl=origurl.substring(0,pos1);
	   alert("亲，您查看的主题还未有人评论过，请先评论");
            window.navigate(origurl);
	}
}
function chkinput()
{
	if(document.form1.username.value==""||document.form1.password.value=="")
	{
        alert("用户名或者密码不得为空");
        event.returnValue = false;
        document.form1.username.focus();
	}
}
register();
</script>




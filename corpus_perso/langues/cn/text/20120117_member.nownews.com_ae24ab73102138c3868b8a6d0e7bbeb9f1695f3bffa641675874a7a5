  <script>
    function ShowHide(SwitchID, OpenID, CloseID) {
      var vobj;
      if (SwitchID > '') {
        vobj = document.getElementById(SwitchID);
        if (vobj.style.display == '') {
          vobj.style.display = 'none';
        } else {
          vobj.style.display = '';
        }
      }
      if (OpenID > '') {
        vobj = document.getElementById(OpenID);
        vobj.style.display = '';
      }
      if (CloseID > '') {
        vobj = document.getElementById(CloseID);
        vobj.style.display = 'none';
      }
    }
  </script>
  <div>今日星座: 【
    <a id="star_url" href="http://astro.click108.com.tw/daily_9.php?iAstro=9" target="_blank" title="點擊看每日運勢" onmouseover="ShowHide('horoscopes','','')">
      <span id="star"> 射手座</span>▼</a>】
    <ul style="display:none;" id="horoscopes" onmouseover="ShowHide('','horoscopes','')" onmouseout="ShowHide('','','horoscopes')">

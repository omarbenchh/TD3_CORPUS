        };
        th.getMY = function(e) {
            return th.isIE ? e.clientY + Math.max(document.body.scrollTop, document.documentElement.scrollTop) : e.pageY
        };
        th.setEvent = function(a) {
            if (a.setCapture) a.setCapture();
            if (window.captureEvents) window.captureEvents(Event.MOUSEMOVE | Event.MOUSEUP)
        };

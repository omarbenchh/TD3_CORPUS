            if ((pos.y + th.height) > wins.height) {
                pos.y = pos.y - a.offsetHeight - th.height - th.layerTitle.offsetHeight - 30
            } else {
                pos.y = pos.y + a.offsetHeight + 5
            }
            pos.x = pos.x - a.offsetWidth / 2;
            th.layerbg.style.width = th.width + 2 + 'px';
            th.layerbg.style.left = (wins.width - th.width) / 2 + 'px';
            //th.layerbg.style.top = pos.y + 'px'
			th.layerbg.style.top = (sTop + (th.getWindowSize().height - th.height) / 2) + 'px';
        };

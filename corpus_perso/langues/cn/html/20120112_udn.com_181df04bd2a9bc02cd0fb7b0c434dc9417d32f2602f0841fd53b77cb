<script>
(function(){
  var header_height = $('.head').height(),
      ad = $('#ad-sticky'),
      sticky = false,
      doc = $(document),
      ad_top = ad.offset().top - header_height,
      main = $('#main'),
      ad_height = ad.height();


  doc.scroll(function(){
      var main_bottom = main.offset().top + main.height(),
          offset = doc.scrollTop() + ad_height - main_bottom + header_height;
      if (offset > 0) {
          ad.css('top', header_height-offset);
      } else {
          ad.css('top', header_height);
      }
      try {
          if (ad_top < doc.scrollTop()) {
              if (!sticky) {
                  ad.addClass('stuck');
                  sticky = true;
              }
          } else {
              if (sticky) {
                  ad.removeClass('stuck');
                  sticky = false;
              }
          }
      } catch(err) {

      }
  });
})();
</script>



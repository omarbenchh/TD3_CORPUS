<script>
	$(function(){
		function BDLike(elm){
			var BDLike = setInterval(function(){
				if($(elm).html() != ''){
					$(elm+" ul").next().remove();
					$(elm+" ul").parent().find(".hm-t-header").removeAttr("style").css({ 
						"height": "27px",
						"color": "#fff",
						"line-height": "24px",
						"font-size": "14px",
						"border-bottom": "none",
						"font-weight": "bold",
						"position": "relative",
						"background": "#fefefc url(http://himg2.huanqiu.com/statics/images/articleTit.png) 0 -37px no-repeat",
						"text-indent": "22px"
					});
					  $(elm+" ul").parent().find(".hm-t-footer").hide();
						clearInterval(BDLike);
					}
			},300);
		}
		BDLike("#hm_t_6878");
	});
</script>

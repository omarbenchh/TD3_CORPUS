  if(msg.status == 1) {
    $("rmd_f").value = "已推荐";
    $("rmd_f").style.cursor="auto";
    $("rmd_f").title="";
  } else {
    V.addListener($("rmd_f"), 'click', function(){rmd_form9();});
    $("rmd_f").title="将本文推荐给凤凰好友";
    $("rmd_f").style.cursor="pointer";
  }
}

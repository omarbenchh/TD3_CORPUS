<script language="javascript">
//用于控制导航标红
if(typeof(url) == 'undefined'){
	var url = window.location.href;
}
var	dongtainav = document.getElementById("dongtainav");
try{
	var links = dongtainav.childNodes;
}catch(err){
	var links = new Array();
}
for(var i = 0 ; i < links.length ; i ++){
	var href = new String(links[i].childNodes[0].href);
		//不含页面名地址
		if(href.lastIndexOf('/')==href.length-1){
			if(href.indexOf(url)>=0){
				links[i].childNodes[0].className = 'link_dqy';
			}
		//含页面名地址
		}else{
			var flag = href.substring(href.lastIndexOf('/')+1,href.lastIndexOf('.'));
			if(getBaseUrl(url,flag).indexOf(getBaseUrl(href,flag))>=0){
				links[i].childNodes[0].className = 'link_dqy';
			}
		}
		i++
}

function getBaseUrl(url,flag){
	var indexA = url.lastIndexOf('/');
	var indexB = url.lastIndexOf('.');
	//含字母
	if(isNaN(flag)){
		return url.substring(0,indexB);
	//不含字母(截取35/这种形式的地址防止跟351类似ID无法区分)
	}else{
		return url.substring(0,indexA+1);
	}
}

</script>


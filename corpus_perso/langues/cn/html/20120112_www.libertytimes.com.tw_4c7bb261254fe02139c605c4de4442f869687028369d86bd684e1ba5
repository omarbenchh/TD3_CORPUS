function pointnewstab(o){
  $('#pointnewstab li').removeClass('pnt_focus');
  $('#pointnewstab li:nth('+o+')').addClass('pnt_focus').children('a').blur();
  $("#pointnew > div").hide();
  $('#pointnew > div:nth('+o+')').show();
}
pointnewstab(0);

  function validateForm(mceId, checktopic) {
    errors=false;
    messages='';
    if ( checktopic == 1 && document.newmessage.f_topic.value == '' ) {
      errors = true;
      messages += "There is no topic provided.\n";
    }
    str = tinyMCE.get(mceId).getContent();
    re = /[a-zA-Z0-9]+/ ;
    if ( ! str.match(re) ) {
      errors = true;
      messages += "There is no message text.\n";
    }
    if ( document.newmessage.nick.value == '' ) {
      errors = true;
      messages += "You must provide a nickname.\n";
    }
    if ( errors ) {
      alert(messages);
      return false;
    }

    return true;

  }
</script>

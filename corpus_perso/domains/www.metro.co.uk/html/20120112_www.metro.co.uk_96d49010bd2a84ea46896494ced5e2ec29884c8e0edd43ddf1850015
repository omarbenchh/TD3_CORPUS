				(function (ted) {

					ted.tedEvent = function (type, params) {

						params = params || {};
						params.r = document.referrer;
						params.g = metro.ted.g || 'GB';
						params.orientation = Math.abs(window.orientation) == 90 ? 'landscape' : 'portrait';
						params.category = metro.pageData.topCategory;
						params.subcategory = metro.pageData.subCategory;
						params.type = metro.pageData.type;
						params.postUrl = document.URL;
						params.postId = metro.pageData.postId;
						params.pubDate = '2012-01-12T08:32:19+00:00';
						if (document.documentElement) {
							params.zoom = document.documentElement.clientWidth / (window.innerWidth || document.documentElement.clientWidth);
						}
						var urlParams = [];
						for (var key in params) {
							urlParams.push('&', key, '=', encodeURIComponent(params[key]));
						}

						var rnd = Math.random() * 10000000;

						if ('ted.metro.co.uk/s/l'.length > 0) {
							var url = '//ted.metro.co.uk/s/l';
							var pixel = new Image(1, 1);
							pixel.src = [url, '?t=', type, urlParams.join(''), '&_cb=' + rnd].join('');
						}


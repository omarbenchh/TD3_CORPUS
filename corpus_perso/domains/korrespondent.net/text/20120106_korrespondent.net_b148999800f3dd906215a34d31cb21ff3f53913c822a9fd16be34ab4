        function ShowDiv(divId) {
            HideDiv('facebook');
            HideDiv('gplus');
            HideDiv('twitter');
            HideDiv('vk');
            $('#lifacebook').removeClass('tabs__item_active');
            $('#ligplus').removeClass('tabs__item_active');
            $('#litwitter').removeClass('tabs__item_active');
            $('#livk').removeClass('tabs__item_active');

            $('#li' + divId).addClass('tabs__item_active');
            $('#' + divId).show();
        }

        function HideDiv(divId) {
            $('#' + divId).hide();
        }

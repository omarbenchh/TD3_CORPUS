        <script type="text/javascript">
            function Hits() {
                $.get('http://korrespondent.net/info.hit?id=1304466', function (data) {
                    if (data) {
                        var x = data.split('|');
                        if (x && x.length == 2) {
                            if (x[0] > 0) {
                                var currentCount = x[0];
                                var count_sync = $("#count-sync");

                                if (count_sync.text()) {
                                    if (parseInt(count_sync.text(), 10) > parseInt(currentCount, 10)) {
                                        currentCount = count_sync.text();
                                    }
                                    else {
                                        $(".comments__head span").text(currentCount);
                                    }
                                }
                                else {
                                    count_sync.text(currentCount);
                                }

                                $('.post-item__comments > span').text(currentCount);
                            }
                            else
                                $('.post-item__comments').hide();
                        
                            if (x[1] > 0)
                                $('.post-item__views > span').text(x[1]);
                            else
                                $('.post-item__views').hide();
                        }
                    }
                });
            }
            Hits();
        </script>

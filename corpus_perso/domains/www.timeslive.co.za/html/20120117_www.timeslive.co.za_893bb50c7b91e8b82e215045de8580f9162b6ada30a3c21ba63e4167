		<script type="text/javascript">

    function checkDevice()
	{
    	var output="";
    	
		//alert("Device Detection");	
		if(navigator.userAgent.match(/iPhone/i)) 
		{
			//alert("iPhone");
			output="LowEndDevice";
		}
		else if (navigator.userAgent.match(/iPad/i))
		{
			//alert("iPad");
			output="HighEndDevice";
		}
		else if (navigator.userAgent.match(/Blackberry/i))
		{
			//alert("Blackberry");
			output="LowEndDevice";
			if (navigator.userAgent.match(/Android/i))
			{
				//alert("Blackberry Z10");
				output="HighEndDevice";
			}
		}
		else if (navigator.userAgent.match(/Nokia/i))
		{
			//alert("Nokia");
			output="LowEndDevice";
		}
		else if (navigator.userAgent.match(/Android/i))
		{
			//alert("Android");	
			output="MidRangeDevice";
			
			if (navigator.userAgent.match(/Mobile/i))
			{
				//alert("Mobile");
				output="HighEndDevice";
			}
			
			if (navigator.userAgent.match(/GT-/i))
			{
				//alert("GT!!!");
				output="MidRangeDevice";
			}
		}
		else if (navigator.userAgent.match(/Chrome/i))
		{
			//alert("Device Using Chrome");
			output="HighEndDevice";
		}
		else if (navigator.userAgent.match(/Firefox/i))
		{
			//alert("Device Using Firefox");
			output="LowEndDevice";
		}
		
		return output;
	}
</script>


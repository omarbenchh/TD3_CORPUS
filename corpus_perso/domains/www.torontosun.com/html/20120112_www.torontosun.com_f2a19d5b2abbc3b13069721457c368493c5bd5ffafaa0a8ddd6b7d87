<script>
    if(Sunrise.Dfp.pageDfpTags) {
   	
        var overrideUnitName = '';
        var unitName = Sunrise.Dfp.pageDfpTags.getUnitName();   // calls dfp.js function getUnitName()
        														// e.g. /3081/SMCO_ENUR_TSUN_EN_WEB/sports/homepage
        var dfpcvarr = Sunrise.Dfp.pageDfpTags.getCustomVariables();
		// keep one of two
        //var burl = window.location.href;
		
		// default values
		dfpcvarr['nk'] = 'print';
		dfpcvarr['et'] = 'sm';
		dfpcvarr['pr'] = 'tsun';
		
		// new code
		var pageurl = window.location.href;
		// remove ?googfc
		var pageurl = pageurl.replace(/\?.*$/i,"");
		// split url on .com
		var qs = pageurl.split(".com/");
	
		// generate pr code for each paper
		var pr = qs[0].search("ottawasun");
		if (pr > 0) { dfpcvarr['pr'] = 'osun'; }
		pr = qs[0].search("calgarysun");
		if (pr > 0) { dfpcvarr['pr'] = 'csun'; }
		pr = qs[0].search("edmontonsun");
		if (pr > 0) { dfpcvarr['pr'] = 'esun'; }
		pr = qs[0].search("winnipegsun");
		if (pr > 0) { dfpcvarr['pr'] = 'wsun'; }
		// end new code
		
		// new code
		var qvars = qs[1].split("/");

		var xection = qvars[0];
		var subxection = qvars[1];
		var subsubxection = qvars[2];

		if (xection.length == 0) { 
			// homepage
			dfpcvarr['page'] = 'index';
			dfpcvarr['ck'] = 'index';
			dfpcvarr['imp'] = 'index';
		} else { 
			// there is a site.com/something
			
			// story or index
			dfpcvarr['page'] = 'index';
			var page = xection.search("201");
			if (page > -1) {
				// story page
				dfpcvarr['page'] = 'story';
				delete dfpcvarr['imp'];
			}
		
			// author pages
			if (xection == 'author') {
				dfpcvarr['ck'] = 'opinion';
				dfpcvarr['sck'] = 'columnists';
				dfpcvarr['imp'] = 'columnists';
			}
			
			// archive pages
			if (subxection == 'archives') {
				// set dfp
				dfpcvarr['imp'] = 'misc';
				if (xection == 'sunshine-girl') {
					dfpcvarr['sck'] = ["sunshine-girl","misc"]; 
				} else {
					dfpcvarr['sck'] = 'misc';	
				}
				dfpcvarr['ck'] = xection;
				if (xection == 'sunshine-girl') { dfpcvarr['ck'] = 'entertainment'; }
				if (xection == 'tech')  { dfpcvarr['ck'] = 'technology'; }
				if (xection == 'money') { dfpcvarr['ck'] = 'business'; }
			}
		
			// sunshine girl
			if (xection == 'sunshine-girl') { 
		
				// default values for sunshine girl
				dfpcvarr['ck'] = 'entertainment';
				dfpcvarr['sck'] = "sunshine-girl";
				dfpcvarr['page'] = 'index';
				
				if (subxection != undefined) {
					// story
					var sg2 = subxection.search("201");
					if (sg2 > -1) {
						dfpcvarr['sck'] = "sunshine-girl";
						dfpcvarr['page'] = 'story';
					}
				
					// calendar
					if (subxection == 'calendar') {	
						dfpcvarr['sck'] = ["sunshine-girl","calendar"];
						dfpcvarr['page'] = 'story';
					}
				
					// become a sunshine girl
					if (subxection == 'become-a-sunshine-girl') {	
						dfpcvarr['sck'] = ["sunshine-girl","become-a-sunshine-girl"];
						dfpcvarr['imp'] = "become-a-sunshine-girl";
						dfpcvarr['page'] = 'index';
					}
				}
			}
			
			// videos
			if (xection == 'videos') {
				// video
				dfpcvarr['ck'] = 'videos';
				dfpcvarr['imp'] = "videos";
			}
			
			// topic
			if (xection == 'topic') {	
				// topic
				dfpcvarr['ck'] = 'topic';
				dfpcvarr['imp'] = "topic";
				dfpcvarr['sck'] = "topic";
			}
			
			// letter to editor
			if (xection == 'letter-to-editor') {	
				// topic
				dfpcvarr['ck'] = 'opinion';
				dfpcvarr['imp'] = "letter-to-the-editor";
				dfpcvarr['sck'] = "letter-to-the-editor";
			}
		}
			
        if (overrideUnitName) {
            var overridedDfpTag = new Sunrise.Dfp.DFPPageTags(Sunrise.Dfp.pageDfpTags.dfpSiteKey, overrideUnitName, Sunrise.Dfp.pageDfpTags.networkCode);
            unitName = overridedDfpTag.getUnitName();           
        }

        var positionTargeting = 'top';
        var defaultPosition = (Sunrise.Dfp.asideAdsPosition) ? Sunrise.Dfp.asideAdsPosition.shift() : "";
        var adType = 'combination';
        var dfpTagSetCall=Sunrise.Dfp.pageDfpTags.addDfpAdUnit('combination');	// calls dfp.js addDfpAdUnit()

        // googletag is a call to gpt.js
        googletag.cmd.push(function() {
            googletag.defineSlot(unitName, [[300,250],[300,251],[300,600],[300,601],[300,1050]],dfpTagSetCall.divId).addService(googletag.pubads()).addService(googletag.companionAds());

            if (!positionTargeting) {
                positionTargeting = defaultPosition;
            }

            if (positionTargeting) {
                googletag.pubads().setTargeting("pos", positionTargeting);
                if (positionTargeting == 'bottom') { positionTargeting = 'bot'; }
                if (positionTargeting == 'middle') { positionTargeting = 'mid'; }
                googletag.pubads().setTargeting("loc", positionTargeting);
            }

            var cookie = $.cookie("sunmedia-userinfo");
            if (cookie) {
                jsonCookie = $.parseJSON($.base64Decode(cookie));
                googletag.pubads().setTargeting("sungender", jsonCookie.gender);
                googletag.pubads().setTargeting("sunage", jsonCookie.ageGroup);
                googletag.pubads().setTargeting("sunstatus", jsonCookie.userStatus);
            } else {
                googletag.pubads().setTargeting("sunstatus", "Anonymous");
            }
            
         	// custom dfp tags
            for (var key in dfpcvarr) {
            	var dfpcvkey = key;
            	var dfpcvval = dfpcvarr[key];
            	googletag.pubads().setTargeting(dfpcvkey, dfpcvval);
            	//alert(dfpcvkey + " " + dfpcvval);
            }

            googletag.pubads().enableSyncRendering();
            googletag.enableServices();
        });
        document.write('<div id="'+dfpTagSetCall.divId+'"><scr'+'ipt type="text/javascript">googletag.cmd.push(function() { googletag.display("'+dfpTagSetCall.divId+'"); })<\/scr'+'ipt></div>');
    };
</script>


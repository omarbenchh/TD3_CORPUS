<script type="text/javascript">
	$().ready(function(){
		
		$('.b-voter ._byes, .b-voter ._bno').on('click', function(e) {
			if ($(this).hasClass('voted'))
				return false;
			var n = '';
			var ryn = $(this).find('span:last');
			var voter = $(this).parents('.b-voter');
			if (voter.data('name') != '' && typeof voter.data('name') != 'undefined') {
				n = voter.data('name') + '';
				n = n.replace(/nxs(\d+)\//, '');
			}
			$.ajax({
				url: '',
				type: 'GET',
				dataType: 'json',
				timeOut: 1000,
				data: {
					'_ctr': 'voteNew',
					'action': 'vote',
					'ajax': 1,
					'variant': $(this).attr('variant'),
					'id': 324392,
					'name': n,
									},
				success: function (xml) {
					voter.find('._voted').show();
//                        voter.find('._ryes, ._rno').show();
					ryn.text(ryn.text()*1+1);
					voter.find('._byes').addClass('voted');
					voter.find('._bno').addClass('voted');
				}
			});
			return false;
		});
		
		$('.b-voter').on('reload', function(e) {
			var n = '';
			var voter = $(this);
			if (voter.data('name') != '' && typeof voter.data('name') != 'undefined') {
				n = voter.data('name') + '';
				n = n.replace(/nxs(\d+)\//, '');
			}
			$('._voted').hide();
			$.ajax({
				url: '',
				type: 'GET',
				dataType: 'json',
				timeOut: 1000,
				data: {
					'_ctr': 'voteNew',
					'action': 'getVoteData',
					'ajax': 1,
					'id': 324392,
					'name': n,
														},
				success: function (xml) {
					if (xml.canVote != undefined && xml.canVote == 0) {
						voter.find('._byes').addClass('voted');
						voter.find('._bno').addClass('voted');
						voter.find('._ryes, ._rno').show();
					}
					else {
						voter.find('._byes, ._bno').removeClass('voted');
//                        	voter.find('._ryes, ._rno').hide();
					}
//                        $('#kpid').text($('._bigImage').attr('kpid'));
					voter.find('._ryes').text(xml.rate_plus);
					voter.find('._rno').text(xml.rate_minus);
					voter.show();
				}
			});
		});
		$('.b-voter').trigger('reload');
	});
</script>


		





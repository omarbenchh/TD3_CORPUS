        $().ready(function(){
            $('blockquote[data-img]').each(function (index, el) {
                $e = $(el);
                $e.wrap('<div></div>').parent().load('?_tpl=quotation',
                        {
                            _tpl:'quotation',
                            img:$e.attr('data-img'),
                            name:$e.attr('data-name'),
                            position:$e.attr('data-position'),
                            text:$e.html()
                        }
                );
            });
        });

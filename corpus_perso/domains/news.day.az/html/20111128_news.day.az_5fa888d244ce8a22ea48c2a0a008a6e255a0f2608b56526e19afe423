            <div class="col-lg-4 col-md-4"></div>
            <div class="col-lg-4 col-md-4"></div>
            <div class="col-lg-12 col-md-12"><textarea id="comment_text" name="comment_text" cols="30" rows="5" maxlength="1020" placeholder="Ваш комментарий *"></textarea></div>
            <div id="latest_content_loader" class="isotope-loading hidden"></div>
            <div class="col-lg-12"><input id="comment_send_btn" type="submit" value="ОТПРАВИТЬ"></div>
        </div>
        <input type="hidden" name="comment_pid" id="comment_pid" value="0">
    </form>
</div>
<!-- Leave a Comment Ends -->

<script type="text/javascript">
    $("#dayaz_comments").submit(function (event) {
        $('#comment_msg_ok').addClass('hidden');
        $('#comment_msg_noauthor').addClass('hidden');
        $('#comment_msg_notext').addClass('hidden');
        $('#comment_msg_error').addClass('hidden');

        var c_author = $('#dayaz_comments #comment_author').val();
        var c_text = $('#dayaz_comments #comment_text').val();
        var c_pid = $('#dayaz_comments #comment_pid').val();
        c_author = $.trim(c_author);
        c_text = $.trim(c_text);
        var is_ok = true;
        if (!c_author) {
            $('#comment_msg_noauthor').removeClass('hidden');
            is_ok = false;
        }
        if (!c_text) {
            $('#comment_msg_notext').removeClass('hidden');
            is_ok = false;
        }
        if (is_ok) {
            $('#dayaz_comments #latest_content_loader').removeClass("hidden");
            $('#dayaz_comments #comment_send_btn').attr('disabled', 'disabled');
            $.ajax({
                url: '/comments_post.php',
                method: 'POST',
                data: {news_id: news_ID, url: share_URL, author: c_author, text: c_text, pid: c_pid}
            }).done(function (data) {
                $('#comment_msg_ok').removeClass('hidden');
                $('#dayaz_comments #comment_text').val('');
                setTimeout(function () {
                    $('#comment_msg_ok').addClass('hidden');
                    $('#dayaz_comments #comment_pid').val(0);
                    var comment_block = $('#dayaz_comments');
                    $('#comments_form_holder').append(comment_block);
                }, 2500);
            }).fail(function (data) {
                $('#comment_msg_error').removeClass('hidden');
            }).always(function () {
                $('#dayaz_comments #latest_content_loader').addClass("hidden");
                $('#dayaz_comments #comment_send_btn').removeAttr('disabled');
            });
        }
        event.preventDefault();
    });
    $(document).on('click', 'a.comment_reply', function (e) {
        e.preventDefault();
        var comment_block = $('#dayaz_comments');
        var sel = $(this).closest('li');
        $('#dayaz_comments #comment_pid').val(sel.data("id"));
        sel.append(comment_block);
    });
</script>            

        </article>
    </div>
</section>
<!-- /. ARTICLE ENDS ========================================================================= -->



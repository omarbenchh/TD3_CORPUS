        <script>
            $(document).ready(function() {
                $(function() {

                    function insertAfter(referenceNode, newNode) {
                        referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
                    }

                    var notifyHtml = $('#myNotification').html();
                    $('#myNotification').remove();
                    var div = document.createElement("div");
                    var footer = document.getElementById('footer');
                    div.setAttribute("id", "myNotification");
                    div.setAttribute("class", "slidenote block blocktab borderRadius5px")
                    div.innerHTML = notifyHtml;
                    insertAfter(footer, div);


                    var slideAnons = $('#myNotification');
                    var heightFixVar = 0;
                    var top = $('#show-new-prev-next').offset().top - ($(window).height() - (slideAnons.height() - heightFixVar));
                    var sliderVisible = false;
                    var sliderDisable = false;
                    if (navigator.userAgent.indexOf('Android') === -1 && navigator.userAgent.indexOf('iPhone') === -1) {
                        $(window).scroll(function() {
                            var y = $(window).scrollTop();
                            if (y > top) {
                                if (!sliderVisible && !sliderDisable) {
                                    slideAnons.stop().animate({right: '0px'}, 'slow');
                                    sliderVisible = true;
                                }
                            } else {
                                if (sliderVisible) {
                                    slideAnons.stop().animate({right: '-345px'}, 'slow');
                                    sliderVisible = false;
                                }
                            }
                        });

                        $('#myNotification_close').click(function() {
                            slideAnons.stop().animate({right: '-345px'}, 'slow');
                            sliderVisible = false;
                            sliderDisable = true;
                        });

                    }
                    
                                    });
            });

        </script>        </div><!-- // #content - Окончание центранльного контента -->

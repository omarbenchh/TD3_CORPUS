<script type="text/javascript">
//var mo=parseInt(document.getElementById('page-nav').getBoundingClientRect().bottom);
var mo2=parseInt(document.getElementById('top-bar').getBoundingClientRect().bottom);
String.prototype.hasString = function(g) {
    if (typeof g == "object") {
        for (var f = 0,
        h = g.length; f < h; f++) {
            if (!this.hasString(g[f])) {
                return false
            }
        }
        return true
    } else {
        if (this.indexOf(g) != -1) {
            return true
        }
    }
};
c = navigator.userAgent;
ie6 = c.hasString("MSIE 6") && !c.hasString("MSIE 7") && !c.hasString("MSIE 8");
function showHideBar(e){
	var scrollHeight=document.documentElement.scrollTop||document.body.scrollTop;
     e = window.event || e;
     wheel = e.wheelDelta / 120 || -e.detail / 3;
//	if(wheel==1 && scrollHeight>mo ){
//		$("#page-nav").css('position','fixed');
//	}else{
//		$("#page-nav").css('position','static');
//	}
	if(wheel==1 && scrollHeight>mo2 ){
		$("#top-bar").css('position','fixed');
	}else{
		$("#top-bar").css('position','static');
	}
}
if(!ie6){
	/*注册事件*/ 
	if(document.addEventListener){ 
		document.addEventListener('DOMMouseScroll',showHideBar,false); 
	}//W3C 
	window.onmousewheel=document.onmousewheel=showHideBar;//IE/Opera/Chrome 
}

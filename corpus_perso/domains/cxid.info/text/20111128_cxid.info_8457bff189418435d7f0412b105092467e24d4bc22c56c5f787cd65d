	<script type="text/javascript">
    function addComment(comment, nid) {
        if ($('#zcomment' + comment).val())
            $.ajax({
                url: "/news/addcomment",
                type: 'POST',
                data: 'comment=' + $('#zcomment' + comment).val() + '&nid=' + nid + '&parent=' + comment,
                success: function (data) {
                    $('#commentsList').load('/v-luganskoy-oblasti-prosyat-ne-otfutbolivat-problemu-spida-n89800 #commentsList', function () {
                    });
                }
            });
    }

    function addLike(id) {
        $.ajax({
            url: "/news/addlike",
            type: 'POST',
            data: 'id=' + id + '&act=like',
            success: function (data) {
                $('#plusminus' + id).remove();
                if (data)
                    $('#like' + id).text(data);
            }
        });
    }

    function addMinus(id) {
        $.ajax({
            url: "/news/addlike",
            type: 'POST',
            data: 'id=' + id + '&act=unlike',
            success: function (data) {
                $('#plusminus' + id).remove();
                if (data)
                    $('#like' + id).text(data);/* –µ—â–µ —Ü–≤–µ—Ç –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –∑–Ω–∞–∫–∞ –º–µ–Ω—è—Ç—å */
            }
        });
    }
</script>


    $(document).ready(function (e) {
        var lsdid = readCookie('LSDID');
        var uriStr = '';
        if (typeof lsdid != 'undefined' && lsdid != null) {
            uriStr = '/' + lsdid + '/';
            $.ajax('http://auth.liga.net/blocks/menu' + uriStr, {
                type: 'GET',
                data: { redirect: '/news/economics/587035-nalogovaya_nachala_sbor_deklaratsiy_dokhodov_grazhdan_po_novoy_forme.htm' },
                error: function(data) {
                    if (console) console.log(JSON.stringify(data));
                }
            }).done(function (data) {
                        $('#login_links').append(data);
                    });
        } else $.ajax('http://news.liga.net/bitrix/templates/.default/include/edit/static/menu.enter.html')
            .done(function (data) {
                $('#login_links').append(data);
                var link = $('#login_links a').attr('popup_link');
                if (link) {
                    var redirectIndex = link.indexOf('redirect=');
                    if (redirectIndex != -1) {
                        var endRedirectIndex = link.indexOf('&', redirectIndex);
                        var redirect = '/news/economics/587035-nalogovaya_nachala_sbor_deklaratsiy_dokhodov_grazhdan_po_novoy_forme.htm';
                        var beginLink = link.substring(0, redirectIndex);
                        if (endRedirectIndex != -1) var endLink = link.substring(endRedirectIndex, link.length);
                        else endLink = '';
                        $('#login_links a').first().attr('popup_link', beginLink + 'redirect=' + redirect + endLink);
                    }
                }
            });

    });
</script>

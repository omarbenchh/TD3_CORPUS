<script type="text/javascript">
    function topMenuResize(branding)
    {
        var TM = document.getElementById('main_top_menu');
        var IE='\v'=='v';
            var tdToHide0 = document.getElementById('menu_item_file');
        
        //var topMenuWidth = TM.clientWidth;
        if (branding){
            var topMenuWidth = 1023;
        } else {
            var topMenuWidth = TM.clientWidth;
        }
        if(topMenuWidth <= 1024)
        {
                    if(tdToHide0) tdToHide0.style.display = 'none';
                    }
        else
        {
            if (document.all && !document.querySelector) {
                            if(tdToHide0) {tdToHide0.style.display = 'block';}
                            } else {
                            if(tdToHide0) {tdToHide0.style.display = 'table-cell';}
                            }

        }
        return true;
    }
    window.onresize = function(event)
    {
        topMenuResize();
        return true;
    }
    topMenuResize();
</script>        <div class="header">

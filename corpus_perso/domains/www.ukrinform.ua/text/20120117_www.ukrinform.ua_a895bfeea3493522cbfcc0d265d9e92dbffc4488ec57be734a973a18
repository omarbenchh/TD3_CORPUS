<script type="text/javascript" >
function date2timestamp(myDate) {
	 myDate=myDate.split(".");
	 return (new Date(parseInt(myDate[2], 10), parseInt(myDate[1], 10) - 1 , parseInt(myDate[0]), 10).getTime())/1000;
};

$(document).ready(function(){

			$(".moreLink").click(function(){ 
				$('.submenu').fadeSliderToggle()
				return false; 
				});
	
			$(".mobileButton").click(function(){
				$('#mobileMenu').fadeSliderToggle()
				return false;
				});
			
			$(".language div").click(function(){
			    $(".language ul").fadeSliderToggle();
			    return false;
			});

            $(".analiticMobile > li").click(function(){
                $(this).next().fadeSliderToggle()
                return false;
            });
            
            $(window).scroll(function () {
                $(".language ul").hide();
                $("#mobileMenu").hide();
                $(".analiticMobile ul").hide();
            });
			
			$("ul.language li ul li").click(function(){
				window.location.href = $('a', this).attr('href');
			});

			$("ul.leftMenu").html( $("ul.leftMenu:first").html() ); // set menu content for all items 
			$("ul.footerCarte").html( $("ul.greyMenu:first").html() ); // .. and for footer
			$("ul.NewMenu").html( $("ul.MainMenu:first").html() ); // set menu content 
			$("div.footerMenu div ul:first").html( $("ul.leftMenu:first").html() ); // footer: rubrics
			$(".siteMenu form").html(  $(".siteMenu form:first").html() ); // дублирование для мобильных
			$("div#mobileMenu div.leftMenuBody .greyMenu").html(  $("div.submenu .greyMenu").html() ); // меню для мобильных

			/* extended search */
			 $('#leftMenu form div a').click(function(){
                 $(".extendedLink").trigger('click');
                 var str = $('#leftMenu form input[type="text"]').val();
                 $('#extendedSearchField').val(str);
             });
             $('#mobileMenu form div a').click(function(){
                 $(".extendedLink").trigger('click');
                 var str = $('#mobileMenu form input[type="text"]').val();
                 $('#extendedSearchField').val(str);
             });
             $('.extendedLink').tosrus({
                 slides:{ load: 'visible'},
                 buttons: false,
                 drag: false
             });

             $('.rubrics select').selectbox();
             $('select.styled').selectbox();
             
             $(".extendedAncor").click(function(){
                 $(this).toggleClass('searchClose');
                 $(this).next().fadeSliderToggle()
                 return false;
             });

							
				$.datepicker.regional['uk'] = {
						closeText: 'Закрити',
						prevText: '&#x3C;',
						nextText: '&#x3E;',
						currentText: 'Сьогодні',
						monthNames: ['Січень','Лютий','Березень','Квітень','Травень','Червень',
						'Липень','Серпень','Вересень','Жовтень','Листопад','Грудень'],
						monthNamesShort: ['Січ','Лют','Бер','Кві','Тра','Чер',
						'Лип','Сер','Вер','Жов','Лис','Гру'],
						dayNames: ['неділя','понеділок','вівторок','середа','четвер','п’ятниця','субота'],
						dayNamesShort: ['нед','пнд','вів','срд','чтв','птн','сбт'],
						dayNamesMin: ['Нд','Пн','Вт','Ср','Чт','Пт','Сб'],
						weekHeader: 'Тиж',
						dateFormat: 'dd.mm.yy',
						firstDay: 1,
						isRTL: false,
						showMonthAfterYear: false,
						yearSuffix: ''};
					             
  			$("#search_dp_from").datepicker({
				dateFormat: "dd.mm.yy",
				minDate: "-5Y", 
				maxDate: "0",
				defaultDate: "-1m",
				changeMonth: true,
				numberOfMonths: 2,
				onClose: function( selectedDate ) {
         	        $( "#search_dp_to" ).datepicker( "option", "minDate", selectedDate );
         	      }
         	    });
			$( "#search_dp_to" ).datepicker({
				//regional: "ru",
				minDate: "-5Y", 
				maxDate: "0",
				dateFormat: "dd.mm.yy",
				defaultDate: "0",
				changeMonth: true,
				numberOfMonths: 2,
				onClose: function( selectedDate ) {
         	        $("#search_dp_from").datepicker( "option", "maxDate", selectedDate );
         	      }
         	    });
			

			$.datepicker.setDefaults( $.datepicker.regional[ "uk" ] );

			$('form[name=extended_search]').submit(function() {
				$('#search_date_beg').val( date2timestamp( $('#search_dp_from').val() ) );
				$('#search_date_end').val( date2timestamp( $('#search_dp_to').val() ) );
				});

			  
             /* extended search */



				// social buttons for blogs/interviews/infographics
				setTimeout(function() { 
				
					// counter values
					var i=1;
					$('#ya_share span.b-share-btn__wrap span:nth-child(even)').each(function(){ 
			 			$('#sharedFixed ul li:nth-child('+i+') div').html(  parseInt( $(this).html()) );
						i++;  
					}) ;

					// link values
					var i=1;
					$('#ya_share span.b-share-btn__wrap a').each(function(){ 
						var $socialSource = $( this );
						$('#sharedFixed ul li:nth-child('+i+') a').attr( 'href', $socialSource.attr('href') );
						i++;
						}) ;
				
					i=null;

					// дублирование контента для нижнего блока
					$('#sharedFixedDouble').html( $('#sharedFixed').html() ); 

					// анимация
					    $("#sharedFixed").animate({
					        opacity: '1',
					    });
				    
				}, 6000);
					
             
				

		})
		
		</script>
<script type="text/javascript">
$(document).ready(function(){
    var is_mobile = false;
    if( $('#some-element').css('display')=='none') {
        is_mobile = true;
    }

    /*
    var slider = $('.sliderMenu ul').bxSlider({
        infiniteLoop: false,
        hideControlOnEnd: true,
        pager: false,
        touchEnabled: !is_mobile
    });
	*/
	
    var slider3 = $('.pressTabs ul').bxSlider({
        pagerCustom: '.bxPager',
        controls: true,
        maxSlides: 1,
        minSlides: 1,
        moveSlides: 1,
        // video: true,
        onSlideBefore: function(){ // action on change slide
        	stopVideo(); // stop all playing video
          },
        touchEnabled: !is_mobile
    });
   
    var slider2 = $('.pressAnons ul').bxSlider({
        infiniteLoop: false,
        hideControlOnEnd: true,
        pager: false,
        touchEnabled: !is_mobile
    });
    
    $('.projects ul').bxSlider({
        controls: false,
        maxSlides: 1,
        touchEnabled: true
    });

    $('.sliderButton').click(function(e){
        e.preventDefault();
    });

    $('.sliderBack').click(function(e){
        e.preventDefault();
    });

});


$(window).bind("load resize",function() {
    $('#mobileMenu').each(function(){
        if($(this).css("position") == "static") {
            $(this).css({height: $(window).height() - 45});
        }
    });

    $('.infoBlock').each(function(){
        if($(this).css("width") == "290px") {
            $(".sliderButton").trigger('click');
        } else {
            $(".sliderBack").trigger('click');
        }
    });
});
</script>


<style type="text/css">
	.sliderMenu {display:block;}
</style>
        


    function checkCookieDevice() {
        return ( document.cookie.indexOf( "toprbc_device" ) >= 0 ) ? 1 : 0;
    }

    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){

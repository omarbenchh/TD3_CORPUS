<script type="text/javascript">
var arid = [];
var linkurl = [];
var content = [];
$.ajax({
       type: "GET",
       url: "http://www.xinhuanet.com/video/titleurl.xml",
			async: false,
       dataType: "xml",
       success: function (xml) {
           $(xml).find("item").each(function (i) {
              
               arid[i] = $(this).find("arid").text();
							linkurl[i] = $(this).find("htmlurl").text();
							content[i] = $(this).find("atitle").text();
           });
       }
 })
 
 if(parseInt(arid.length,10)>0){ 	
 	var thisurl = window.location.href
 	var thisarticleid = thisurl.substring(thisurl.lastIndexOf("."),thisurl.lastIndexOf("c_")+2);
 	var thisindex = -1;
 	var preurl;
 	var nexturl;
 	for(var i=0;i<arid.length;i++){
 		if(parseInt(thisarticleid,10)==parseInt(arid[i],10)){
 			thisindex = i;
 			break;
 		}
 	}

 	if(parseInt(thisindex,10)<=0){
 		nexturl = linkurl[parseInt(thisindex,10)+1];
 		$("#prearticle").parent().hide();
 		$("#nextarticle").parent().attr("align","left");
 		$("#nextarticle").attr("href",nexturl);
 	}else if(parseInt(thisindex,10)===(parseInt(arid.length,10)-1)){
 		preurl = linkurl[parseInt(thisindex,10)-1];
 		$("#nextarticle").hide();
 		$("#prearticle").parent().attr("align","left");
 		$("#prearticle").attr("href",preurl);
 	}else{
 		preurl = linkurl[parseInt(thisindex,10)-1];
 		nexturl = linkurl[parseInt(thisindex,10)+1];
 		$("#prearticle").attr("href",preurl);
 		$("#nextarticle").attr("href",nexturl);
 	}
 }else{
 	$("#prearticle").hide();
 	$("#nextarticle").hide();
 }
 
</script>

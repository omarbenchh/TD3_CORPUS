<script type="text/javascript">
$(function() {
    var saved_stories = $.stuffStorage.get_item('saved_stories');
    if (typeof saved_stories === "string") {
        saved_stories = JSON.parse(saved_stories);
    }
    if (saved_stories && saved_stories[6244541]){
        $("#story_saved").show();
        $("#story_unsaved").hide();
    } else {
        $("#story_saved").hide();
        $("#story_unsaved").show();
    }
});

</script>


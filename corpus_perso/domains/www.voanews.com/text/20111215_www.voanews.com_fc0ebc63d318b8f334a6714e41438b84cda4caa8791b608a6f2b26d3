<script type="text/javascript">
    var isOnlyForRegistered = false; 
    function WriteComment(articleid,replyid) {
        var isLoggedIn=false;
        if(typeof(_isLoggedIn)!='undefined')
            isLoggedIn=_isLoggedIn;
        if(isOnlyForRegistered && !isLoggedIn)
            LoginLogoutSignupClick('login', 0);
        else {
            var userName = $("#liMembershipUserName").find('span').text();
            $("#replycomment" + replyid).html('<iframe class="commentIframe" id="commentiframe' + replyid 
                 + '" frameborder="0" onload="SetIfrHeight(this)" width="580" height="450" marginwidth="0" marginheight="0"' +
                 'scrolling="no" src="../../comment.aspx?articleid=791771&threadid=' + replyid + '&username=' + escape(userName) +'" />');
        }
    }
//  this was in source:  http://www.voachinese.com

    function SetIfrHeight(elm) {
        var oIfr = $(elm);
        oIfr.css("height", $("body", oIfr.contents()).outerHeight(true) + 15 + "px");
    }

    function CloseComment(replyid) {
        $("#replycomment" + replyid).html('');
    }
    
    function ClosedForum(replyid) {
        $("#replycomment" + replyid).html('<div class="boxwidget_part instruction">评论期已过。</div>');
    }
    
    function LoginLogoutSignupClick(pageToGo, replyid) {
        $('div[id^=replycomment]').html('');
        openIframeModal(pageToGo);
        //window.location ='http://www.voachinese.com'+pageToGo;
    }
</script>

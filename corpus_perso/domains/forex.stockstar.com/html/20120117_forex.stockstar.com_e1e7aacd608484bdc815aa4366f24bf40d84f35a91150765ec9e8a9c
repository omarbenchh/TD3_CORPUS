    $(function () {
        //分页
        var $current = $("#Page a.current"); $("body").keydown(function (event) { if (event.keyCode == 37) { var $prev = $current.prev("a"); if ($prev != null && $prev.length > 0) { window.location.href = $prev.attr("href") } } if (event.keyCode == 39) { var $next = $current.next("a"); if ($next != null && $next.length > 0) { window.location.href = $next.attr("href") } } });
        //报价
        if (window.FEED_QUOTE && window.FEED_QUOTE.length > 0) {
            //手风琴效果
            var $a = $("#list-related-stock dl"); $a.find("dt").click(function () { var $t = $(this).next(); if ($t.is(":hidden")) { $t.show(); $(this).find(".stretch").addClass("minus"); $(this).parents("dl").siblings("dl").find("dd").hide().end().find(".stretch").removeClass("minus") } else { $t.hide(); $(this).find(".stretch").removeClass("minus") } }); $a.eq(0).find("dt").trigger("click"); $a.find("a").click(function (e) { e.stopPropagation() });
            //报价
            var s = new QuoteBinder({ data: window.FEED_QUOTE || [], tag: "sQuoteTag_", interval: 600, format: function (a) { return a[4] > 0 ? "<span class=\"red\">" + a[3].toFixed(2) + " " + a[5] + "</span>" : (a[4] < 0 ? "<span class=\"green\">" + a[3].toFixed(2) + " " + a[5] + "</span>" : (a[3] == 0 ? a[6].toFixed(2) + "  0.00%" : a[3].toFixed(2) + "  0.00%")) } })
        }


    //360合作推广
    (function () {
        window.soNewsCallback_10101 = function (data) {
            //            
            var newsTemplate = '<a href="{shURL}"><img src="{imgURL}" alt="{title}" width="120" height="96"></a><p><a href="{shURL}">{title}</a></p>';
            var template = function (template, filler) {
                for (attr in filler) {
                    template = template.replace(new RegExp('{' + attr + '}', 'img'), filler[attr]);
                }
                return template;
            };
            //加载数据
            var getByClass = function (cls, p) {
                var arr = [],
                            reg = new RegExp("(^|\\s+)" + cls + "(\\s+|$)", "g"),
                            nodes = (p || document).getElementsByTagName("*"),
                            len = nodes.length;
                for (var i = 0; i < len; i++) {
                    if (reg.test(nodes[i].className)) {
                        arr.push(nodes[i]);
                        reg.lastIndex = 0;
                    }
                }
                return arr;
            };
            var li = getByClass('coop', document.getElementById('cfsh'));
            var newsList = data['data'];
            var newsHTML = '';
            for (var i = 0, len = newsList.length; i < len && i < 2; i++) {
                var news = newsList[i],
                            title = news['title'],
                            imgURL = news['img'],
                            shURL = news['url'];
                (function (o) {
                    li[i].innerHTML = template(newsTemplate, { title: title, imgURL: imgURL, shURL: shURL });
                })(i);
            }
        };
        soNewsCallback_10101({
	data: [{"img":"http://i.ssimg.cn/news/2015/09/22/8d6ee54e36ca4b05ae501f393e51ef8a.jpg","url":"http://sh.qihoo.com/?src=lm&mt=10101&v=26564&p=10101_1#resoutu","title":"中国最性感女总裁"},{"img":"http://i.ssimg.cn/news/2015/09/22/60560f8cc5904a2c980d2bab54d5bab1.jpg","url":"http://sh.qihoo.com/?src=lm&mt=10101&v=26564&p=10101_2#resoutu","title":"乌克兰街头美女成“灾”"}]
});

(function(win){
	/* 展示打点 */
	new Image().src = 'http://stat.lianmeng.360.cn/media/i.gif?ls=10101&_t=' + Math.random();
	/* 点击打点 */
	var isLink = function(elem) {
		return elem.tagName == 'A';
	}
	var clickHandler = function(e) {
		e = win.event || e;
		var target = e.target || e.srcElement;
		var count = 0;
		while (count < 3 && !isLink(target)) {
			target = target.parentNode;
			count++;
		}
		if (!target || !isLink(target)) {
			return;
		}
		var href = target.href;
		if (!href || href.indexOf('http://sh.qihoo.com/') != 0) {
			return;
		}
		new Image().src = 'http://stat.lianmeng.360.cn/media/c.gif?ls=10101&t=' + encodeURIComponent(win.location.href) + '&_t=' + Math.random();
	}
	
	if (win.QIHOO_NEWS_CLICK_TRACK) {
		return;
	}
	var container = document.body;
	if (container.addEventListener) {
		container.addEventListener('click', clickHandler, false);
	} else {
		container.attachEvent('onclick', clickHandler);
	}
	win.QIHOO_NEWS_CLICK_TRACK = 1;
})(window);
    })();

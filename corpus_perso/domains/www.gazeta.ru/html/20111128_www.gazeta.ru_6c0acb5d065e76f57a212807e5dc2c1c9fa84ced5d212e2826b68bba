$(function() {
$('*[data-init]').each(function() {
var $this = $(this);
var fun = window[$this.data('init')];
//if (window.console) console.log(['init', this, fun]);
if (fun && typeof(fun) == 'function') {
fun($this);
//$this.data('init', '');
}
});
$('.common_height').each(function(){
$this=$(this);
var maxh = 0; // ищем самую высокую заметку и подставляем ее высоту всем остальным
$this.find('article').each(function(){
var h = $(this).height();
if (h > maxh) maxh = h;
});
$this.find('article').each(function(){
$(this).height(maxh);
});
});
$('#but_dinamic_240_1').click(function() {

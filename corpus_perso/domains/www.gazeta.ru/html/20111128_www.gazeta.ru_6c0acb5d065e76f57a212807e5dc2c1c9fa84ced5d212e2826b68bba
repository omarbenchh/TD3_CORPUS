// ---------------------------------- Fancy_box ------------------------------------
function close_netot_fancy_box(){
$('.fancy_box').fadeOut(100, function(){$(this).remove()});
return false;
}
$(window).keydown(function(event){
if(event.keyCode == 27){
close_netot_fancy_box();
}
});
function open_netot_fancy_box(source) {
$('<div class="fancy_box" style="display:none">\
<a href="#" class="close">Закрыть</a>\
' + source + '\
</div>\
')
.appendTo('body')
.fadeIn(100)
.click(function(){
close_netot_fancy_box();
return false;
})
var $img = $('.fancy_box img');
$img.load(function(){
var img_height = $img.height();
var img_width = $img.width();
var window_height = $(window).height();
var window_width = $(window).width();
if(img_height > window_height){
img_height = window_height - 20;
$img.css('max-height', img_height + 'px');
img_width = $img.width();
};
if(img_width > window_width){
img_width = window_width - 20;
$img.css('max-width', img_width + 'px');
img_height = $img.height();
};
$img.css({
'marginTop' : '-' + img_height/2 + 'px',
'top' : '50%',
'marginLeft' : '-' + img_width/2 + 'px',
'left' : '50%'
});
});
return false;
}
$('a.zoom_in').click(function(){
var source = $(this).attr('href');
if(source.match('#fb')){
source = $(this).closest('article').find(source).html();
} else {
source = '<img src="' + source + '">';
}
return open_netot_fancy_box(source);
})
window.open_netot_fancy_box = open_netot_fancy_box;
window.close_netot_fancy_box = close_netot_fancy_box;
// ---------------------------------- END Fancy_box ------------------------------------
});
</script>


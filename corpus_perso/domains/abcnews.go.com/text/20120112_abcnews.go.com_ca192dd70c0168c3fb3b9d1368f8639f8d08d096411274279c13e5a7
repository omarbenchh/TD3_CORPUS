<script>
var abcn_nativo = {};
abcn_nativo.path = '.b_col #ff_lite:first .ffl_wrapper';
abcn_nativo.string = '<div class="nativo"></div>';
abcn_nativo.embedded = false;
$(abcn_nativo.path + ' > div').each(function(i){
if(i===5){
$(this).after(abcn_nativo.string);
abcn_nativo.embedded = true;
}
});
if(!abcn_nativo.embedded){
$(abcn_nativo.path + ' .last').after(abcn_nativo.string);
}

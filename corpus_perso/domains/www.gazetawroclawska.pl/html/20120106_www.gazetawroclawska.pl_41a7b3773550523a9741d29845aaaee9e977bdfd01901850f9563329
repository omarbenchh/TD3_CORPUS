<script type="text/javascript">
  GR('zaladujPlikJS', 'http://s-pt.ppstatic.pl/p/js/komentarze.js?11140890');


  jQuery('#art-komentarze input[name="usun_watek"]').click(function() {
      if (confirm("Czy na pewno chcesz usunąć wątek?")) {
          return true;
      }
      return false;
  });
    
  jQuery('#material-komentarze input[name="dodaj_komentarz"]').click(function() {
      var submit = jQuery(this);
      if (submit.data('wyslane') === true) {
          return false;
      } else {
          submit.data('wyslane', true);
      }
  });
    
  GR("o_formularz_inicjalizuj", "form-komentarz", {multi: "fK", "plik": sUrl + "/p/js/komentarze_formularz.js"});
  
  jQuery(document).ready(function(){
    var max = 4000;
    
    function przelicz_znaki() {
      var self = jQuery('#komentarz-tresc');
      if (self.length == 0) {
        return;
      }
      if (jQuery(self).val().length > max){
        jQuery(self).val(jQuery(self).val().substr(0, max));
      }

      ilosc_znakow = max - jQuery(self).val().length;
      ilosc_enterow = jQuery(self).val().split(/\\n/).length - 1;
      ilosc_znakow = ilosc_znakow + ilosc_enterow - ilosc_enterow*100;
      jQuery('#komentarz-tresc-limit > b').html(''+(ilosc_znakow)+'');
    }
  
    przelicz_znaki();
    jQuery('#komentarz-tresc').keyup(function() {
      przelicz_znaki();
    });
  });
  podepnij_kciuki = function(idx, self) {
    var tmp = jQuery(self).attr('id').split('-'), id_post = tmp[1], ocena = tmp[0] == 'plus' ? 1 : -1, 
      id_watek = tmp[2], strona = tmp[3], sortowanie = tmp[4];
    jQuery(self).click(function(event) {
      event.preventDefault();
      ocen(id_post, ocena, id_watek, strona, sortowanie, jQuery(this).find('span'));
    });
  };
  
  jQuery('.kciukGora, .kciukDol').each(podepnij_kciuki);
  
  
  (function(){
    var dane_kontroler = {
      katalog : "/artykul/",
      wartosc : "488595,ospa-party-tu-celowo-zarazaja-dzieci-ospa,id,t",
      rozszerzenie : "html",
      strona : 1,
      async: 1,
      regionalne: 1
    };
    id_modul = 1000;
    liczba_stron = 2;

    function pokaz_formularz_z_bledami() {
      odpowiedz(0, 0, 1);
    }

    
    

    var wiecej = jQuery(document.createElement('input')).attr('id', 'komentarze-wiecej').attr('type', 'button')
      .val('Pokaż więcej komentarzy').addClass('button').click(function() {
        pobierz_nastepna_strone(false);

        if (liczba_stron <= dane_kontroler.strona) {
          jQuery('#komentarze-wiecej').remove();
        }
      });
      
      function pobierz_nastepna_strone(wszystkie) {
          dane_kontroler.strona++;
          jQuery.get('/ajax/laduj_asynchronicznie/'+id_modul+'.html', dane_kontroler, function(data) {
            jQuery('#art-komentarze').append(data).find('.kciukGora, .kciukDol').each(podepnij_kciuki);
            if (wszystkie && dane_kontroler.strona < liczba_stron) {
                pobierz_nastepna_strone(wszystkie);
            }
          });
      }
      
      var wszystkie = jQuery(document.createElement('input')).attr('id', 'komentarze-wszystkie').attr('type', 'button')
      .val('Pokaż wszystkie komentarze').addClass('button').click(function() {
        jQuery('#komentarze-wiecej').remove();
        jQuery(this).hide();
        pobierz_nastepna_strone(true);
      });
    jQuery('#art-komentarze .stronicowanie').remove();

    if (liczba_stron > dane_kontroler.strona) {
      jQuery('#art-komentarze').after(wiecej);
      
            
      tr = [];
      
    }
    
    tr = [];
    tr.push({elements: GR('$$', 'regulaminLink', $('dodaj-komentarz')), href: '/onclick/komentarze/dodaj-komentarz-regulamin-link'});
    tr.push({elements: GR('$$', 'regulaminLink', $('dodaj-odpowiedz')), href: '/onclick/komentarze/dodaj-odpowiedz-regulamin-link'});
    tr.push({elements: GR('$$', 'przyciskWyslij', $('dodaj-komentarz')), href: '/onclick/komentarze/dodaj-komentarz-wyslij'});
    tr.push({elements: GR('$$', 'przyciskWyslij', $('dodaj-odpowiedz')), href: '/onclick/komentarze/dodaj-odpowiedz-wyslij'});
    tr.push({elements: [$('link-zwin-rozwin-wszystkie')], href: '/onclick/komentarze/zwin-wszystkie'});
    tr.push({elements: GR('$$', 'kciukGora', $('art-komentarze')), href: '/onclick/komentarze/kciuk-gora'});
    tr.push({elements: GR('$$', 'kciukDol', $('art-komentarze')), href: '/onclick/komentarze/kciuk-dol'});
    tr.push({elements: GR('$$', 'komentarzOdpowiedz', $('art-komentarze')), href: '/onclick/komentarze/odpowiedz'});
    GR('$$', 'ikonaNarzedzia', $('art-komentarze'), '', function(e) {
      var nazwa = e.innerHTML.replace(" ", "-");
      tr.push({elements: [e], href: '/onclick/komentarze/' + nazwa.trim()});
    });
    tr.push({elements: [$('komentarze-wiecej')], href: '/onclick/komentarze/pokaz-wiecej'});
    GR('oTrackEvents', tr);
    
  })();


    (function($){
        $(document).on('click', '#art-komentarze .koment .wiecej', function() {
            $(this).parents('.tresc').find('.pelnaTresc').removeClass('schowaj');
            $(this).parents('.krotkaTresc').addClass('schowaj');
        });
        $(document).on('click', '#art-komentarze .koment .mniej', function(){
            $(this).parents('.pelnaTresc').addClass('schowaj');
            $(this).parents('.tresc').find('.krotkaTresc').removeClass('schowaj');
        });
        if (window.location.hash && window.location.hash.match(/komentarz-\d/)) {
            $(window).load(function() {
                setTimeout(function() {
                    window.scrollTo(0, $(window.location.hash).offset().top - 50);
                }, 1000);
            });
        }
    })(jQuery)
</script>

<script type="text/javascript">
function showRating(element, value) {
    $(element).find('li').each(function(i){
        if (i < value)
        	$(this).addClass('selected');
        else
        	$(this).removeClass('selected');
    });                
}

function setRating(element, value) {
	var currentValue = $(element).attr('class').replace(/.*selected/, '');
	$(element).removeClass('selected'+currentValue);
	$(element).addClass('selected'+value);
}

$('ul.rating li').each(function(j){
    var elem = $(this).parent();
    
	$(this).bind('mouseover', function(){
		var value = $(this).text();
        showRating(elem, value);
    });

    $(this).bind('mouseout', function(){
        var value = $(this).parent().attr('class').replace(/.*selected/, '');
        showRating(elem, value);
    });

    $(this).bind('click', function(){
    	var value = $(this).text();
    	setRating(elem, value);

        var url = '/?controllerName=article&page=1&contextId=2866283&siteId=5&action=changeRating&bizClass=article&bizId=2866283&rateValue='+value;
        $.get(url, function(d){
            $('p.rating').show();
        });
    });
});

var elem = $('ul.rating');
var value = $(elem).attr('class').replace(/.*selected/, '');
showRating(elem, value);
</script>


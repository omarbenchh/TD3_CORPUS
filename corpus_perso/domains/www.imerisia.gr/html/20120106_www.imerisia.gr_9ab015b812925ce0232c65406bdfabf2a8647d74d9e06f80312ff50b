    <div class="leftcol"><script type="text/javascript">

var Scroller=0;
var Timer;
function ScrollLeft()
{
Timer = setInterval("document.getElementById('scroltainer').scrollLeft -= 4", 5);
}

function ScrollRight()
{
Timer = setInterval("document.getElementById('scroltainer').scrollLeft += 4", 5);
}

function ScrollMid()
{
Timer = setInterval("document.getElementById('scroltainer').scrollLeft += 14", 5);
var t=setTimeout("clearme()",1000);
}

function ScrollEnd()
{
Timer = setInterval("document.getElementById('scroltainer').scrollLeft += 18", 5);
var t=setTimeout("clearme()",1270);
}

function clearme()
{
window.clearInterval(Timer)
}

function getScroll(nn){
   Scroller=nn;
}

function setScroll()
{
document.getElementById('scroltainer').scrollLeft=Scroller;
}
</script>


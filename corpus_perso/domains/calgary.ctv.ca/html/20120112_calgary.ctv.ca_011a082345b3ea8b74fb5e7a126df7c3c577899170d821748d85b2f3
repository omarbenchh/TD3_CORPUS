			window.axisAuthApi_pageCfg = {
            destinationCode: 'ctvnews_web',
            id: 'ctvnews', // id for tracking
            preloadAIS:false
        }; 



        Requests = {
            QueryString: function (item) {
                var svalue = location.search.match(new RegExp("[\?\&]" + item + "=([^\&]*)(\&?)", "i"));
                return svalue ? svalue[1] : svalue;
            }
        }

        var destCode = Requests.QueryString('destinationCode');
        if (destCode) { axisAuthApi_pageCfg.destinationCode = destCode; }

        var mobile = Requests.QueryString('mobile');
        if (mobile) { axisAuthApi_pageCfg.mobile = { width: mobile } }

        var bilingual = Requests.QueryString('bilingual');
        if (bilingual) { axisAuthApi_pageCfg.override = { lang: { bilingual: bilingual } } }


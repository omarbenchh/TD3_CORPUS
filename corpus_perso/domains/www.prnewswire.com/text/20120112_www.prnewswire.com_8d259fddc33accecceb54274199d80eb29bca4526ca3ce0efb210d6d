							<div class="col-sm-12 large-bottom-margin" id="sitePreview">
							<div class="" id="frameContainer">
								<img src="http://content.prnewswire.com/designimages/loading.gif" alt="" />
							</div>
							</div>
						</div>
						<script>
						$(function () {

							var $sitePreviewToggle = $('#site-preview-toggle');
							var $frameContainer = $('#frameContainer');
							var sitePreviewIframe;

							$sitePreviewToggle .on({
								click: function(event) {
								  if (sitePreviewIframe) {
								    sitePreviewIframe.appendTo($frameContainer);
								    sitePreviewIframe = null;
								  } else {
								    sitePreviewIframe = $('#previewFrame').detach();
								  }
								  event.preventDefault();
								}
							});

							$sitePreviewToggle .on({
								click: function(event) {
									var $this = $(this);
									var originalText = $this.data('text-original');
									var swapText = $this.data('text-swap');

									if($this.text() === originalText) {
										$this.text(swapText)
									} else {
										$this.text(originalText);
									}

									event.preventDefault();
								}
							});
						});
						</script>
						</main>

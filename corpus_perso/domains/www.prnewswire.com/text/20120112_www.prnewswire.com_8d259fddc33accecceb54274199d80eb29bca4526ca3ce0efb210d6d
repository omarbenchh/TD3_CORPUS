			function showIframe(){
		              requestCrossDomain("http://www.ford.com", function(results) {
        if(results != 'false')
        {
         var HTML = "<iframe id='previewFrame' onload='this.style.display='block';' class='site-preview' frameborder='0' allowTransparency='true' src='http://www.ford.com'></iframe>";
         document.getElementById('frameContainer').innerHTML = HTML;
       }
       else
       {
         $("div.previewlinks").css("display", "none");
         $("div#sitePreview").css("display", "none");
       }
     });
            }



	/* tnt tracking - starts*/
	function sch_trackTTLink(mboxName,link,linkName)
	{ 
		var cookieVal =  readCookie('prncom_schF');
		mboxUpdate(mboxName, 'click='+linkName);
		setTimeout(function () { 
			document.location.href = link.href; 
		}, 500); 
	}
	/* tnt tracking - end*/

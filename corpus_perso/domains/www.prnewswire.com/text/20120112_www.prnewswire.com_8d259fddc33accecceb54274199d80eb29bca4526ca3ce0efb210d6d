<script>
var clickedCarousel;
var clickedSlide;
var windowWidth;
var carouselContent = $('.prn-carousel-modal').html();
$('.photo').hammer().on('tap', function(event){
  event.preventDefault();
  windowWidth = $(window).width();
  clickedSlide =  $(event.target).parents('.slide').prevAll().length;
  if ( windowWidth > 480 ) {
    clickedCarousel = $(event.target).parents('.photo').prevAll().length + clickedSlide * 3;
  } else {
    clickedCarousel = clickedSlide;
  }

});

	 // Flexslider
	 $('#flexslider-modal').on('shown.bs.modal', function () {


				$('#slider').flexslider({
                animation: "slide",
                controlNav: false,
                animationLoop: false,
                video: true,
                slideshow: false,
                touch: true,
                itemMargin: 45,
                sync: "#carousel",
                smoothHeight: true,
                  useCSS: true
                });
              $('#carousel').flexslider({
                animation: "slide",
                controlNav: false,
                directionNav: true,
                animationLoop: false,
                slideshow: false,

                itemWidth: 150,
                itemMargin: 5,
                useCSS: false,
                  //thins type if true
                  asNavFor: '#slider'
                });
              var carouselItem = $('#carousel li').eq(clickedCarousel);
              var carouselItemHeight = carouselItem.outerHeight();
              carouselItem.trigger('click');
              $('#slider .flex-viewport').delay(2000).stop(true, true).innerHeight(carouselItemHeight);

  }).on('hidden.bs.modal', function () {
    $('.prn-carousel-modal .modal-dialog').remove();
    $('.prn-carousel-modal').html(carouselContent);
  });
  </script>
  <script type="text/javascript">

   function getposOffset(overlay, offsettype){
    var totaloffset=(offsettype=="left")? overlay.offsetLeft : overlay.offsetTop;
    var parentEl=overlay.offsetParent;
    while (parentEl!=null){ totaloffset=(offsettype=="left")? totaloffset+parentEl.offsetLeft : totaloffset+parentEl.offsetTop; parentEl=parentEl.offsetParent; }
    return totaloffset;}

		</script>

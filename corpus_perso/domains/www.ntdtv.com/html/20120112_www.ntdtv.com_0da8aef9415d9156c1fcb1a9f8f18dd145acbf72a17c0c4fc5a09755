googletag.cmd.push(function() {
	googletag.defineSlot('/37445998/NTDtv_cn_roadblock_336x280', [[336, 280], [300, 250], [600, 400]], 'div-gpt-ad-1448378036145-0').addService(googletag.pubads());
	googletag.pubads().addEventListener('slotRenderEnded', function(event) {
		var slotname = event.slot.getAdUnitPath();
		console.log('rendered end');
		console.log(slotname);
		if(slotname.indexOf('NTDtv_cn_roadblock_336x280') > -1) {
			console.log('ntdtv_cn_gw roadblock ' + event.isEmpty);
			if(event.isEmpty === false) {
				$("#roadblockads").show();
			}
		}
	});
	googletag.enableServices();
});


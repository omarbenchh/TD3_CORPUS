 <script>
$(function() {
$( ".infodialog" ).dialog({autoOpen: false,modal: true});
$( "button" ).button();
});
function Comment_up( e ){
    $.ajax({
        method: "GET",
        url: "/publib/CommentRating.php",
        data: { vote_id : e, score : "up" },
        dataType:'json',
        cache: false,
        success : function( msg ) {
 //           alert( "Data Saved: "+e+" "+msg.msg.up );
            $( "#st_up_comm_"+e ).html( '<span id="st_up_comm_'+e+'">'+msg.msg.up+'</span>' );
            $( "#st_down_comm_"+e ).html(  '<span id="st_down_comm_'+e+'">'+msg.msg.down+'</span>'  );
            $( "#status_comm_"+e ).attr( "title", ' Всего '+msg.msg.count+': +'+msg.msg.up+' и -'+msg.msg.down+' ' );
        }   
    })
}

function Comment_down( e ){ 
    $.ajax({
        method: "GET",
        url: "/publib/CommentRating.php",
        data: { vote_id : e, score : "down" },
        dataType:'json',
        cache: false,
        success : function( msg ) {
//            alert( "Data Saved: "+msg.status+" "+msg.msg.down );
            $( "#st_up_comm_"+e ).html( '<span id="st_up_comm_'+e+'">'+msg.msg.up+'</span>' );
            $( "#st_down_comm_"+e ).html(  '<span id="st_down_comm_'+e+'">'+msg.msg.down+'</span>'  );
            $( "#status_comm_"+e ).attr( "title", ' Всего '+msg.msg.up+': +'+msg.msg.up+' и -'+msg.msg.down+' ' );
        }   
    })
    
}

</script>

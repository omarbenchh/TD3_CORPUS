<script type="text/javascript">
$(document).ready(function(){
	var fixedPlaceOffset=0;
	var bottomPlaceOffset=0;
	var OffsetBottom=0;
	fixedPlaceOffset = $('#FixBlock').offset().top;
	OffsetBottom = $('#FixBlock').height();
	bottomPlaceOffset = $('#footer').offset().top;
	var bts=bottomPlaceOffset-OffsetBottom;  
	$(window).scroll(function () {
		var $block = $('#FixBlock');
		var top = $(document).scrollTop();
		if( top >= fixedPlaceOffset && top < bottomPlaceOffset-OffsetBottom-20){ 
			$block.addClass('BlockFixed');
			$('#FixBlock').css( 'top' , 0 );
		}else{
			if( top > bottomPlaceOffset-OffsetBottom-20 ){
				$('#FixBlock').css( 'top' , bts-top );
			}else{
				$block.removeClass('BlockFixed');
			}
		}
	});
});
</script>

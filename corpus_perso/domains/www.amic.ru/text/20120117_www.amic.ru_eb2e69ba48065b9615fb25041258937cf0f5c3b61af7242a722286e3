<script>
function show_bar(ev, id) {
    MouseX = ev.clientX + document.documentElement.scrollLeft;
    MouseY = ev.clientY + document.documentElement.scrollTop;
    obj = document.getElementById("hint[" + id + "]");
    obj.style.top = (MouseY + 10)+"px";
    obj.style.left = (MouseX+1)+"px";
    obj.style.visibility = "visible";
}
function hide_bar(id) {
    document.getElementById("hint[" + id + "]").style.visibility="hidden";
}
</script>

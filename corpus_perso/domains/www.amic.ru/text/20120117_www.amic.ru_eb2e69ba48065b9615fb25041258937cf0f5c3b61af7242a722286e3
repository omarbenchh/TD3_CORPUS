    jQuery(function($) {
        newsImg = $("#news_body img");
        newsImg.wrap(function(){
            imgTitle = $(this).attr("title");
            float = $(this).attr("align");
			if( float == undefined ){ float = 'none'; }
					// alert( $(this).css("width") );
			if( $(this).css("width") == "720px" ){
				$(this).css("width", "100%");
				$(this).css("height", "");
			}
            if( imgTitle ) return "<div class='photo' style='float:"+float+"' data-title='"+$(this).attr('title')+"'>"+$(this).text()+"</div>";
        });
    });

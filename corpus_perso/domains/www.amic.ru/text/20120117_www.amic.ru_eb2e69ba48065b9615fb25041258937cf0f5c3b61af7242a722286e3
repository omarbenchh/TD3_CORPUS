   $('#titogi_2015_amic_ru').click( function() {
      ga('send', 'event', 'nav', 'click', 'button_itogi_2015_amic_ru');
  });
   $('#how_change').click( function() {
      ga('send', 'event', 'nav', 'click', 'button_how_change');
  });
});
  
</script>

<script type="text/javascript" >
$(document).ready(function(){
	$(".account").click(function(){
		var X=$(this).attr('id');
		if(X==1){
			$(".submenu").hide("fade");
			$(this).attr('id', '0');	
		}else{
			$(".submenu").show("fade");
			$(this).attr('id', '1');
			$("#pan").hide("blind");
		}
		
	});
	$(".submenu").mouseup(function(){
		return false
	});
	$(".account").mouseup(function(){
		return false
	});
	$(document).mouseup(function(){
		$(".submenu").hide("fade");
		$(".account").attr('id', '');
		$("#pan").hide("blind");
	});
});
</script>


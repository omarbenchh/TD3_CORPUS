	var fFnG = parseInt('3861') ? parseInt('3861') : null;
}

find_region:
for(var region in accounts){
	for(var i=0; i<accounts[region].length; i++){
		if(accounts[region][i][1] == fFnG){
			break find_region;
		}
	}
}
var checkbox_html='';
for(var i=0; i<accounts[region].length; i++){
	if(accounts[region][i][1] != fFnG) checkbox_html += '<label><span><input type="checkbox" onchange="updateFfng();" value="'+accounts[region][i][1]+'"> '+accounts[region][i][0]+'</span></label> ';
}
	






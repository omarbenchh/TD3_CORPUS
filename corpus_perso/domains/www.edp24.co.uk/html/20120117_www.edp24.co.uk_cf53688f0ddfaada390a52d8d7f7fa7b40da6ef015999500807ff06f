                        <script type="text/javascript">
function MultiTabs(id, opts){
	this.id = id;
	this.el = document.getElementById(id);
	this.splitTabs = false;
	
	opts = typeof opts == 'object' ? opts : {};
	
	var that = this;
	if(window.addEventListener){
		window.addEventListener('resize', function(){ 
			that.checkIfSplit(); 
			that.setSplitClass(); 
		});
	} else { // IE8
		window.attachEvent('onresize', function(){ 
			that.checkIfSplit(); 
			that.setSplitClass(); 
		});
	}
	
	// Add a .tab-groups div around the tab sets
	// 1. Create the div
	var d = document.createElement('div');
	d.setAttribute('class', 'tab-groups');
	jQuery(this.el).prepend(d);
	
	// 2. Add the tab sets
	var tabSets = document.querySelectorAll('#'+this.id + ' > ul');
	for(var i=0, len = tabSets.length; i<len; i++){
		jQuery(d).append(tabSets[i]);
	}
	
	jQuery(this.el).addClass('tab-wrap multiline-tabs');
	
	// Add click handlers to tabs
	var as = document.querySelectorAll('#'+this.id+' > .tab-groups > ul > li');
	for(var i=0, len = as.length; i<len; i++){
		if(as[i].addEventListener){
			as[i].addEventListener('click', function(e){
				//e.preventDefault();
				that.selectTab(this);
			});
		} else { // IE8
			as[i].attachEvent('onclick', function(e){
				//e.returnValue = false;
				that.selectTab(e.srcElement);
			});
		}
	}
	
	if(typeof opts.defaultTab == 'string'){
		this.selectTab(opts.defaultTab);
	} else { // otherwise, just get the first tab
		this.selectTab(document.querySelector('#'+this.id + ' .tab-groups > ul > li'));
	}
  
  	// Loop through the tabs and record their original positions
  	this.positions = [];
  	var uls = document.querySelectorAll('#'+this.id+' > .tab-groups > ul');
 	for(var i=0, len = uls.length; i<len; i++){
    	this.positions[i] = [];
    	var lis = uls[i].querySelectorAll('li');
    	for(var j=0, len2 = lis.length; j<len2; j++){
      		this.positions[i][j] = lis[j];
    	}
  	}
	
	this.checkIfSplit();
	this.setSplitClass();
}
function trim(s){
	return s.replace(/^\s+|\s+$/g, '');
}

MultiTabs.prototype.checkIfSplit = function(){
	this.splitTabs = false;
	var lastValue;
	// go through each LI to see if they're on the same line
	var lis = document.querySelectorAll('#'+this.id+' .tab-groups ul > li');
	for(var i=0, len = lis.length; i<len; i++){
		var offsetTop = lis[i].offsetTop; 
		// if offsetTop is different on any of the tabs, there must be at least two rows
		if(lastValue != null && lastValue != offsetTop){
      		
			this.splitTabs = true;
			
			// If the tabs are split and the .current tab is in the wrong ul (such as after a resize event)...
			var currentTab = document.querySelector('#'+this.id+' ul > li.current');
			if(this.splitTabs && jQuery(currentTab.parentNode).next().length != 0){
				// ... call selectTab to move it into the correct tab set
				this.selectTab(currentTab);
			}
			this.setSplitClass();
			
			return false;
		}
		lastValue = offsetTop;	
	}
  
  	// If they're not split, put the tabs back in their original order
 	var uls = document.querySelectorAll('#'+this.id+' > .tab-groups > ul');
 	for(var i=0, len = uls.length; i<len; i++){
    	for(var j=0, len2 = this.positions[i].length; j<len2; j++){
      		jQuery(uls[i]).append(this.positions[i][j]);
    	}
  	}
}
MultiTabs.prototype.setSplitClass = function(){
	if(this.splitTabs) jQuery(this.el).addClass('split');
		else jQuery(this.el).removeClass('split');
}
MultiTabs.prototype.selectTab = function(tab){
  	if(tab.querySelector('a') !== null) return;
	if(typeof tab == 'string') tab = document.querySelector('#'+this.id+' > .tab-groups > ul > li[data-tab-id="'+tab+'"]');
	// If tabs are split and the .tab-set clicked was not in the last row...
	if(this.splitTabs && jQuery(tab.parentNode).next().length != 0){
		// ...swap it with one that is (slightly bloated because IE8 doesn't support last-child CSS)
		var tabSets = document.querySelectorAll('#'+this.id+' .tab-groups > ul');
		var lastTabSet = tabSets[tabSets.length-1];
		var current = lastTabSet.children[0];
		var removedFromSet = tab.parentNode;
		jQuery(removedFromSet).append(current);
		jQuery(lastTabSet).prepend(tab);
	}
	var lis = document.querySelectorAll('#'+this.id+' > .tab-groups > ul > li');
	for(var i=0, len=lis.length; i<len; i++){
		jQuery(lis[i]).removeClass('current');
	}
	jQuery(tab).addClass('current');
	
	// Hide all tab contents
	var tabs = this.el.querySelectorAll('div[data-tab-id]');
	for(var i=0, len=tabs.length; i<len; i++){
		jQuery(tabs[i]).removeClass('current');
	}
	
	// Show the clicked tab's contents
	var tabContentElement = this.el.querySelector('div[data-tab-id="'+tab.getAttribute('data-tab-id')+'"]');
	jQuery(tabContentElement).addClass('current');
	
}
MultiTabs.prototype.isSplit = function(container){
	this.splitTabs = false;
	var lastValue;
	var lis = document.querySelectorAll('#'+this.id+' > .tab-groups > ul > li');
	for(var i=0, len=lis.length; i<len; i++){
		var offsetTop = lis[i].offsetTop; 
		// if offset.top() is different on any of the tabs, there must be at least two rows
		if(lastValue != null && lastValue != offsetTop){
			this.splitTabs = true;
			return false;
		}
		lastValue = offsetTop;	
	}
	return this.splitTabs;
}
MultiTabs.prototype.clickTab = function(){
  	if(this.el.querySelector('a') === null) this.selectTab(this.el);
}

</script>


      
      
      
    
    
        
        

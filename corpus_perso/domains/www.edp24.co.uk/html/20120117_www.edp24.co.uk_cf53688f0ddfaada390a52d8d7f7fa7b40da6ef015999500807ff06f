if(typeof window.shortened_url == 'undefined'){
    function vanity_url(url){
      url = url.split('#')[0].split('?')[0];
      
      var domain_rx = /www.([0-9a-z.]+)\//i;
      var id_rx = /_1_([0-9]+)$/i;
      
      var domain_match = url.match(domain_rx);
      var id_match = url.match(id_rx);
      if(domain_match != null && id_match != null){
        return "http://" + domain_match[1] + "/1." + id_match[1];
      } else return url;
    }
    var shortened_url = vanity_url(window.location.href);
}
</script>


	<script type="text/javascript">var switchTo5x=true;</script>
	<script type="text/javascript" src="http://w.sharethis.com/button/buttons.js"></script>
	<script type="text/javascript">
	stLight.options({publisher: "9cb30723-287f-4d75-88d4-9f2c80fedd1e", doNotHash: true, doNotCopy: false, hashAddressBar: false, shorten:false, onhover: false, tracking: false});
	
	function selectText(text) {
		var doc = document, range, selection;
		if (doc.body.createTextRange) {
			range = document.body.createTextRange();
			range.moveToElementText(text);
			range.select();
		} else if (window.getSelection) {
			selection = window.getSelection();        
			range = document.createRange();
			range.selectNodeContents(text);
			selection.removeAllRanges();
			selection.addRange(range);
		}
	}
	function showShareLink(el){
  		if(typeof ga != 'undefined') ga('send', 'event', 'Social share buttons', 'click', 'Get link');
		var sharePanel = jQuery(el).parent();
 		el = jQuery(el).siblings('#share-url')[0];
		if(sharePanel.hasClass('clicked')){
			sharePanel.removeClass('clicked');
		} else {
			sharePanel.addClass('clicked');
			setTimeout(function(){ selectText(el); }, 300);
		}
	}
	
	jQuery(document).on('ready', function(){
		if(jQuery('.utitlity-belt').eq(1).offset().top - jQuery('.utitlity-belt').eq(0).offset().top < 500) jQuery('.article > .utitlity-belt').hide();
	});
	
	</script>
      
      
      
    
    
        


<script type="text/javascript">
    function addBanner(blockId, bannerAddr, width, height, link, target) {
        var target = target || '_blank';
        if (bannerAddr == '')
            return;
        if (!document.getElementById(blockId))
            return;
//        var link = link || false;
        var banTypeArr = bannerAddr.split('.');
        var banType = banTypeArr[banTypeArr.length - 1];
        if (banType === 'swf') {
            var block = document.getElementById(blockId);
            var iframe = document.createElement('iframe');
            var html4_banner_array = banTypeArr;
            html4_banner_array[html4_banner_array.length-1] = 'html';
            var html4_banner = html4_banner_array.join('.');
            iframe.setAttribute('src', 'http://www.zabinfo.ru/graph/' + html4_banner);
            iframe.setAttribute('width', width);
            iframe.setAttribute('height', height);
            block.appendChild(iframe);

            swfobject.embedSWF("http://www.zabinfo.ru/graph/" + bannerAddr, blockId, width, height, "9.0.0", "", {menu: "false", quality: "high", wmode: "opaque"}, {id: blockId, name: blockId, wmode: "opaque"});
            if (link !== '') {
                var a = document.createElement('a');
                a.setAttribute('href', link);
                a.setAttribute('style', 'display:block;position:absolute;left:0;top:0;z-index:5;width:' + width + 'px;height:' + height + 'px;background:url(/themes/news_01/images/pix.gif);');
                a.setAttribute('target', target);
                var block = document.getElementById(blockId).parentElement;
                block.appendChild(a);
                block.setAttribute('style', 'position:relative');
                block.style.width=width + 'px';
                block.style.height=height + 'px';
            }
        }
        else if (banType === 'gif' || banType === 'jpg' || banType === 'png') {
            var block = document.getElementById(blockId);
            var img = document.createElement('img');
            img.setAttribute('src', 'http://www.zabinfo.ru/graph/' + bannerAddr);
            img.setAttribute('alt', '');
            img.setAttribute('width', width);
            img.setAttribute('height', height);
            if (!link) {
                block.appendChild(img);
                return;
            }
            var a = document.createElement('a');
            a.setAttribute('href', link);
            a.setAttribute('target', target);
            a.appendChild(img);
            block.appendChild(a);
        }
    }


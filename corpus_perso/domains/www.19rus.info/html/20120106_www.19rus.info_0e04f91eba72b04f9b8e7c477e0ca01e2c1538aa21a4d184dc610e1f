<script charset="utf-8">
(function() {
    // балансировщик высоты центральной и левой колонок
    var $materialsWrap = $('.article-footer-materials'),
        $materials = $materialsWrap.find('li'),
        totalBlocks = $materials.length;

    $(window).on('scroll', function () {
        if($(this).scrollTop() > $('.article-footer-media').offset().top){
            // remove scroll listener
            $(window).unbind('scroll');

            // balance height logic
            var leftColHeight = $('.dinamic_scroll').height(),
                centerColHeight = $('#article').height(),
                rightColHeight = $('.right').height(),
                maxColHeight = Math.max(leftColHeight, rightColHeight),
                maxHeight = Math.max(maxColHeight, centerColHeight),
                delta = maxHeight - centerColHeight,
                middleBlockHeight = 250,
                hideBlocksCount = 0;

            if(delta == 0) {
                // cut couple of rows
                hideBlocksCount = 3 * Math.round((centerColHeight - maxColHeight)/middleBlockHeight);
                if(hideBlocksCount >= totalBlocks) {
                    $materialsWrap.hide();
                    $materialsWrap.next('hr').hide();
                }
                else {
                    $materials.slice(-hideBlocksCount).hide();
                }
            }
            else if(delta > 0) {
                if(leftColHeight >= rightColHeight) {
                    $('.left_more li')
                        .slice(
                            -Math.round((leftColHeight - centerColHeight)/middleBlockHeight)
                        )
                        .hide();
                }
            }
        }
    });
})();
</script>

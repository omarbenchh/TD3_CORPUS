				$(function(){
					var navBarItemClass = 'navbar-item';

					$('.navbarxs-toggle').click(function(){
					    $(this).toggleClass('active');
					    $('.navbarxs').slideToggle();
                        yaCounter1598573.reachGoal('RESPONSIVE_MENU');
					});

					$('.' + navBarItemClass).click(function(){
					    if (!$(this).hasClass(navBarItemClass)){
					        location.href = $(this).attr('href');
					        return false;
					    }

					    $('.navbarxs-submenu').slideUp(function(){
					        $(this).prev().find('a').addClass(navBarItemClass);
					    });

					    $(this).parent().next().slideDown();
					    $(this).removeClass(navBarItemClass);
					    return false;
					});

                    var $regions = $('.regionsxs'),
                        $select = $('.regionsxs-select'),
                        cookieName = 'region-selected',
                        nowRegion = 0,
                        cookieRegion = $.cookie(cookieName),
                        isFederal = true;

					$('.navbar-regions').click(function() {
					    api.shadow.show();
                        $regions.show();
                        return false;
					});

                    $('.regionsxs-submit').click(function() {
                        var $option = $select.find('option:selected'),
                            newRegion = $option.data('id'),
                            domain = $option.val().split('/');
                        // $.cookie(
                        //     cookieName,
                        //     newRegion,
                        //     {expires: 365, path: '/', domain: '.' + window.location.host}
                        // );
                        if(newRegion != cookieRegion) {
                            window.location.href = $option.val() + '?set_region=1';
                        }
                        api.shadow.hide();
                        $regions.hide();
                    });

                    $('.regionsxs-no').click(function() {
                        api.shadow.hide();
                        $regions.hide();
                        return false;
                    });

                    if($('.navbarxs-toggle').css('display') == 'block') {
                        if(!isFederal && !cookieRegion) {
                            api.shadow.show();
                            $regions.show();
                        }
                    }
				});
			</script>


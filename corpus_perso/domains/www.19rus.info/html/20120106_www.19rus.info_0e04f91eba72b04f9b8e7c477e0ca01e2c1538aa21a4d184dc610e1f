                    <script>
                        $(function(){
                            $.getScript("https://relap.io/api/v2/similar_pages_jsonp.js?url="+encodeURIComponent(location.href)+"&callback=renderRelap&with_description=1&algorithms=%5B%7B%22id%22%3A74%2C%22limit%22%3A1%7D%5D");
                        });

                        function renderRelap(recs){
                            $('.left_block.news.news_wide').next('hr').after('<div class="left_block relap-list"><div class="left_more"><ul></ul></div></div>');
                            $.each(recs.recs, function(k, v) {
                                var li = $('<li></li>');
                                var html =  '<div class="img_container left_more_img">' +
                                            '<a href="'+ v.url +'"><img alt="'+ v.title +'" src="'+ v.thumb +'"></a>' +
                                            '</div>' +
                                            '<p><a href="'+ v.url +'">'+ v.title +'</a></p>';
                                li.append(html);
                                if (v.is_ad) {
                                    li.find('a').attr('target', '_blank');
                                    li.append('<div style="margin-top:7px; font-size:12px; color:#b1b1b1;">Реклама</div>');
                                }
                                $('.relap-list ul').append(li);
                            });
                            var img = new Image(); 
                            img.src = 'https://relap.io/api/v2/pixel.gif?rgid='+recs.rec_group_id
                        }
                    </script>


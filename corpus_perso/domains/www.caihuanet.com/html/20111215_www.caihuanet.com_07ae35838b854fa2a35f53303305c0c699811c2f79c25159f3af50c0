<SCRIPT type=text/javascript>
var _speed=30;
var _slide = $("#slide");
var _slideli1 = $(".slideli1");
var _slideli2 = $(".slideli2");
_slideli2.html(_slideli1.html());
function Marquee(){
	if(_slide.scrollLeft() >= _slideli1.width())
		_slide.scrollLeft(0);
	else{
		_slide.scrollLeft(_slide.scrollLeft()+1);
	}
}

var sliding=setInterval(Marquee,_speed)
_slide.hover(function() {
	clearInterval(sliding);
},function(){
	sliding=setInterval(Marquee,_speed);
});


var _aslide = $("#aslide");
var _aslideli1 = $(".aslideli1");
var _aslideli2 = $(".aslideli2");
_aslideli2.html(_aslideli1.html());
function aMarquee(){
	if(_aslide.scrollLeft() >= _aslideli1.width())
		_aslide.scrollLeft(0);
	else{
		_aslide.scrollLeft(_aslide.scrollLeft()+1);
	}
}
var asliding=setInterval(aMarquee,_speed)
_aslide.hover(function() {
	clearInterval(asliding);
},function(){
	asliding=setInterval(aMarquee,_speed);
});

var _bslide = $("#bslide");
var _bslideli1 = $(".bslideli1");
var _bslideli2 = $(".bslideli2");
_bslideli2.html(_bslideli1.html());
function bMarquee(){
	if(_bslide.scrollLeft() >= _bslideli1.width())
		_bslide.scrollLeft(0);
	else{
		_bslide.scrollLeft(_bslide.scrollLeft()+1);
	}
}
var bsliding=setInterval(bMarquee,_speed)
_bslide.hover(function() {
	clearInterval(bsliding);
},function(){
	bsliding=setInterval(bMarquee,_speed);
});

</SCRIPT>






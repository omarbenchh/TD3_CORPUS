<script>
    $("#searching").click(function () {
        $(".weather").hide();
        $("#searching").hide();
        $(".searcharea").show();

        $("#searchingon").show();
    });

    $("#searchingon").click(function () {

        $(".searcharea").hide();
        $("#searchingon").hide();
        $(".weather").show();
        $("#searching").show();
    });


    $("input[id=searchinput]").keypress(function (e) {
        var code = (e.keyCode ? e.keyCode : e.which);
        if (code == 13 || code == 10) {
            searchInput();
            return false;
        }
        return true;
    });



    function searchInput() {
        window.location.href = "/Search/?area=0&author=&cid=-1&so=1&sa=0&pt=3&words=" + decodeURI($("#searchinput").val());
    }



</script>

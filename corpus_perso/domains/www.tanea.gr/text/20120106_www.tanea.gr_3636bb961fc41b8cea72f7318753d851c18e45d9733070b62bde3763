<script type="text/javascript">

    $(document).ready(function () {
        $("select[name='moreUrlNavigateSelect']").change(function () {
            $("select option:selected").each(function () {
                if ($(this).attr('value') != 'null') {
                    window.open($(this).attr('value'), "newwindow");

                }
            });
        });


        ///Search/?area=0&author=&cid=-1&so=1&sa=0&pt=5&words=

        $("input[id=searchInputFooter]").keypress(function (e) {
            var code = (e.keyCode ? e.keyCode : e.which);
            if (code == 13 || code == 10) {
                searchFromFooter();
                return false;
            }
            return true;
        });

        $("img[id=searchButtonImage]").click(function () {
            searchFromFooter();
        });

    });

    function searchFromFooter() {
        var words = "";
        $("input[id=searchInputFooter]").each(function () {
            words += $(this).val();
        });
        window.location.href = "/Search/?area=0&author=&cid=-1&so=1&sa=0&pt=3&words=" + decodeURI(words);
    }

</script>

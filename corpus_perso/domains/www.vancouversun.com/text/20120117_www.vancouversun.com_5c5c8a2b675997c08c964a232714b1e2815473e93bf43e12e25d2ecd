				function ParseDartAdUrl(){
				if (typeof dartad_url != 'undefined') {
					var dd = adSync.ParseDartAdUrl(dartad_url);
					cwZoneAd=dd["zone"];
					cwSiteAd =dd["site"];
					cwKeyAd=dd["keyValues"];
				}else {
					cwSiteAd = "vs_bh.com"; // plug in the Ad site variable
					cwZoneAd= "health/story"; // plug in the ad zone.
					cwKeyAd="kw=ron;kw=health"; // plug in the Ad key words.
				}	
				}
			</script>
		
			<script type="text/javascript">
	
				var addthis_pub = 'canada.com';

				function tabClick(eventText, hasVideo, hasStn, id, classname, pause, syncAds)
				{	
					//the statsEvent and Ad Refresh have been disabled on the story tab clicks
					//I've left the code in the xsl (just commented out), in case the decision gets reversed.
									
					//omniture event
					//statsEvent(eventText);	
					
					// Show/hide SendtoNews player
					if(hasStn)
					{
						if (classname == 'story_content') {
							jQuery('#stnFrame').attr('height', '349');
						}
						else {
							jQuery('#stnFrame').attr('height', '0');
						}
					}

					//set tab
					if(hasVideo) {
						ooVideo.switchTab(id,classname);
					}

					setClass('storypage', classname);

					//refresh ads
					
						
							
							//if(syncAds)
								//Postmedia.adConfig.refreshAds(true);
							
						
				}

				
				function pageClick(eventText, pg)
				{			
	        var prevpage,
            pageList = $j('ul.pagebreak li'),
            pageListArr = $j( 'ul.pagebreak li a' ),
        lastPos = pageList.length,
		scrollToPage = '',
		viewAll = false;

            if( $j( $j(pageList[pg]).children()[0] ).attr('class') == '' ){
		//goes through list of pages and find previous active page
              pageListArr.each(function(e){
	           if($j(this)[0].className == 'active'){
	               prevpage = e;
	           }
	        });
        	
		if( pg == lastPos - 2){
			if (prevpage == (lastPos -3)){
				scrollToPage = lastPos - 3;
			} else {
				scrollToPage = prevpage + 1;
			}
			pg = pg + 1;	
		}else if(pg == 0){
	           if(prevpage == 1) { 
			        return false; 
		        } else { 
			        pg = prevpage 
		        };
	        } else if(pg == (lastPos-1) ) {
			if(prevpage == (lastPos-3)) {
			        return false; 
                } else {
			        pg = (prevpage + 2);
		        }
	        } else if(pg == prevpage) {
		        return false; 
	        } else {
	           pg = pg + 1;
	        };
        	
              pageListArr.removeClass('active inactive');
		
              $j( $j(pageList[pg-1]).children()[0] ).addClass('active');
	        if(pg == 2){
                 $j( pageListArr[pg-2] ).addClass('inactive');
		}
		else if( pg == lastPos-2){
			$j( pageListArr[pg+1] ).addClass('inactive');
            };
        	
	        setClass('storyContent', 'page' + (pg - 1) + ' para18');
            
		if (pg == lastPos - 1){
			$j('#storyContent').removeClass('page' + (pg - 1));
			$j(pageList).hide();
			viewAll = true;
		}else {
			viewAll = false;
		};
    document.body.scrollTop = document.documentElement.scrollTop = 0;
            //refresh ads
					
					
                
								Postmedia.adConfig.refreshAds(true);
							
              
					
					//omniture event
           scStoryPageBreak((lastPos - 2),(pg - 1),viewAll);
          }
					
				}
				
				function statsEvent(eventText)
				{
					//omniture event
					s.prop38=s.pageName + eventText;
					s.events='';
					void(s.t());				
				}

				function showTab(firstentrytype)
				{
					var tab = getQueryVar('tab');
					var tabclass = '';

					if (tab == 'PHOT' || (firstentrytype.indexOf('image/') != -1 && tab == ''))
						tabclass = 'story_photo_content';
					else if (tab == 'VID' || ((firstentrytype == 'application/vnd.ooyala' || firstentrytype == 'application/vnd.kaltura') && tab == ''))
						tabclass = 'story_video_content';
					else if (tab == 'FLSH' || (firstentrytype == 'application/x-shockwave-flash' && tab == ''))
						tabclass = 'story_interactive_content';

					if (tabclass != '')
						setClass('storypage', tabclass);
				}
				function EmailThis(qs, emailViewer) 
				{
					var url = "http://" + window.location.hostname + window.location.pathname;
					var id = "";

					id = getQueryVar("id");
					if (id != "")
					{
						url = addQS(url, "id=" + id);
					}
					if (qs != "")
					{
						url = addQS(url, qs);
					}
					if (typeof(emailViewer) == "undefined" || emailViewer == null || emailViewer == "") emailViewer = "/story_emailthis.html";
					openWindow(emailViewer + '?referrer=' + encodeURIComponent(url), 'StoryEmailThis', 'scrollbars=yes,resizable=yes,width=550,height=380')
				}
				function getQueryVar(key) 
				{
					var qs = window.location.search.substring(1);
					var arrQS = qs.split("&");
					var value = "";

					for (i=0; i<arrQS.length; i++) {
						var ft = arrQS[i].split("=");
						if (ft[0] == key) {
							value =  ft[1];
						}
					}

					return value;
				}
				function addQS(theURL, QS)
				{
					var url = theURL;
					if (QS != '')
					{
						if (url.indexOf("?") == -1)	{
							url += "?" + QS;
						} 
						else {
							url += "%26amp;" + QS;
						}
					}
					return url;
				}
				function openWindow(theURL, WinName, WinFeatures) {
					window.open(theURL, WinName, WinFeatures);
				}
				function jumpToAnchor(anchor) 
				{
					window.location = String(window.location).replace(/\#.*$/, "") + anchor;
				}
				function showAd(story_elem, ad_elem, ad_wrapper, offset) {
					var eosPos = Y(story_elem);
					var adPos = Y(ad_wrapper);
					var showAd = false;
					var offsetPos = 0;
					
					if (eosPos >= adPos)
					{
						showAd = true;
					}
					else
					{
						offsetPos = eosPos + offset;
						if (offsetPos >= adPos)
						{
							showAd = true;
						}
					}

					// write out ad include
					if (showAd == true)
					{
					   document.getElementById(ad_elem).style.display = "block"; 
					}
				}				
				function X(id) {
					var obj = document.getElementById(id);
					var x = obj.offsetLeft

					while (obj = obj.offsetParent) x += obj.offsetLeft
					return x
				}
				function Y(id) {
					var obj = document.getElementById(id);
					var y = obj.offsetTop

					while (obj = obj.offsetParent) y += obj.offsetTop
					return y
				}

			</script>

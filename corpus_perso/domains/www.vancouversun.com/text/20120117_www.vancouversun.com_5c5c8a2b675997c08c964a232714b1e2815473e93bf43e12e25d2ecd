		var contentid = "5999273";
		var iabterms = new Array();
		var informterms = new Array();

				
				informterms.push("Emma Godmere"); 				
					
				informterms.push("Contagious and Infectious Diseases"); 				
					
				informterms.push("Health and Fitness"); 				
					
				informterms.push("Noroviruses"); 				
					
				informterms.push("Canadian University Press"); 				
					
				informterms.push("Vancouver Island Health Authority"); 				
					
				informterms.push("News"); 				
					
				informterms.push("Suzanne Germain"); 				
			

		var iablength = iabterms.length,
			inflength = informterms.length,
			iabstring = "",
			informstring = "";
		
		
		
		for(i=0; i<iablength; i++)
		{
			iabterms[i] = iabterms[i].replace(/[\&]/g,"and");
			iabterms[i] = iabterms[i].replace(/[^A-Za-z0-9\s]/g,"");
			iabstring += "ciab=" + iabterms[i] + ";";
		}

		for (j=0; j<inflength; j++)
		{
			informterms[j] = informterms[j].replace(/[\&]/g,"and");
			informterms[j] = informterms[j].replace(/[^A-Za-z0-9\s]/g,"");
			informstring += "cinf=" + informterms[j] + ";";
		}
		
		iabstring = iabstring.toLowerCase();
		iabstring = iabstring.replace(/\s/g,"-");
		informstring = informstring.toLowerCase();		
		informstring = informstring.replace(/\s/g,"-");		
		

	</script>


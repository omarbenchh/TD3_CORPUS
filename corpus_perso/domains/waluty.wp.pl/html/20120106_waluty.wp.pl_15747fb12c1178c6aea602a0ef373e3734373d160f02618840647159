					<!-- stampBxNaglowek [2.0] { -->
		<div class="stampBxNaglowek">
					<div class="stampTytul">
					<h1 class="stampTytulHeader">
			Złotówka traci przez sylwestra		</h1>
				<!-- stampZrodlo [2.0] { -->
		<div class="stampZrodlo stamp">
			<div class="cnt">

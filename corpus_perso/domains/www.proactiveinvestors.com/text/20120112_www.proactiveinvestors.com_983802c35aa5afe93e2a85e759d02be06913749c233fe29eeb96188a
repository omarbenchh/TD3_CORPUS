                        <script>
                            $(document).ready(function() {
                    var opts = {
                        lines: 13, // The number of lines to draw
                        length: 6, // The length of each line
                        width: 3, // The line thickness
                        radius: 11, // The radius of the inner circle
                        color: '#003A56', // #rbg or #rrggbb
                        speed: 1, // Rounds per second
                        trail: 100, // Afterglow percentage
                        shadow: false // Whether to render a shadow
                    };
                    var target = document.getElementById('spinner-gainers-losers');
                    var spinner = new Spinner(opts).spin(target);
                });

                $(window).load(function() {
                    var id = 'gainers-losers-lazy-element-holder';
                    $.ajax({
                        url: '/lazy/loadLazyElement/gainers-losers/0/0',
                        success: function(response) {
                            $("#spinner-gainers-losers").fadeOut('slow', function() {
                                $('#' + id).hide();
                                $('#' + id).html(response);
                                $('#' + id).children('.spinner').remove();
                                $('#' + id).removeClass('lazy-element-holder');
                                $('#' + id).addClass('lazy-element-holder-done');
                                $('#' + id).fadeIn('slow');
                            });

                        }

                    });
                });
            </script>

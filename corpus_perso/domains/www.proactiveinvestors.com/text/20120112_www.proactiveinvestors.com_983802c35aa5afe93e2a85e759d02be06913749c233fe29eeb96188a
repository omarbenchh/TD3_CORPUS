                        shadow: false // Whether to render a shadow
                    };
                    var target = document.getElementById('spinner-value-of-trades');
                    var spinner = new Spinner(opts).spin(target);
                });

                $(window).load(function() {
                    var id = 'value-of-trades-lazy-element-holder';
                    $.ajax({
                        url: '/lazy/loadLazyElement/value-of-trades/0/0',
                        success: function(response) {
                            $("#spinner-value-of-trades").fadeOut('slow', function() {
                                $('#' + id).hide();

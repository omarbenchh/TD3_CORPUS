<script>
var active_exists = '0';

if(active_exists == 1) {
	$('.secondary-navigation li:first').removeClass('active');
} else {
	$('.secondary-navigation li:first').addClass('active');
}

// remove active class when on channel - big picture (if not, multiple links will be set active)
var is_channel = '0';
var controller = 'home';
var action = 'index';
if(is_channel && controller=='archives' && action=='bigPicture') {
	$( ".primary-navigation li:contains('Big Picture')" ).removeClass('active');
}

</script>


<script>
$(document).ready(function() {
	$('#advanced-search-area').change(function(){
		$('.advanced-all-areas').hide();	
		var visible_area=$('#advanced-search-area').val();
		$('#advanced-search-'+visible_area).show();
		$('.advanced-all-areas:hidden').val('');
	});

	//keyword autocomplete
	$("#advanced-search-keyword").autocomplete({
		delay: 100,
		source: function( request, response ) {
		$('#advanced-search-company_id').val('');
		$.ajax({
			url: "/swift/autocompleteGetCompanies",
			dataType: "json",
			data: {term: request.term},
			success: function(data) {
						response($.map(data, function(item) {
						return {
							label: item.name,
							id: item.id
							};
					}));
				}
			});
		},
		minLength: 2,
		select: function(event, ui) {
			$('#advanced-search-company_id').val(ui.item.id);
		}
	});         
});
</script>			<div class="container">

<script type="text/javascript">
	$(document).ready(function() {

		/* slider */
		var totalItems = $('#HomeCarousel .item').length;
		var currentIndex = $('div.active').index() + 1;

		$('#HomeCarousel .num').html(''+currentIndex+'/'+totalItems+'');
		$('#HomeCarousel').carousel({
		    interval: 8000
		});
		$('#HomeCarousel').on('slid.bs.carousel.bs.carousel', function (e) {
			if(e.direction=='left'){
				if(currentIndex==5){
		    		currentIndex=1;
		    	} else{
		    		currentIndex = currentIndex + 1;
		    	}
			}
			if(e.direction=='right'){
				if(currentIndex==1){
		    		currentIndex=5;
		    	} else{
		    		currentIndex = currentIndex - 1;
		    	}
			}
		   	$('#HomeCarousel .num').html(''+currentIndex+'/'+totalItems+'');
		});

		$('#LeftArrow').click(function() {
		  	$('#HomeCarousel').carousel('prev');
		});
		$('#RightArrow').click(function() {
		  	$('#HomeCarousel').carousel('next');
		});
		$('#PausePlay').click(function() {
		  	if ($(this).hasClass('pause')) {
		  		$(this).removeClass('pause');
		  		$('#HomeCarousel').carousel('cycle');
		  		$(this).html('<span class="fa-stack fa-lg"><i class="fa fa-circle fa-stack-2x"></i><i class="fa fa-pause fa-stack-1x fa-inverse"></i></span>');
		  	} else {
		  		$(this).addClass('pause');
		  		$('#HomeCarousel').carousel('pause');
		  		$(this).html('<span class="fa-stack fa-lg"><i class="fa fa-circle fa-stack-2x"></i><i class="fa fa-play fa-stack-1x fa-inverse"></i></span>');
		  	}
		}); 
	});
</script>		    			

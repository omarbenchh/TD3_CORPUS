<script type="text/javascript">
$(function()
{
    $("#user_profile").submit( function()
    {
        var f = this;
        var data = formCollect(this);
        $(".gate_regerror", f).empty();
        $.ajax({
            dataType: 'json',
            url: '/ajax_create_user',
            type: "post",
            data: data})
        .done( function(ldata)
        {
            if(ldata.islogged)
               gate.onLoggedUser();
            else if(ldata.error)
            {
                if(ldata.fielderrors)
                {
                    var fe = ldata.fielderrors;
                    $("input, select, textarea", f).each( function(idx, el)
                    {
                        if(fe[el.name])
                        {
                            var fp = el.parentNode;
                            var $ge = $(".gate_regerror", fp);
                            $ge.empty();
                            for(var idx in fe[el.name])
                            {
                                var p = document.createElement('p');
                                p.className = "errors";
                                $(p).text(fe[el.name][idx]);
                                $ge[0].appendChild(p);
                            }
                        }
                    });
                }
            }
        });
        return false;
    });
});
</script>

<div><label class="gateform_label" for="password_reminder_pin">Podaj jednorazowy kod:</label>
<input id="password_reminder_pin" name="pin" type="text" class="focused gateform_input input-spacer focus_glow" /><br />
<div class="gate_regerror center"></div>
</div>
<div><label class="gateform_label" for="password_reminder_password">Nowe hasło:</label>
<input id="password_reminder_password" name="password" type="password" class="focused gateform_input input-spacer focus_glow" /><br />
<div class="gate_regerror center"></div>
</div>
<div><label class="gateform_label" for="password_reminder_cpassword">Powtórz hasło:</label>
<input id="password_reminder_cpassword" name="cpassword" type="password" class="focused gateform_input input-spacer focus_glow" />
<div class="gate_regerror center"></div>
</div>
<div class="gate_regerror center"></div>
<input type="submit" value="Wyślij" class="button2 gateform_submit gateform_submit_button_spacer no_border no_outline gate_inline_button" />
    </form>
<script type="text/javascript">
$(function()
{
    $("#password_reminder_form").submit( function()
    {
        var f = this;
        var data = formCollect(f);
        $.ajax({
            dataType: 'json',
            url: '/ajax_passwordreminder',
            type: "post",
            data: data})
        .done( function(ldata)
        {
            $("p.errors", f).detach();
            if(ldata.status == "OK")
            {
                $(f).slideUp();
                $("#password_pin_form").slideDown();
            }
            else if(ldata.fielderrors) 
            {
                var fe = ldata.fielderrors;  
                for (var idx in fe)
                {
                $("div.gate_one_field_error", f).prepend('<p class="errors">'+fe[idx]+'</p>');
                }
            }

        });
        return false;
    });
    $("#password_pin_form").submit( function()
    {
        var f = this;
        var data = formCollect(f);
        $.ajax({
            dataType: 'json',
            url: '/ajax_passwordpin',
            type: "post",
            data: data})
        .done( function(ldata)
        {
            $("p.errors", f).detach();
            if(ldata.islogged)
                gate.onLoggedUser();
            else if(ldata.fielderrors) 
            {
                        var fe = ldata.fielderrors;
                        $("input, select, textarea", f).each( function(idx, el)
                        {
                            if(fe[el.name])
                            {
                                var fp = el.parentNode;
                                var $ge = $(".gate_regerror", fp);
                                $ge.empty();
                                for(var idx in fe[el.name])
                                {
                                    var p = document.createElement('p');
                                    p.className = "errors";
                                    $(p).text(fe[el.name][idx]);
                                    $ge[0].appendChild(p);
                                }
                            }
                        });
             }
        });
        return false;
    });
});
</script>
</div>
</div>
<script type="text/javascript">
gate.update();
var links = document.querySelectorAll('.gate_section_link');
for(var i = 0; i < links.length; i++) 
    {
        var el = links[i];
        el.onclick = gate.TabSwitcher;
    }
</script>





var result = $("#top_bar").height();
var result1 = $("#box_ad1").height() + $("#box_ad2").height();
if (result > 0) {
$('#spacer').height(function (index, height) {
    return (height + result + 5);
    });
}
if (result1 > 294) {
    document.getElementById('latest_content_last').style.display = "none";
}
var result2 = $("#helpful_list").height();
if ($(".gadget_list").css('display') != 'none') {
$('#spacer3').height(function (index, height) {
    return (1657 - result1 - result2);
    });
}
</script>

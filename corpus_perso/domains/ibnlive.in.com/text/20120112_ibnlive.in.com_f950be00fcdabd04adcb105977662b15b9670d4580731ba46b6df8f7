var pconteners=$(".cats");
var plists=pconteners.find(".plisting_boxs").width();
var tlistings=pconteners.find(".plisting_boxs").length;
pconteners.css("width", tlistings*plists);
//alert(tlisting);
$("a.pbluts").on("click", function(v){
v.preventDefault();
var lcks=$(this).data("type");
$(this).addClass("act_blut").removeClass("dact_blut");
$(this).siblings().removeClass("act_blut").addClass("dact_blut");
pconteners.animate({
marginLeft:"-"+plists*lcks,
})

})

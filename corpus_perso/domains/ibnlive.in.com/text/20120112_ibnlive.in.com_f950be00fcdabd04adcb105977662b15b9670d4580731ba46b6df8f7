<script>
 
 $(document).ready(function(){
var ln_pos = $('#contener').position().top;
var width = $(window).width();
var cheight= $('#contener_box').height();

$('#left_nav ul').css("height", cheight + "px");

$(window).scroll(function() {
               var wintop = $(window).scrollTop();
               if (wintop >= ln_pos)
               {
                   $('#left_nav').addClass('ln_pfixed');
                   $('#contener_box').css('margin-left','140px');
                                                               
                     if (width<=1024){
                                    $('#contener_box').css('margin-left','0px');
                                    }
               }
               else
               {
                               $('#left_nav').removeClass('ln_pfixed');
                               $('#contener_box').css('margin-left','0px');
               }
               
});
if (width<=1024){
                $('.home_pcake').on('click', function(e){
                   e.preventDefault();
                $('#left_nav').slideDown();
                $('#contener_box').css('margin-left','0px');
                $('#left_nav.ln_pfixed').css('top', '0px');
          });

              $('aside#left_nav h2.ttu a').on('click', function(e){
                          e.preventDefault();
                          $('#left_nav').slideUp();
                          $('#left_nav.ln_pfixed').css('margin-top', '0px')
                          $('.home_pcake').show();
                                                            
               });
                   
                                            
 }


});
</script>

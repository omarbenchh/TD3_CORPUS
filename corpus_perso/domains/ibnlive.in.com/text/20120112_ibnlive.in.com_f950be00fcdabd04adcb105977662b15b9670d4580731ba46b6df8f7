<script>
var vcontener=$("#top_video .tv_innerbox ul")
var vcount=0;
var vli_width=vcontener.find("li").width();
//alert(vli_width);
var vtlwp=vli_width+50;
var vtotel_li=4; /* total li contener width*/
$("a.vclck").click(function(e){
e.preventDefault();
var dir2=$(this).data("dir")
if(dir2=="vright"){
vcount=vcount+1;
}
else{
vcount=vcount-1;
}
if(vcount <0 || vcount>=vtotel_li){
vcount=0;
}
//alert(count);
vcontener.animate({marginLeft:'-'+vtlwp*vcount+'px'},500);
})
</script>

<script>function onchangebox(text){
var fileurl;
$("#news18box").html("<img style='margin-left:90%;' src='http://static.ibnlive.in.com/ibnlive/pix/ibnhome/31.gif'>");
   if(text=="latest"){
        fileurl="/newsredis.php?t=latest";
            }else{
        fileurl="/newsredis.php?t="+text;   
      }
	  if(text=="latest"){
			var stname="";
			}else{
				var res = text.replace("-", " "); 
			var stname=res.toUpperCase(); 
			
			}
            $("#stname").html(stname);
    $.get(fileurl, function(data)
    { 							
          	
            $("#news18box").html(data).hide().fadeIn();
		
         


    }); 

}

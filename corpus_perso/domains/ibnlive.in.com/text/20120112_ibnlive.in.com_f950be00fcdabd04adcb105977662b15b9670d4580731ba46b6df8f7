        </section><script>
var pcontener4=$(".cat4");
var plist4=pcontener4.find(".plisting_box4").width();
var tlisting4=pcontener4.find(".plisting_box4").length;
pcontener4.css("width", tlisting4*plist4);
//alert(tlisting);
$("a.pblut4").on("click", function(v){
v.preventDefault();
var lck4=$(this).data("type");
$(this).addClass("act_blut").removeClass("dact_blut");
$(this).siblings().removeClass("act_blut").addClass("dact_blut");
pcontener4.animate({
marginLeft:"-"+plist4*lck4,
})

})
</script>


<script>
var dr_contener6=$(".rm_contenerf")
var dr_count6=0;
var dr_li_width6=276;
var dr_totel_li6=dr_contener6.find("li").length; /* total li contener width*/

$("a.dr_clck6").click(function(e){
e.preventDefault();
var dir6=$(this).data("dir")
var drblt6=$(this).data("page");
if(dir6=="dr_right"){
dr_count6=dr_count6+1;

}
else{
dr_count6=dr_count6-1;
}

if(dr_count6 <0 || dr_count6>=dr_totel_li6){
dr_count6=0;
}

$(".dr6").removeClass("mract_blut").addClass("mrdact_blut");
$("a[data-page="+dr_count6+"]").addClass("mract_blut").removeClass("mrdact_blut");

dr_contener6.animate({marginLeft:"-"+dr_li_width6*dr_count6+"px"},500);
})
$(".dr6").on("click", function(e){
e.preventDefault();
var drblt6=$(this).data("page");
dr_count6=drblt6;
$(this).addClass("mract_blut").removeClass("mrdact_blut");
$(this).siblings().removeClass("mract_blut").addClass("mrdact_blut");

	dr_contener6.animate({marginLeft:"-"+dr_li_width6*drblt6+"px"},500);
	})
</script>


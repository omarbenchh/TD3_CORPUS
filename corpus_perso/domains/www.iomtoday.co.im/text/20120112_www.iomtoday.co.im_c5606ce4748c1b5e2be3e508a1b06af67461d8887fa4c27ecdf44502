<script type="text/javascript">
	var currentArticleUrl = window.location.protocol + "//" + window.location.hostname + window.location.pathname;
	
	var noOfStories = 5;
	var addedStories = 0;
	$("#topStories1 li").each(function(index, liElement) {
		var articleUrl = $(liElement).find("a").attr("href");
		
		if (articleUrl !== currentArticleUrl) {
			$(liElement).show();
			addedStories++;
		}
		
		if (addedStories == noOfStories) {
			return false;
		}
	});
</script>

          
        


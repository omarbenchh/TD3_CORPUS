<script type="text/javascript">
	window._taboola = window._taboola || [];
			var isResponsiveSite = false;
		
	var pushTaboolaObject = function(){
		var taboolaObject = {
			mode: "thumbnails-a-escalator",
			placement: "Mobile Escalator Thumbnails",
			target_type: "mix"
		};
				
					taboolaObject.slider = true;
				_taboola.push(taboolaObject);
	}
	if (isResponsiveSite){
		pushTaboolaObject();
		var taboolaContainer = $("#" + '' + "-title");
					taboolaContainer.append("");
			} else {
		if ("mobile" === Taboola().getPlatform()){
			pushTaboolaObject();
		}
	}
</script>

          
        							

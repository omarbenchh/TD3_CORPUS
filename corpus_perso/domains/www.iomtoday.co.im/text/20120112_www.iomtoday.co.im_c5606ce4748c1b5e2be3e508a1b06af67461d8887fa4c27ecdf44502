					taboolaObject.container = "taboola-below-article-thumbnails";
				_taboola.push(taboolaObject);
	}
	if (isResponsiveSite){
		pushTaboolaObject();
		var taboolaContainer = $("#" + 'taboola-below-article-thumbnails' + "-title");
					taboolaContainer.append("");
			} else {
		if ("desktop" === Taboola().getPlatform()){
			pushTaboolaObject();
		}
	}
</script>

          
        


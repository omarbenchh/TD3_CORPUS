<script type="text/javascript" language="javascript">

	function rateComment(inId) {

		var vElem		= __$('rev_'+inId);
		var vElemDiv	= __$('com_'+inId);
		var vElemLine	= __$('com_line_'+inId);

		if (vElem.rev=='show') {
			vElem.rev = 'hide';
			vElemDiv.style.display = 'none';
			vElem.innerHTML = 'Показать';

			if(vElemLine!=null)
				vElemLine.style.display = 'block';
		} else if (vElem.rev=='hide') {
			vElem.rev = 'show';
			vElemDiv.style.display = 'block';
			vElem.innerHTML = 'Скрыть';

			if(vElemLine!=null)
				vElemLine.style.display = 'none';
		}

	}

	function rateAJAX(inUserComId, inComId, inVal) {

		var vars = [{'name':'comRateHttpReq', 'value':'1'}, {'name':'userComId', 'value':inUserComId}, {'name':'comId', 'value':inComId}, {'name':'valSwitch', 'value':inVal}];

		var vSpan	= __$('rate_' + inComId);
		var vBuffer	= vSpan.innerHTML;

		var prikol2LoadObj = new UserHttpRequest(vars, 'http://ivona.bigmir.net/health/news/323860-Spasti-zheludok-posle-novogodnih-guljanij-pomozhet-sup');

		prikol2LoadObj.onSate_Complete = function() {
			var vBody=prikol2LoadObj.httpRequest.responseJS.body;
			if (vBody=='not_allow') {
				vBody = '';
				vSpan.innerHTML = 'Вы уже проголосовали за этот комментарий!';
			} else if (vBody=='error'||vBody=='') {
				vBody = '';
				vSpan.innerHTML = 'Ошибка';
			}
			vBody = vBody.replace(/[^-+\d]/g,'');
			if(vBody) {
				vSpan.innerHTML = vBuffer;
				var oRateVal = __$('rate_val_' + inComId);
				oRateVal.className = vBody == 0 ? "c-gray" : (vBody > 0 ? "comment_green" : "comment_red");
				var vRateVal = oRateVal.innerHTML = "<b>" + vBody + "</b>";
				var vRateJs = __$('rate_js_' + inComId);

				if (vRateJs!=null) vRateJs.innerHTML = '';
			}
		}

		prikol2LoadObj.Do_BeforSend = function() {
			vSpan.innerHTML = '<img src="//bm.img.com.ua/hp/img/progress_bar.gif" />';
		}

		prikol2LoadObj.Do_After_ExceedTimeout = function() {
			vSpan.innerHTML = vBuffer;
		}

		prikol2LoadObj.Load(true);
	}
</script>



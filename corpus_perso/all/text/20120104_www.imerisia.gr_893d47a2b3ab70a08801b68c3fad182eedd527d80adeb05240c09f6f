    position:absolute;
    top:0;
    left:0;
    z-index:8;
    list-style: none;
    opacity:0.0;
}

#promoshow li.active {
    z-index:10;
    opacity:1.0;
}

#promoshow li.last-active {
    z-index:9;
}


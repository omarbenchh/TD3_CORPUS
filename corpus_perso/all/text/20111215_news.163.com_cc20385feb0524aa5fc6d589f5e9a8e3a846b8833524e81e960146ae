		}
	]};
	var domain = window.location.host;
		domain = domain.split(".")[0];
		var len = demo.obj.length;
		var arr = [];
		var j = 0;
		for(var i=j=0;i<len;i++){
			if(domain != demo.obj[i].name){
				arr[j] = demo.obj[i].url;
				j++;
			}
		};
		if(document.getElementById(demo.id) && arr.length != 0){
			var src = arr[Math.floor(Math.random()*arr.length)];
			var html = "<iframe src='" + src +"' width='" + demo.width +"' height='" + demo.height +"' frameborder='0' border='0' marginwidth='0' marginheight='0' scrolling='no'></iframe>";
			document.getElementById(demo.id).innerHTML = html;
		}
})();

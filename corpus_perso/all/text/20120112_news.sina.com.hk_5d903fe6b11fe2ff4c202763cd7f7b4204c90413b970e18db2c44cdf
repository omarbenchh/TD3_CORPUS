        	utag_data.net_subsubsubsec = utagSections[3];
        	utag_data.net_content_type = (utag_data.net_content_type ? utag_data.net_content_type + "+" : "") + "story";
                utag_data.ad_pagetype = overridePageType || utag_data.net_content_type;

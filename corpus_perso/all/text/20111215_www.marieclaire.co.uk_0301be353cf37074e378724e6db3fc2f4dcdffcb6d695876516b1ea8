// Facebook like tracking for Google analytics 
var go = function( ) {
    if (window.FB !== undefined && typeof(ipc) == 'object' && typeof(ipc.analytics) == 'object' && typeof(ipc.analytics.trackSocial) == 'function') {
        FB.Event.subscribe('edge.create', function(response, widget) {
              if (typeof(callback_FBedgeCreate) != "undefined") { 
                    callback_FBedgeCreate (response, widget);
              } else {
                     ipc.analytics.trackSocial({
                           'action' : 'Like',
                           'network' : 'Facebook',
                           'url' : response
                     });
              }
        });

        FB.Event.subscribe('edge.remove', function(response) {
           ipc.analytics.trackSocial({
              'action' : 'Unlike',
              'network' : 'Facebook',
              'url' : response
           });
       });

       FB.Event.subscribe('message.send', function(response) {
           ipc.analytics.trackSocial({
              'action' : 'Send',
              'network' : 'Facebook',
              'url' : response
           });
       });

	FB.Event.subscribe('comment.create', function(response) {
           ipc.analytics.trackSocial({
              'action' : 'Comment',
              'network' : 'Facebook',
              'url' : response
           });
	});
    } else {
        setTimeout(go, 100);
    }
};

go();

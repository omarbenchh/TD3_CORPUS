    <h1 class="cN-headingPage" itemprop="name">Science proves alcohol is fun</h1>
    <!-- google_ad_section_end -->
    <div class="meta hnews">
        <dl>
            <dt class="hiddenVisually">Date</dt>
            <dd class="updated dtstamp">
                <time itemprop="datePublished" datetime="January 13, 2012">
                January 13, 2012</time>
            </dd>
        </dl>
        <!--todo: on UGC-->
        <ul class="tools">
            <li class="reading collectorHidden" style="display:none">(29)</li>
            <li class="comments"><a href="#comments">Comments <strong>16</strong></a></li>
                <li class="clipping" data-assetid="d-1pxj7" data-assettype="ARTICLE" data-asseturl="http://www.theage.com.au/technology/sci-tech/science-proves-alcohol-is-fun-20120112-1pxj7.html">
                    <button class="btnClipping btnFauxLink" type="button">Read later</button>
                </li>

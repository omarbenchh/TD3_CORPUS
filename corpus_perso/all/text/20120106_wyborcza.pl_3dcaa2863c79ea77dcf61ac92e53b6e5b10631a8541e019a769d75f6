	var pianoIntegrator = {
		
		serviceUrl: "https://login.wyborcza.pl/services/piano/update-subscription",
				
		updateSubscription: function(email, remainingDays) {

			var pianoVisitKey = this.getCookie("pianovisitkey");
			var url = this.serviceUrl +"?email="+ email +"&remainingDays="+ remainingDays +"&visitKey="+ pianoVisitKey;
			
		    this.jsonpRequest({
			    url : url,
			    jsonpCallback : ""
			});
		    

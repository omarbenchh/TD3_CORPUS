<script type="text/javascript">
var zero = 0;
var cmtNum = "0";
try{
	if(document.getElementById("commentCount").innerText)
	cmtNum = document.getElementById("commentCount").innerText ;
	else if(document.getElementById("commentCount").textContent)
	cmtNum = document.getElementById("commentCount").textContent;
	zero = parseInt(cmtNum);
}catch(e){
	zero = 0;
}
if(document.getElementById("cmtspan"))
{
document.getElementById("cmtspan").style.display='';
if(zero == 0 ) document.getElementById("cmtspan").innerHTML = "欢迎发表评论<span id='commentCount' style='display:none'>0</span>";
else  document.getElementById("cmtspan").innerHTML = "已有<span id='commentCount'>"+zero+"</span>条评论";
}
</script>

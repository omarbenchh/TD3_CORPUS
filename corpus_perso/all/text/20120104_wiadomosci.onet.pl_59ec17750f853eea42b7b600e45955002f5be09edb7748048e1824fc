<script type="text/javascript">
    var commaLength = 4,
        spaceLeft = $('#relatedTopics').width() - $('#relatedTopicsEllipsis').width() - $('#seeAlsoRelatedTopics').width(),
        allTopicsCount = 2,
        currentTopicsCount, 
        currentWidth;

    $('.relatedTopic').each(function(ind, el){
        currentWidth = $(el).width();
        if (spaceLeft-currentWidth-commaLength>=0) {
            $(el).css('visibility', 'visible');
            spaceLeft = spaceLeft - currentWidth - commaLength;
        } else {
            $(el).remove();   
        }
    });
    
    var newContent = $('.relatedTopic:last a');
    $('.relatedTopic:last').html(newContent);
    
     currentTopicsCount = $('.relatedTopic').length;
     if (currentTopicsCount !== allTopicsCount) {
        $('.relatedTopic:last').append($('#relatedTopicsEllipsis'));
        $('#relatedTopicsEllipsis').css('visibility', 'visible'); 
     }   
     
     $('.relatedTopic:not(:last)').each(function(ind, el){
        $(el).append(', ');
     });   
     
     $('#newsstream ul').css({'border-bottom': 'none', 'padding-bottom': 0, 'margin-bottom': 0});
     $('#relatedTopics').css('border-bottom', '1px solid #E8E8E8');
     
</script>

                        
                        
                        

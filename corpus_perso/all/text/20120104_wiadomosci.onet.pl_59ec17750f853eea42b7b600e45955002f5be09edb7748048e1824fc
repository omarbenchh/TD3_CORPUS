        <noscript><img height="1" width="1" src="http://kropka.onet.pl/_s/kropka/1?DV=WIADOMOSCI/SWIAT&amp;PU=af6c29d2-b84a-41aa-a386-a87c8ca41cea" alt="" /></noscript>
    
    <script type="text/javascript">

if ((typeof mvp == 'undefined' || typeof mvp.PlayerFacade == 'undefined') && $(".mvp").length > 0) {
    var head = document.getElementsByTagName("head")[0];
    script = document.createElement('script');
    script.type = 'text/javascript';
    script.defer = true;
    script.src = "http://lib.onet.pl/mvp/mvp-loader.js";
    head.appendChild(script);

    if($(".multigallery-autoslide").length>0 || $(".multigallery-autoslideEnd").length>0){
        script.onload = function(){
                mvp.ready = function(facade) {
                    console.log("mvp.ready");
                    mvp.LocalMediator = function() {
                        console.log("mvp.LocalMediator");
                        this.Extends = mvp.Mediator; 
                        this.initialize = function() {
                            this.parent(mvp.LocalMediator.NAME);           
                        };
                        this.handleNotification = function(nott) {
                            switch(nott.getName()) {
                                case mvp.PlayerFacade.END:
                                    if($(".multigallery-autoslide").length>0){
                                        if(multigallery && multigallery.nextSlide){
                                            multigallery.nextSlide();
                                        }
                                    }
                                    if($(".multigallery-autoslideEnd").length>0){
                                        if(multigallery && multigallery.closeSlide){
                                            multigallery.closeSlide();
                                        }
                                    }
                                    
                                    break;
                            }   
                        }
                        this.listNotificationInterests = function() {
                            return [
                                mvp.PlayerFacade.END
                            ];
                        };
                    };
                    mvp.LocalMediator = new mvp.Class(new mvp.LocalMediator()); 
                    mvp.LocalMediator.NAME = "mvp_LocalMediator";
                    if(!facade.hasMediator(mvp.LocalMediator.NAME)) {
                      var localMediator = new mvp.LocalMediator();
                      facade.registerMediator(localMediator);
                    }

                };  
        }
    }

}

</script>
    




    







    
    

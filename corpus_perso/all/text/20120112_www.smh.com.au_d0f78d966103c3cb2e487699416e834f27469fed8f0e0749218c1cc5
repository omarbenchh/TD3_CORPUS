    <h1 class="cN-headingPage" itemprop="name">Alcohol releases feel-good endorphins, study reveals</h1>
    <!-- google_ad_section_end -->
    <div class="meta hnews">
        <dl>
            <dt class="hiddenVisually">Date</dt>
            <dd class="updated dtstamp">
                <time itemprop="datePublished" datetime="January 13, 2012">
                January 13, 2012</time>
            </dd>
        </dl>
        <!--todo: on UGC-->
        <ul class="tools">
            <li class="reading collectorHidden" style="display:none">(29)</li>
            <li class="clipping" data-assetid="d-1pxiz" data-assettype="ARTICLE" data-asseturl="http://www.smh.com.au/world/science/alcohol-releases-feelgood-endorphins-study-reveals-20120112-1pxiz.html">
                    <button class="btnClipping btnFauxLink" type="button">Read later</button>

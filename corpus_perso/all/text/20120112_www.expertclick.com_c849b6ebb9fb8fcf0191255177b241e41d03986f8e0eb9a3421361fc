<script language="javascript" type="text/javascript">   
    function ShowHideSpecialFeatures(shouldShow)
    {
        var divSpecialFeatures = document.getElementById('divSpecialFeatures');
        var divDisplayContainter = document.getElementById('divDisplayContainter');
        if(shouldShow)
        {
            divSpecialFeatures.style.display = '';
            divDisplayContainter.style.display = 'none';
            document.getElementById('divOtherExperts').style.display='none';
        }
        else
        {
            divSpecialFeatures.style.display = 'none';
            divDisplayContainter.style.display = '';
            document.getElementById('divOtherExperts').style.display='';
        }   
        SetHeader();     
    }
 
    window.onresize = SetHeader;
    SetPrimaryNavigation();
    //SetHeader();
    
    SetHeader();
    function SetPrimaryNavigation()
    {
        var tdPrimaryNavigation = document.getElementById('tdPrimaryNavigation');
        var divPrimaryNavigation = document.getElementById('divPrimaryNavigation');
        tdPrimaryNavigation.innerHTML = divPrimaryNavigation.innerHTML;
    }
    function SetHeader()
    {
        var headerContainer = document.getElementById('ucHeader');
        var divHeaderHtml = document.getElementById('divHeaderHtml');        
        var position = new Array();
        position = GetPosition(headerContainer);             
        divHeaderHtml.style.left = position[0] + 'px'; 
        divHeaderHtml.style.top = 0 + 'px';
    }
    
    function GetPosition( oElement ) 
    {
        if( typeof( oElement.offsetParent ) != 'undefined' ) 
        {
            for( var posX = 0, posY = 0; oElement; oElement = oElement.offsetParent ) 
            {
                posX += oElement.offsetLeft;
                posY += oElement.offsetTop;
            }
            return [ posX, posY ];
        } 
        else 
        {
            return [ oElement.x, oElement.y ];
        }
    }
</script>
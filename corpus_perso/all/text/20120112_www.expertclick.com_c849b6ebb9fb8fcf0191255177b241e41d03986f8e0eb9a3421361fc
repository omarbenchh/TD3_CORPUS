<script type='text/javascript'>
    function showDiv() {
        document.getElementById('divRegisterEmail').style.display = "inline";
        document.getElementById('divtable').style.display = "inline";
    }
    function hide() {
        document.getElementById('divtable').style.display = "inline";
        document.getElementById('divsucess').style.display = "none";
    }
    function checksubscriberEmail(sender, args) {
        var txtEmail = document.getElementById('txtEmailRSS');
        if (txtEmail.value == null || txtEmail.value == '') {
            args.IsValid = false;
        }
        else {
            args.IsValid = true;
        }
    }
    function hidealert() {
        document.getElementById('lblsucess').style.display = "none";
        document.getElementById('lblalert').style.display = "none";
    }
</script>

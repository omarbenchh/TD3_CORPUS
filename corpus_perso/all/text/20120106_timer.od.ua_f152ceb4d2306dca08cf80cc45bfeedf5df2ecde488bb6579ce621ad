<script>
var sRotations= [];
var BTimer= setInterval(function()
{
	$('.GetB').map(
		function(index, element){
			RID= $(element).attr('RID');
			LastIndex= $(".Rotation"+ RID).length- 1;
			Rnum= getRandomInt(0, LastIndex);
			if(sRotations[RID+ 'current']== undefined) sRotations[RID+ 'current']= 0;

			$(".Rotation"+ RID).each(function(i,elem) {
				$(elem).hide();
				if(i== sRotations[RID+ 'current'])
				{
					$(elem).show();
				}
			});
			sRotations[RID+ 'current']++;
			if(sRotations[RID+ 'current']> LastIndex) sRotations[RID+ 'current']= 0;

//			alert(sRotations[RID+ 'current']);
		});
}, 10000);

// использование Math.round() даст неравномерное распределение!
function getRandomInt(min, max)
{
  return Math.floor(Math.random() * (max - min + 1)) + min;
}

</script>


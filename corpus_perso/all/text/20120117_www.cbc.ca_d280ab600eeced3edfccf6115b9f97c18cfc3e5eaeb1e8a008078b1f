      <script type="text/javascript">
  try{

    var minId = 2000000;//Oct 2013
   var listmax = 9;
    var listcount = 0;
    var mv = document.querySelector('.most-viewed-filtered');
    var elements = mv.querySelectorAll('.lineuproll-item-body');
    for (var i=0; i<elements.length; i++) {
     
      var anchor = elements[i].querySelector('a');
      var id = anchor.href.substr(anchor.href.lastIndexOf('.')+1);
      
      if (parseInt(id) < minId || listcount>=listmax){
        elements[i].removeChild(anchor);
        elements[i].style.display='none';
      }else{
      	listcount++;
      }
    }
  }catch(e){}
</script>
  



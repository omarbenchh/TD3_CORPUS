						$('div.social-close').click(function(){
							$(this).parent().css('display', 'none');
						});
					  	
					  	$('div.widget-social-icon > img').click(function(){
						  	$('div.social-icon').css('display', 'none');
							$(this).next('div.social-icon').css('display', 'inline-block');
						  	$(this).next('div.social-icon').css('opacity', '1');
						});
					});  
					</script>  

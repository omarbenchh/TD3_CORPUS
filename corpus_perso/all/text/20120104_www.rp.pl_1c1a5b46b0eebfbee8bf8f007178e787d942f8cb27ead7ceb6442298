<script>
$(document).ready(function() {
$(".search_btn").click(function () {
      var status = $(".searchbar").css("display");
      if(status == "block"){
           $(".searchbar").css("display","none");
           $("div.menu").css("marginBottom","18px");
      }else{
           $(".searchbar").css("display","block");      
           $("div.menu").css("marginBottom","1px");
      }
});
});
</script>




<script type="text/javascript">
    jCMS_domainName = 'http://www.forbes.pl';
//    jCMS_ssoCookiesChecker();
//    jCMS_setHighChartGradient();

    $(document).ready(function () {
        jCMS_mainMenuHangings($("nav[class=Top-Menu]"));
    });

    //funkcja wywołująca lazy load poprzez hover lub kliknięcie
    function customLazy(id,event,contId){

        /*
        * id - element wywołujący funkcję
        * event - rodzaj eventu wywołania (hover lub click)
        * contId - id bloczka do przeładowania zdjęć
        */

        if(event=="click"){
            $(id).click(function() {
                $(contId + " img.lazy").lazyload({
                    chainable: false,
                    bind: "event",
                    effect : "fadeIn"
                });
            });
        }
        if(event=="hover"){
            $(id).hover(function() {
                $(contId + " img.lazy").lazyload({
                    chainable: false,
                    bind: "event",
                    effect : "fadeIn"
                });
            },function(){});
        }
    };

    function isiPhone(){

        return (
            //Detect iPhone
                (navigator.platform.indexOf("iPhone") != -1) ||
            //Detect iPod
                (navigator.platform.indexOf("iPod") != -1) ||
            //Detect iPad
                (navigator.platform.indexOf("iPad") != -1)
                );
    }

    function foriPad(){
        $(window).scroll(function(){
            var fromTop = $(document).scrollTop();
            if(fromTop > 50){
                if(fromTop > 500){
                    $(".flexMenu-viewMore").removeClass("under500");
                }
                else{
                    $(".flexMenu-viewMore").addClass("under500");

                }
            }
            else{
            }
        });
    };

    if(isiPhone()){
        foriPad();
    }

</script>






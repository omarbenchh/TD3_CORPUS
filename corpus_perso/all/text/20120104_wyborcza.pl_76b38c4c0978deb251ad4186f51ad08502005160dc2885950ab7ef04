<script type="text/javascript">
    function loadPopupHTML() {
        
        
        
        
        var html = '<div class="in"><span id="closeMark"></span><span class="logo"></span><p>Do&#322;&#261;cz do nas na<br />Facebooku i otrzymuj<br />najlepsze artyku&#322;y<br />z Forbesa</p><a class="likepopup" href="https://www.facebook.com/ForbesPolska"></a><span class="footer">Je&#380;eli nie chcesz polubi&#263; strony, zamknij komunikat.</span></div>';
        
        
        var e = document.createElement("div");
        e.id = "popup";
        e.className = "popup1"
        e.style.display = "none";
        e.innerHTML = html;
        document.body.appendChild(e);
    }

    function loadNLPopupHTML() {
        var e1 = document.createElement("div");
        e1.id = "popupOverlay";
        document.body.appendChild(e1);

        var html = '<a id="closeMark" class="zamknij" href="#"><img src="/resources/front/images/zamknij.png" width="50" height="50" alt="zamknij" /></a> <img src="/resources/front/images/forbes_newsletter_logo.png" width="161" height="58" alt="newsletter" /><div class="content"><h2>Zapisz się do newslettera</h2><span>Otrzymuj najlepsze teksty z Forbesa<br />prosto na swojego e-maila.</span><form action="newsletter.html"><input id="subscribe_email_pop" type="text" class="mail" name="email" onfocus="if(this.value == \'wpisz e-mail\'){this.value = \'\';}" onblur="if(this.value == \'\'){this.value=\'wpisz e-mail\';}"><input class="zapiszButton" type="submit" value="zapisz"></form></div>';
        var e = document.createElement("div");
        e.id = "popupNL";
        e.style.display = "none";
        e.innerHTML = html;

        document.body.appendChild(e);
    }

    $(document).ready(function () {
        var val = document.domain;
        if (val == 'undefined') {
            return;
        }
        if (val.indexOf("pierwszymilion") >= 0 || val.indexOf("life") >= 0 || val.indexOf("www") >= 0) {
            if(! /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {
                if ($.cookies.get('forbes_popup') == 0) {
                    var exdate = new Date();
                    exdate.setDate(exdate.getDate() + 365);
                    var val = "forbes_popup=1;path=/;expires=" + exdate.toUTCString();
                    document.cookie = val;

                    exdate = new Date();
                    exdate.setDate(exdate.getDate() + 1);
                    var val2 = "forbes_nl_popup=1;path=/;expires=" + exdate.toUTCString();
                    document.cookie = val2;
                    loadPopupHTML();
                    $('#popup.popup1').show();
                    $('.popup1 #closeMark').click(function () {
                        $('#popup.popup1').hide();
                    });
                }

                if ($.cookies.get('forbes_nl_popup') == null && $.cookies.get('forbes_nl_popup_disp') == null) {
                    exdate = new Date();
                    exdate.setMonth(exdate.getMonth() + 3);
                    var val3 = "forbes_nl_popup_disp=1;path=/;expires=" + exdate.toUTCString() + ";";
                    document.cookie = val3;
                    loadNLPopupHTML();
                    $('#popupNL').show();
                    $('#popupNL #closeMark').click(function () {
                        $('#popupNL').hide();
                        $('#popupOverlay').hide();
                    });
                }
            }
        }
    });


</script>




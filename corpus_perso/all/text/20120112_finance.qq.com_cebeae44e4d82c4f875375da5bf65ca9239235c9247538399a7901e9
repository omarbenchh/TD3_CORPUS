	var keyword = qq.GT(hd, 'span'), i = 0;

	//相关内容曝光
	if(bd.children.length > 0){
		new BOSS.Exposure(bd, 'EXrNews');
	}
	
	//关键词曝光
	if(keyword.length > 0){
		new BOSS.Exposure(hd, 'EXkeyword');
	}

	//相关视频list曝光
	if(qq.G('aboutVideoTjlist') && qq.G('aboutVideoTjlist').getElementsByTagName('li').length > 0){
		new BOSS.Exposure(qq.G('aboutVideoTjlist'), 'EXrNews_video');
	}


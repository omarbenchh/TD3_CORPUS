function navigate_tabs(container, tab){
	$(".b").css('display' , 'none');
	$(".c").css('display' , 'none');
	$(".d").css('display' , 'none');
	$(".e").css('display' , 'none');
	$(".f").css('display' , 'none');
	$(".a").css('display' , 'none');
	
	$("#1-tab").removeClass('buttonHover');
	$("#2-tab").removeClass('buttonHover');
	$("#3-tab").removeClass('buttonHover');
	$("#4-tab").removeClass('buttonHover');
	$("#5-tab").removeClass('buttonHover');
	$("#6-tab").removeClass('buttonHover');
	
	$("#"+tab).addClass('buttonHover');
	$("."+container).show('slow');
}

<script type="text/javascript">
    //<![CDATA[
    function cisJsonClient()
{
	var clients = {
		'common' : {obj:false, status: false}
	},
	def = 'common';

	this.client = function(c) {
		c = c || def;
		if(!clients[c] || clients[c].status !== true) {
			clients[c] = {
				obj:jQuery.Zend.jsonrpc({url: "/api/v1/jsonrpc?map=" + c}),
				status:true
			};
		}
		return clients[c].obj;
	}

	/*deprecated*/
	this.catalogMap = function() {
		return this.client('catalog');
	}

}
document.jsonrpcWrapper = new cisJsonClient();
var jsonrpcWrapper = document.jsonrpcWrapper;    //]]>
</script>

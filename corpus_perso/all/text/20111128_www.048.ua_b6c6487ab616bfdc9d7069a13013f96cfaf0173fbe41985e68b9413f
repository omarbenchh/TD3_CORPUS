	// Кнопка вверх
	$(function(){
		var backTopButton = $('.back_top_button');
		var container = $('#wrapper');

		backTopButton.click(function(){
			$('html, body').animate({
			scrollTop: 0
			}, 400);

			return false;
		});

		$(window).scroll(function () {
			if ($(this).scrollTop() > container.offset().top) {
				backTopButton.fadeIn();
			//			backTopButton.css('left', container.offset().left - backTopButton.width() - 8);
			} else {
				backTopButton.fadeOut();
			}
		});
	});	

	$('a.loader').each(function() {
		var $loadingIndicator = $('<div class="loader"><img src="/design/img/loading.gif"></div>');
		var loadMore = function(evt) {
			evt.preventDefault();
			$(this).after($loadingIndicator).remove();
			$loadingIndicator.show();
			$.get($(this).attr('href'), function(data) {
				var $data = $($.parseHTML(data));
				$data.appendTo($loadingIndicator.parent());
				$loadingIndicator.hide();
				$('body').trigger('newPageDataLoaded');
				$('noscript').remove();				
				$('a.loader').each(function() {
					$(this).show().click(loadMore);
				});
			});
		};
		$('noscript').remove();
		$(this).show().click(loadMore);
	});

	$('.cisinza').colorbox({
		onOpen: function(){
			$('html').css({overflow : 'hidden'});
			$.get('http://www.048.ua/auth/login',function(data){
				$.colorbox({
					html : data, 
					title : 'Авторизация',
					onClosed: function(){
						$('html').css({overflow : 'auto'});
					}
				});
			});
		}
	});
	
	
});

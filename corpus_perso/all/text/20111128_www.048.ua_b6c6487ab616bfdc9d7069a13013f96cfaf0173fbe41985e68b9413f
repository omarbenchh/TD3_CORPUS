<script type="text/javascript">
    //<![CDATA[
    	function auth(token)
	{
		$.get('http://www.048.ua/auth/entrance', {token:token}, function(data){
			if(data.success == 1)
			{
				$('.cisinza').colorbox.close();
				$(document).trigger('auth');
			}
		});
	}

    // формирование ссылки с utm
    function utm(elm) {
        if($(elm).data('utm').length > 0) {
            var href = $(elm).attr('href') + $(elm).data('utm');
            $(elm).attr('href', href);
            $(elm).data('utm', '')
        }
    }
	
	$(document).on('auth',function(){
		location.reload();
	});
    //]]>
</script>

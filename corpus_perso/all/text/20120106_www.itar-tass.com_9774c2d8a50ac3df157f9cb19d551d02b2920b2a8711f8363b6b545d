                            <span>
                                <a id="{{item.id}}" name="#{{item.id}}">
                                    <span class="day" data-ng-bind-html="item.formatted_time"></span>{{item.suffix?", "+item.suffix:""}}
                                </a>
                            </span>
                        </div>
                        <div class="text" broadcastcompile="item.message"></div>
                    </div>

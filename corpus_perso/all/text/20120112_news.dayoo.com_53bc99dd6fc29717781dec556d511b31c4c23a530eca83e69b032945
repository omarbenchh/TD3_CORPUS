//var _hmt = _hmt || [];
function searchSubmit(){
	var select = $("#searchselect").find("option:selected").val(); //被选中的值
	var search = $("#search").val();  //搜索值
	if(search == '请输入关键词')
	{
		search = "";
	}
	var link = null;

	if (select == "baidu") {
		link = "http://zhannei.baidu.com/cse/search?q=" + search + "&s=12590748706517226876&nsid=0" ;
	} else if (select == "360") {
		link = " http://www.so.com/s?q=" + search + "&pq="+ search +"&src=srp&rg=1&site=dayoo.com";		
	} else if (select == "sogou")	{
		link= "http://www.sogou.com/web?query=" + encodeURIComponent(search)+ "+site:dayoo.com";
	}else if (select == "google")	{
		link= "http://www.google.com.hk?q=" + encodeURIComponent(search)+"&cx=partner-pub-2065709201317913:4000532658&ie=UTF-8";
	}
	window.open(link);
	//_hmt.push(['_trackEvent','zhannei_search','click',select]);
	_gaq.push(['_trackEvent','zhannei_search','click',select]);
	return false;
}
</script>

			<label><input name="contact" id="contactYes" class="contact" value="Yes" type="radio" required/> Yes</label>
			<label><input name="contact" id="contactNo" class="contact" value="No" type="radio" /> No</label>
		</div>
		<div id="extra-info">
            <span class="V4-error error-fullname"></span>
            <div class="V4-input-wrap">
                <label for="fullname">Your Name: *</label>
                <input class="populateInfo"  name="fullname" type="text" value="{{curUserInfo.fullname}}" required/>
            </div>
            <span class="V4-error error-email"></span>
            <div class="V4-input-wrap">
                <label for="email">Your Email Address (This will only be used to contact you if we have a question about your submission, it will not be used to identify you or be published.): *</label>
                <input class="populateInfo" type="email" name="email" id="email" value="{{curUserInfo.email}}" required/>
            </div>
        </div>
        <div class="V4-input-wrap">
		    <span class="V4-error V4-register-max-width error-reCAPTCHA"></span>
        </div>
		<div id="v4-recaptcha-report-error" class="cb"></div>
		<div class="cb">
			<input type="button" value="cancel" class="fl cancel">
			<input type="button" value="Submit" class="fr submit">
		</div>
	</form>	
	<div class="clear">

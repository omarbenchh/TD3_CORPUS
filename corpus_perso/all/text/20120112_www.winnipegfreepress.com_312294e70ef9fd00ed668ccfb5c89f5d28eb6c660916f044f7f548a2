					{{ /jobs }}
					</ul>
				{{ /top_jobs }}
				</div>
				<div class="button-line bottom cb"><a href="Javascript: void(0);" class="previous">PREV</a> / <a href="Javascript: void(0);" class="next">NEXT</a></div>
   			</div>
        </div>
    </div>
</div>
</script>

<script type='text/x-mustache'   id='v4_tile_iframe'>
<!-- expects json data object:
	{data: { url: 'header click link (optional)'
		, title: 'header name'
		, iframe_src: 'url of iframe embed'
		, size: 1 //(defaults to 1, can be 2 or 3), iframe width adjusts accordingly
	}
	position: {
		size: 1 //(defaults to 1, can be 2 or 3), iframe width adjusts accordingly
	}} -->
<div class="tile {{ css_class }}">
	<div class="tile-padding">
		<div class="tile-content">
		{{ #url }}
			<h3><a href="{{ url }}">{{ title }}</a></h3>
		{{ /url }}
		{{ ^url }}
			<h3 style="padding: .25em .5em;">{{ title }}</h3>
		{{ /url }}
			<div class="responsive-wrap"><iframe src="{{ iframe_src }}" frameborder="0" scrolling="no"> </iframe></div>
		</div>
	</div>
</div></script>


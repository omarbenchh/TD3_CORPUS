         });

//         $(".searchIcon").click(function () {
//             //closeMenu();
//             //if (searchSate) { closeSearch() } else { openSearch() }
//         });

         var fl = 0;
         $(".menuMore").click(function (event) {
             if (fl == 0) {
                 $(".menuMore ul").addClass("selected");
                 fl = 1;
             }
             else {
                 $(".menuMore ul").removeClass("selected");
                 fl = 0;
             }
             event.stopPropagation();
         });

         $("body").click(function () {
             if (fl == 1) {
                 $(".menuMore ul").removeClass("selected");
                 fl = 0;
             }
         });

     });

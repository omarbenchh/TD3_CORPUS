<script type="text/javascript">
					function definePage () {

	var parentChapter, tabChapter, s_pagenameCheck;
	var subChapter ="1";
	var theUrl=location.href;
	s_pagenameCheck = theUrl.toLowerCase();
	
	if (s_pagenameCheck.lastIndexOf("?")  >  0) {
		var s_pagenameCheck = s_pagenameCheck.substring(0,s_pagenameCheck.lastIndexOf("?"));
	} 
					
		if( s_pagenameCheck in chapters ) { 
		parentChapter = s_pagenameCheck;
		highlightLeftBin(parentChapter);
                  highlightCenterBin(parentChapter);
		highlightTab();
		} else {
		
			if( s_pagenameCheck in c1 ) { parentChapter = chapters[0]; tabChapter = "1";}
			
			if( s_pagenameCheck in c2 ) { parentChapter = chapters[1]; tabChapter = "2";}
			
			if( s_pagenameCheck in c3 ) { parentChapter = chapters[2]; tabChapter = "3";}
			
		if( s_pagenameCheck in c4_1 ) { parentChapter = chapters[4]; tabChapter = "4";subChapter = "1";}
	
		if( s_pagenameCheck in c4_2 ) { parentChapter = chapters[4]; tabChapter = "4";subChapter = "2";}
	
			if( s_pagenameCheck in c5 ) { parentChapter = chapters[4]; tabChapter = "5";}
			
			if ((typeof(parentChapter)!="undefined")  &  (typeof(tabChapter)!="undefined")) {
			highlightLeftBin(parentChapter);
                           highlightCenterBin(parentChapter);
			setNavState(tabChapter, subChapter);
			}
		}
	}
	
	definePage();
				
				</script><div id="newsletter-signup-harmony" class="newsletter-harmony"> </div>
<script>
						var containerAboveCurrentScriptElement = $('script').last().prevAll('div.newsletter-harmony:first');
						$(window).load($.proxy(function() {
							var containerId = 'newsletter-signup-harmony',
								containerEle,
								isCenterRail,
								uiMode = 'harmony-right';

							containerEle = $(this[0]);   //obj 'this' is the context passed in by $.proxy method.

							//[step-1] :  ensure the uniqueness of the container ID
							if(containerEle.length > 0) {
								var rand = Math.random(),
									randNum = rand*1000-(rand*1000)%1;
								containerId =  containerEle.attr('id') + '_' + randNum;
								containerEle.attr('id', containerId);
							} else {
								containerEle = $('#' + containerId);
							}

							//[step-2] :  determine whether it is a center-well or/and homepage mode and add class accordingly.
							isCenterRail = containerEle.position().left < $(window).width() / 2;

							if(isCenterRail == true) {
								containerEle.addClass('center');
								uiMode = 'harmony-center';
							}

							if (containerEle.closest('#s4').length > 0 && containerEle.closest('#s4').position().left < $(window).width() / 2) {
								containerEle.addClass('homepg');
							}

							require(['newsletter/1/mapping-centers-desktop', 'newsletter/1/moduleUI-harmony', 'css!newsletter/1/newsletter-signup-harmony.css'], function(module, UiModule, cssTxt){
								//$('<style type="text/css"></style>').html(cssTxt).appendTo("head");
								module.init({
									selector: '#' + containerId,
									uiMode: uiMode,
									uiModule: new UiModule()
								});
							});
						}, containerAboveCurrentScriptElement));
					</script></div>

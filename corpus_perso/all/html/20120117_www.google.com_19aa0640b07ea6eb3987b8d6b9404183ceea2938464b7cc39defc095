else if( document.domain.indexOf('belfasttelegraph') !== -1 ){
$.cookie('site_preference','mobile',{ expires: 30, path: '/', domain: ".belfasttelegraph.co.uk" });
}
else if( document.domain.indexOf('herald') !== -1 ){
$.cookie('site_preference','mobile',{ expires: 30, path: '/', domain: ".herald.ie" });
}
$.cookie.json = oldJsonValue;
window.location.href = window.location.href.replace('//www.','//m.');
});
});
});
</script>

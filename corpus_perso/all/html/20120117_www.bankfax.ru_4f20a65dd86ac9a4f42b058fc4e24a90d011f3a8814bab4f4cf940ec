    <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem"><a href="/" itemprop="item"><span itemprop="name">Главная</span></a><meta itemprop="position" content="1" /></span> 
    <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem"><a itemprop="item" href="/news/"><span itemprop="name">Лента новостей</span></a><meta itemprop="position" content="2" /></span>
    <span itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem"><font itemprop="item"><span itemprop="name">В Алтайском крае отремонтировали кардиодиспансер.</span></font><meta itemprop="position" content="3" /></span>

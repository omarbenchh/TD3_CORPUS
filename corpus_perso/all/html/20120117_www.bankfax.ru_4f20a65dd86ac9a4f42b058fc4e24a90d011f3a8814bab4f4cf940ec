<script type="text/javascript">

$(document).ready(function(){
	getCalendar();

	var date = new Date();
	$('.iday').val(date.getDate());
	$('.imonth').val(date.getMonth() + 1);
	$('.iyear').val(date.getFullYear());
});

</script>

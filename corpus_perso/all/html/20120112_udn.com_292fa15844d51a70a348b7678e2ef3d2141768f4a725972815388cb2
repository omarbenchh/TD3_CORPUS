(function()
{
    var news_id = 2539528;
    var last_news_id = null;
    if(sessionStorage) {
        last_news_id = localStorage.getItem('last_news_id'); 
        localStorage.setItem('last_news_id', news_id);
    }
    if(news_id != last_news_id) {
        nhi = new Image;
        nhi.src = '/hit/2539528?ref=&r=' + Math.random() + '&p=1&gc=1';
        document.querySelector('.article').appendChild(nhi);
    } else {
        // 
    }
})();
</script>



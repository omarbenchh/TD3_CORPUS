<script type="text/javascript">
$(function()
{
    $("#id-login-form").submit( function()
    {
        var f = this;
        var data = formCollect(this);
        data['news_id'] = 2539528;
        $.ajax({
            dataType: 'json',
            url: '/ajax_login',
            type: "post",
            data: data})
        .done( function(ldata)
        {
            $("p.errors", f).detach();
            if(ldata.islogged)
                gate.onLoggedUser();
            else if (ldata.error) 
            {
                var emsg = ldata.fielderrors;
                if (emsg) 
                {
                    emsg.username && $("div.gate_loginerror", f).prepend('<p class="errors">'+emsg.username+'</p>');
                    emsg.password && $("div.gate_passworderror", f).prepend('<p class="errors">'+emsg.password+'</p>');
                }
                else 
                {
                    $("div.gate_formerror", f).prepend('<p class="errors">'+ldata.error+'</p>');
                }
            }

        });
        return false;
    });
});
</script>

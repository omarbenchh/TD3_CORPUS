<div class="box_pulsinwestora">
<h2>Masz problem? Napisz do nas.</h2>
<div id="investor_form_pulsinwestora">
<form action="/zadajpytanie" method="post" id="inv_form" onsubmit="return investor_validate(this); "><div>
<div id="inv_status" style="font-weight: bold; color: red; "></div>
<textarea name="qtxt" id="inv_qtxt">(Tu wpisz pytanie pytanie)</textarea><br />
Imię i nazwisko:<br />
<input name="author"><br />
Adres email:<br />
<input name="email"><br />
<button class="more" type="submit">Zapytaj</button>
<input type="hidden" name="piciupiciu" value="" />
</div></form>
<script type="text/javascript">
$(document).ready(function() {
function investor_check()
{
    try{
	inv_status = document.getElementById('inv_status');
	inv_qtxt = document.getElementById('inv_qtxt');
	if(inv_qtxt.value.length > 1000) 
	{
		inv_status.innerHTML = 'Tekst jest za długi'; 
		inv_qtxt.value = inv_qtxt.value.substring(0, 1000); 
	} 
	else 
	{
		inv_status.innerHTML = ''; 
	}
    }
    catch(err)
    {
    }
}

});

function investor_validate(f)
{
	var ok = true;
	ok = (f.qtxt.value.length > 10) && ok; 
	ok = (f.author.value.length > 2) && ok;
	re_email = /.+@.+\..+/
	ok = re_email.test(f.email.value) && ok;
    if( ok )
        f.piciupiciu.value = f.qtxt.value.length + f.author.value.length + f.email.value.length
	if(!ok) alert('Proszę wypełnić formularz przed wysłaniem!');  
	return ok;
}
</script>
</div>
</div>

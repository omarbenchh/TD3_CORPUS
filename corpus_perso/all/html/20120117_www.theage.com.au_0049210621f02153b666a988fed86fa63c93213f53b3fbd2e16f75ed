    <h1 class="cN-headingPage" itemprop="name">Study finds increase in caffeine poisoning</h1>
    <!-- google_ad_section_end -->
    <div class="meta hnews">
        <dl>
            <dt class="hiddenVisually">Date</dt>
            <dd class="updated dtstamp">
                <time itemprop="datePublished" datetime="January 16, 2012">
                January 16, 2012</time>
            </dd>
        </dl>
        <!--todo: on UGC-->
        <ul class="tools">

					 }
					 catch(e){
						 alert("您的浏览器未启用[设为首页]功能，开启方法：先在地址栏内输入about:config,然后将首选项名称中 signed.applets.codebase_principal_support 值该为true即可");
					 }
				}
				var prefs=Components.classes['@mozilla.org/preferences-service;1'].getService(Components.interfaces.nsIPrefBranch);
				prefs.setCharPref('browser.startup.homepage','http://www.qianlong.com');
		   }
		}
		// EndSetHomepage
		</script>

    $( document ).ready(function() {
        var userInfo = jQuery.parseJSON(jQuery.cookies.get('__FRONT_UID'));
        if(userInfo){
            $('#userLoginBox .user_box_logged').find('.profil').text(userInfo.nickname.replace(/\+/g,' '));
            $('#userLoginBox .user_box_logged').show();
            $('#userLoginBox .user_box_notlogged').remove();
        } else {
            $('#userLoginBox .user_box_notlogged').show();
            $('#userLoginBox .user_box_logged').remove();
        }
    });

</script>

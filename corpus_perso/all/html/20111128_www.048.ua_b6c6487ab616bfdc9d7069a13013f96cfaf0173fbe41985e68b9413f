	var $topCommentedLi = $('.sidebar_news.smallest_iconed .top.tab li');
	var changeTopCommented = function(period) {
		$topCommentedLi.removeClass('active');
		$('#tTCP_' + period).addClass('active');
		$("#weekComList").hide();
		$("#monthComList").hide();
		$("#yearComList").hide();
		$("#"+period+"ComList").show();
		$.cookie('newstopcommented', period);
	}
	
	$topCommentedLi.click(function() {

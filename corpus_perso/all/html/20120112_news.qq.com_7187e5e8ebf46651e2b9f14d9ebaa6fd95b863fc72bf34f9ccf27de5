function(){a.releaseEvent(a.layerTitle);a.closePopup()};a.layerTitle.appendChild(d);a.layerTitle.appendChild(a.close);a.main.appendChild(a.layerTitle);a.con=a.creatDom({cls:"share_layer_cont"});a.iframe=a.creatDom({tag:"iframe"});a.iframe.setAttribute("frameBorder","0",0);a.iframe.setAttribute("marginheight","0");a.iframe.setAttribute("marginwidth","0");a.iframe.setAttribute("scrolling","no");a.iframe.style.width=a.width+"px";a.iframe.style.height=a.height+"px";a.iframe.style.display="block";a.con.appendChild(a.iframe);

                <div id="trunk">
                    <div id="main">
                        <div itemscope itemtype="http://schema.org/Article">
                            <div id="articleHeading" class="articleHeading">
                                
                                
                                
	                            
    


<div class="breadcrumbs breadcrumbsBlock">
    
        <span class="breadcrumbItem" itemscope itemtype="http://data-vocabulary.org/Breadcrumb"> <a href="/" itemprop="url">
                <span itemprop="title">
                    
                        Wiadomości
                    
                </span>
            </a></span>
			
    
        <span class="breadcrumbItem" itemscope itemtype="http://data-vocabulary.org/Breadcrumb"><span class="divider">›</span> <a href="/swiat" itemprop="url">

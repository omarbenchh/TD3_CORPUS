(function () {
	var demo = {
		id : "crossiframe",
		width : 360,
		height : 195,
		obj : [{
			name :"news",
			url:"http://news.163.com/special/acrossthechannel/"
		},{
			name:"ent",
			url:"http://ent.163.com/special/acrossthechannel/"
		},{
			name:"sports",
			url:"http://sports.163.com/special/acrossthechannel/"

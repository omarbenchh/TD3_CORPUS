			 <div id='Ibnlive/IBN_India/IBN_India_Internal/IBN_India_Internal_660x80'>
				<script type='text/javascript'>
					googletag.cmd.push(function() { googletag.display('Ibnlive/IBN_India/IBN_India_Internal/IBN_India_Internal_660x80'); });

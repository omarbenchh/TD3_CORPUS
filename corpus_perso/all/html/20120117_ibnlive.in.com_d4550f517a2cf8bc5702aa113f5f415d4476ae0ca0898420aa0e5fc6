               var wintop = $(window).scrollTop();
               if (wintop >= ln_pos)
               {
                   $('#left_nav').addClass('ln_pfixed');
                   $('#contener_box').css('margin-left','140px');
                                                               
                     if (width<=1024){
                                    $('#contener_box').css('margin-left','0px');
                                    }
               }
               else
               {
                               $('#left_nav').removeClass('ln_pfixed');
                               $('#contener_box').css('margin-left','0px');
               }
               
});
if (width<=1024){
                $('.home_pcake').on('click', function(e){
                   e.preventDefault();

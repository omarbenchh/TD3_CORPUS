    } else {
        form9_show();
    }
}
function userRecommend(msg) {
    if(msg.status != 0) {
        alert(msg.body.msg);
    } else {
        var cnt = parseInt($("rmd_count").value)+1;
        $("rmd_d").innerHTML=cnt+"人";
        $("rmd_d").title="共有"+cnt+"人推荐此文章";

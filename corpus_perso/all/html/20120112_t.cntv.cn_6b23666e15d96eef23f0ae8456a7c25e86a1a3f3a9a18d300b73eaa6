<script type="text/javascript">
$(document).ready(function(){
	//防止点击评论在弹出输入框
	$("#cm").attr("rel","sdsd");
	$("#cmtCt li").live({
		   mouseenter:
		   function()
		   {
			  //todo
			   $(this).find(".msgdel").removeAttr("style");
				$(this).find(".c-for").removeAttr("style");
				$(this).find(".msgdel").attr({style:"display:block"});
				$(this).find(".c-for").attr({style:"display:block"});  
		   },
		   mouseleave:
		   function()
		   {
			  //todo
			   $(this).find(".msgdel").removeAttr("style");
				$(this).find(".c-for").removeAttr("style");
				$(this).find(".msgdel").attr({style:"display:none"});
				$(this).find(".c-for").attr({style:"display:none"});
		   }
		});
});
</script>

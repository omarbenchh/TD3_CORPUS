<script type="text/javascript">
$(function () {
				//下拉菜单
				jQuery(".list_btn").mouseover(function(){
				jQuery(this).find("a.menu-pub").attr("class","menu-pub-hover");
				jQuery(this).find("ul").show();
					}).mouseout(function(){	
						jQuery(this).find("a.menu-pub-hover").attr("class","menu-pub");
						jQuery(this).find("ul").hide();
				});
				
				// 浏览器插件页面不需要导航条
				//				//导航距离屏幕顶部距离
				var _defautlTop = $("#header").offset().top;
				
				var ulHeight = $("#container").height(); //UL高度
				var ulTop = $("#container").offset().top+ulHeight;
				//导航距离屏幕左侧距离
				var _defautlLeft = $("#header").offset().left - $(window).scrollLeft();				
		
				//导航默认样式记录，还原初始样式时候需要
				var _position = $("#header").css('position');
				var _left = $("#header").css('left');
				var _zIndex = $("#header").css('z-index');

				window.onscroll = function(){
					if(document.documentElement.scrollTop > _defautlTop){
						//alert(2);
						$("#header").addClass("headerfix");
					}else{
						//alert(1);
						$("#header").removeClass("headerfix");  
					}
				//返回顶部
					var isIE=!!window.ActiveXObject;
					var isIE6=isIE&&!window.XMLHttpRequest;
					if(isIE6){
						}else{
							var bd =document.documentElement || document.body;
											
							BackTopDiv=document.getElementById('back_top_div');
							SuggestionDiv=document.getElementById('suggestion_div');
							if(document.documentElement.scrollTop==0){
								BackTopDiv.style.display="none";
								SuggestionDiv.style.display="none";
							}else{
								BackTopDiv.style.display="block";
								SuggestionDiv.style.display="block";
							}
						}
				}
				
			  	//点击导航
				$("#header").click(function(){
				  $(window).scrollTop(_defautlTop);
				})
				
});
</script>

<script type='text/javascript'>
(function() {
    function createCookie(name,value,days) {
        if (days) {
            var date = new Date();
            date.setTime(date.getTime()+(days*24*60*60*1000));
            var expires = "; expires="+date.toGMTString();
        }
        else var expires = "";
        document.cookie = name+"="+value+expires+"; path=/; domain=.money.pl";
    }

    function readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
        }
        return null;
    }

    function eraseCookie(name) {
        createCookie(name,"",-1);
    }

    function getPlacement(e,isImage) {
        var element,name,selector;
        element = jQuery(e).parents('div:has(div.reklama,div.hd):first');
        if (element.length) {
            name = jQuery('.hd',element).text();
            if (!name) {
                if (jQuery('.reklama',element).length==1) {
                    name = 'Reklama';
                }
            }
            if (isImage) {
                selector = 'a:gt(0):has(img)';
            } else {
                selector = 'a:gt(0):not(:has(img))';
            }
            return {'element':element,'name':name,'links':jQuery(selector,element)}
        } else {
            element = jQuery(e).parents('#dzialy');
            if (element.length) {
                element = jQuery(e).closest('ul');
                name = jQuery(element).attr('id');
                return {'element':element,'name':name,'links':jQuery(element).children().children('a')}
            }
        }
        return false;
    }

    function getUser() {
        var user = readCookie('mnyuser');
        if (!user) {
            user = readCookie('usertrack');
            createCookie('mnyuser',user,30);
        }
        return user;
    }
})();
</script>

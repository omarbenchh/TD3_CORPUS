  <!-- script>
    jQuery(window).load(function() {var column3_scrollingAd = jQuery('#ad_column3_3');var column3_adTop = column3_scrollingAd.offset().top;
    function fixDiv() {
      var column3_stoppingHeight = jQuery('#footer').offset().top;
      if (jQuery(window).scrollTop() >= (column3_stoppingHeight - 700))
        column3_scrollingAd.css({
          'position': 'relative',
          'top': (column3_stoppingHeight - column3_adTop - 700) + 'px'
        });
      else if (jQuery(window).scrollTop() >= (column3_adTop - 10))
        column3_scrollingAd.css({
          'position': 'fixed',
          'top': '10px'
        });
      else
        column3_scrollingAd.css({
          'position': 'static',
          'top': 'auto'
        });
      }
      jQuery(window).scroll(fixDiv);
      fixDiv();
    });
  </script -->

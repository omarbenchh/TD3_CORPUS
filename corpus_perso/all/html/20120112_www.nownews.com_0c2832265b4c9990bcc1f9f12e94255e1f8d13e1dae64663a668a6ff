  <script>

    var setAdCookies = function() {

      // 設定 cookie
      var expireYear = new Date().getFullYear();
      var expireMonth = new Date().getMonth() + 1;
      var expireDate = new Date().getDate() + 1;

      var expireTimeUTC = new Date(expireYear + '-' + expireMonth + '-' + expireDate).toUTCString();

      document.cookie = 'hennessy_ad=hennessy_ad; expires=' + expireTimeUTC + ';';
    };

    var stage = new swiffy.Stage(document.getElementById('swiffycontainer'),
        swiffyobject, {});

    stage.start();

    var tag = document.createElement('script');

    tag.src = 'https://www.youtube.com/iframe_api';
    var firstScriptTag = document.getElementsByTagName('script')[0];
    firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

    var ad_player;
    function onYouTubeIframeAPIReady() {
        ad_player = new YT.Player('hennessy_ad_player', {
            height: '540',
            width: '960',
            videoId: 'Ns9fJ4otmeg',
            playerVars: { 
                'autoplay': 0,
                'controls': 0,
                'showinfo': 0,
                'vq': 'hd720'
            },
            events: {
                'onReady': onPlayerReady,
                'onStateChange': onPlayerStateChange
            }
        });
    }

    function onPlayerReady(event) {
        var cookieValue = document.cookie.replace(/(?:(?:^|.*;\s*)hennessy_ad\s*\=\s*([^;]*).*$)|^.*$/, '$1');
        if(cookieValue !== 'hennessy_ad') {
            setAdCookies();
            $('.play-box').show();
            event.target.playVideo();
        }
    };

    function onPlayerStateChange(event) {
        if(event.data === 0) {
            ad_player.stopVideo();
            $('.play-box').hide();
        }
    }

    $('.open').click(function() {
        $('.play-box').show();
        ad_player.seekTo(0);
        ad_player.playVideo();
    });

    $('.close').click(function() {
        $('.play-box').hide();
        ad_player.stopVideo();
    });
  </script>
</div>


                      <script type="text/javascript" src="http://api.youneeq.ca/scripts/legacyapiscripts/detect_timezone.js"></script>
<script type="text/javascript" src="http://api.youneeq.ca/app/yqmin"></script>
<script type="text/javascript">
	$(document).ready(function () {

		function on_yq_suggest(suggestions) {
			if (suggestions && suggestions.suggest && suggestions.suggest.node) {

			var nodeids = suggestions.suggest.node;
			var url = "example_suggestions?nodeids=" + nodeids.join(',');
			var stories = "";

			for (var i = 0; i < nodeids.length; i++) { 
			  //console.log('SUGGESTION' + [i], nodeids[i]);
			  //console.log('**articleTitle ' + [i], articleTitle);
			  //console.log('**articleLink ' + [i], articleLink); 


			  var articleTitle = nodeids[i].title;
			  var articleLink = nodeids[i].url;
			  stories += "<li><h3><a class='a-link-view' href='" + articleLink + "'>" + articleTitle + "</a></h3></li>";
				}

				stories = "<ul class='list'>" + stories + "</ul>";

				$("#youneeq").append(stories);

			}

		}

		function my_yq_init() {
			var name = $('meta[property="og:url"]').attr("content");//  <----------- change to this
			var title = $('meta[name=title]').attr("content");
			var content_id = name.substr(name.lastIndexOf('-') + 1);

			//var metaTitle = $('meta[name=title]').attr("content");
			//var TitleUrl = metaTitle + "|" + name;

			if (name.indexOf("1.") > 0) {

				//console.log('!!!!!!!YQ observing', title);

				Yq.observe({

					'domain': _sf_async_config.domain,//domain name
					'observe': [{ 'type': 'node', 'name': content_id, 'title': title, //the yqmin script captures the document.title by default, so this is only necessary if not using that script or passing in a different title 
					'categories': [_sf_async_config.sections], }, ],
					'suggest': [{ 'type': 'node', 'count': 6, 'categories': [], 'is_panel_builder': 'true', 'domains': ['timescolonist.com'] }]//   <-------- clear category filter 
				},

					on_yq_suggest);

			}
			else{

				Yq.observe({
					'domain': _sf_async_config.domain,//domain name
					'suggest': [{ 'type': 'node', 'count': 6, 'categories': [], 'is_panel_builder': 'true', 'domains': ['timescolonist.com'] }]//    <-------- clear category filter 
				},

			on_yq_suggest);

			}

		}

	Yq.onready(my_yq_init);

	})
</script>
                        

			        		break;
			        		
			        	case 'googlead-300x50_S1':
            
                            gptAdSlots['googlead-300x50_S1'] = googletag.defineSlot('/4326737/atex_TC_site_Sports_subsection_300x50_S1_RON_GNG-network', [300,50], 'googlead-300x50_S1')

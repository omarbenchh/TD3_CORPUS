                            .addService(googletag.pubads());
                            
                            break;
	
			        	case 'googlead-300x250_S1':
			
					      	gptAdSlots['googlead-300x250_S1'] = googletag.defineSlot('/4326737/atex_TC_site_section_subsection_300x250_S1_RON_GNG-network', [[300,600],[300, 250]], 'googlead-300x250_S1')

<script type='text/javascript'>
  var googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];
  (function() {
    var gads = document.createElement('script');
    gads.async = true;
    gads.type = 'text/javascript';
    var useSSL = 'https:' == document.location.protocol;
    gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
    var node = document.getElementsByTagName('script')[0];
    node.parentNode.insertBefore(gads, node);
  })();
</script>
<script>

  // GPT slots
  var gptAdSlots = [];
  
  window.gptDebug = false;
  
    
jQuery(document).ready(function(){
	
	var dfpslots = $("#container").find(".adunit-gpt").filter(":visible");

	
	if (dfpslots.length) {
		
		  window.gmgRespAdUnit = {
		  	mainContainerWidth: jQuery('#container > section[role="main"]').width(),
		  	refreshArr: [],
		  	refreshArrSlotName: []
		  }
	
		  googletag.cmd.push(function() {
		  	
		  	  //globally collapse empty ads
		  	  googletag.pubads().collapseEmptyDivs(true);
		  	  
		  	  // Define a size mapping object. The first parameter to addSize is
			  // a viewport size, while the second is a list of allowed ad sizes.
			  
		        
        		
						  var one_one_mapping = googletag.sizeMapping().
			  		addSize([375,300],[320,460]).
					addSize([768,200],[[1000,1050],[800,600]]).
					addSize([980,200],[[1200,1050],[800,600]]).
					addSize([1200,200],[[1950,1050],[800,600]]).build();
					
			  var rec_42_mapping = googletag.sizeMapping().
			  		addSize([700,200],[]).
			  		addSize([768,200],[800,42]).
			  		addSize([980,200],[960,42]).
			  		addSize([1200,200],[1200,42]).
			  		build();
						
			  var one_one_MLF_mapping = googletag.sizeMapping().
					addSize([768,200],[320,200]).
					addSize([980,200],[610,240]).
					addSize([1200,200],[845,240]).build();
			  
			  var vdy_polar_two_two_mapping = googletag.sizeMapping().
			  		addSize([0,0],[2,2]).build();
			  		
			  var box_50_mapping = googletag.sizeMapping().
                    addSize([0,0],[300,50]).build();
			  
			  var box_100_mapping = googletag.sizeMapping().
			  		addSize([0,0],[300,50]).
			  		addSize([700,200],[300,100]).build();
			  		
			  var box_150_mapping = googletag.sizeMapping().
			  		addSize([0,0],[300,150]).
			  		addSize([700,200],[300,150]).build();
			  		
			  var box_150_200_mapping = googletag.sizeMapping().
			  		addSize([0,0],[300,200]).
			  		addSize([700,200],[300,200]).build();
			  		
			  var box_250_mapping = googletag.sizeMapping().
			  		addSize([0,0],[300,50]).
			  		addSize([700,200],[300,250]).
			  		addSize([980,200],[[300,600],[300,250]]).build();
			  		
			  var lb_728_mapping = googletag.sizeMapping().
			  		addSize([0,0],[300,50]).
			  		addSize([768,200],[728,90]).build();
			  		
			  var rec_600_mapping = googletag.sizeMapping().
			  		addSize([700,200],[]).
			  		addSize([768,200],[800,600]).build();
		
		  					  		
				jQuery(dfpslots).each(function(){
					
					if (window.gptDebug) console.log('!!! GPTDEBUG dfpslot found: id->'+jQuery(this).attr('id'));
				
					switch(jQuery(this).attr('id')) {
					
						case 'googlead-728x90_S1':
							gptAdSlots['googlead-728x90_S1'] =  googletag.defineSlot('/4326737/atex_TC_site_section_subsection_728x90_S1_RON_GNG-network', [728, 90], 'googlead-728x90_S1')
					        .defineSizeMapping(lb_728_mapping)
					        .addService(googletag.pubads());
					        
					   			        		
			        		break;
			        		
			        	case 'googlead-728x90_S2':
			      
					      	gptAdSlots['googlead-728x90_S2'] = googletag.defineSlot('/4326737/atex_TC_site_section_subsection_728x90_S2_RON_GNG-network', [728, 90], 'googlead-728x90_S2')
					        .defineSizeMapping(lb_728_mapping)
					        .addService(googletag.pubads());
			      
							break;
							
						case 'googlead-728x90_S3':
			      
					      	gptAdSlots['googlead-728x90_S3'] = googletag.defineSlot('/4326737/atex_TC_site_section_subsection_728x90_S3_RON_GNG-network', [728, 90], 'googlead-728x90_S3')
					        .defineSizeMapping(lb_728_mapping)
					        .addService(googletag.pubads());
			      
							break;

						case 'googlead-800x600_S1':
					      	gptAdSlots['googlead-800x600_S1'] =  googletag.defineSlot('/4326737/atex_TC_site_section_subsection_800x600_S1_RON_GNG-network', [800, 600], 'googlead-800x600_S1')
					      	.defineSizeMapping(rec_600_mapping)
					        .addService(googletag.pubads());
					        
					        break;
						
						case 'div-gpt-ad-1425572185364-0-oop':					        
					        gptAdSlots['div-gpt-ad-1425572185364-0-oop'] =  googletag.defineOutOfPageSlot('/4326737/atex_TC_site_section_subsection_800x600_S1_RON_GNG-network', 'div-gpt-ad-1425572185364-0-oop')
					        .defineSizeMapping(rec_600_mapping)
					        .addService(googletag.pubads());
					        
					        break;
					        
					   case 'googlead-1x1_S1':
					        gptAdSlots['googlead-1x1_S1'] = googletag.defineSlot('/4326737/atex_TC_site_section_subsection_1x1_S1_RON_GNG-network', [[1, 1],[320,460]], 'googlead-1x1_S1')
					        .defineSizeMapping(one_one_mapping)
					        .addService(googletag.pubads());
					        
					        window.gmgRespAdUnit.refreshArr.push(gptAdSlots['googlead-1x1_S1']);
					        window.gmgRespAdUnit.refreshArrSlotName.push('googlead-1x1_S1');
					        
					        break;
					        
					   case 'googlead-1x1_S1-oop':
					        gptAdSlots['googlead-1x1_S1-oop'] = googletag.defineOutOfPageSlot('/4326737/atex_TC_site_section_subsection_1x1_S1_RON_GNG-network', 'googlead-1x1_S1-oop')
					        .defineSizeMapping(one_one_mapping)
					        .addService(googletag.pubads());
			        		
			        		window.gmgRespAdUnit.refreshArr.push(gptAdSlots['googlead-1x1_S1-oop']);
			        		window.gmgRespAdUnit.refreshArrSlotName.push('googlead-1x1_S1-oop');
			        		
			        		break;
			        		
			        	case 'googlead-960x42_S1-oop':
			        	
					      	gptAdSlots['googlead-960x42_S1-oop'] = googletag.defineOutOfPageSlot('/4326737/atex_TC_site_section_subsection_960x42_S1_RON_GNG-network', 'googlead-960x42_S1-oop')
					        .defineSizeMapping(rec_42_mapping)
					        .addService(googletag.pubads());
					        
					        window.gmgRespAdUnit.refreshArr.push(gptAdSlots['googlead-960x42_S1-oop']);

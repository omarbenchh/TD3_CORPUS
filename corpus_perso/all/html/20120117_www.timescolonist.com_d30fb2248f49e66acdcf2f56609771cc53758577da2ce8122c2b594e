			        		break;
			        		
			        	case 'googlead-960x42_S1':
			        	
					      	gptAdSlots['googlead-960x42_S1'] = googletag.defineSlot('/4326737/atex_TC_site_section_subsection_960x42_S1_RON_GNG-network', [960, 42], 'googlead-960x42_S1')

<script defer="defer">/**
 * @file
 * Injects ads into ew_com theme header.
 */
/* globals window, jQuery */
(function ($) {
  'use strict';

  if ($('#cm>.ads-content')) {
    $('#cm>.ads-content').html('<img width="100%" src="http://s1.wp.com/wp-content/themes/vip/ew-mobile2/assets/images/ad' + (window.innerWidth <= 320 ? 1 : 2) + '.jpg?m=1391161796g"></img>');
  }

  $(function () {
    var $page = $('#page'),
      $header_ad = $('#ad-global-header'),
      $header_ad_block = $('#ad-global-header>.block');

    function hideTopBanner() {
      window.setTimeout(function () {

        $(window).off('scroll', stickyAdHandler);

        if ($page.hasClass('snap') && $header_ad.hasClass('stick')) {
          $header_ad_block.slideUp(
            1000,
            function () {
              $page.removeClass('snap');
              $header_ad.removeClass('stick');
              $header_ad_block.show();
            }
          );
        }
        else if ($page.hasClass('snap')) {
          $page.removeClass('snap');
        }
      }, 5000);
    }

    hideTopBanner();

    /**
     * Add class "stick" to ad wrapper on scroll.
     */
    function stickyAdHandler() {
      var window_scroll_top = $(window).scrollTop(),
        div_offset_top = $('.ad-placeholder').offset().top;

      if( !$('body').hasClass('page-home') && $page.hasClass('snap') ) {
        if (window_scroll_top > div_offset_top) {
          $header_ad.addClass('stick');
        }
        else {
          $header_ad.removeClass('stick');
        }
      }
    }

    $(window).scroll(stickyAdHandler);

  });

})(jQuery);
</script>

   function recordOutboundLink(link, category, action, label) {
     try {
       ga('send', 'event', category, action, label);
       if (link.href)
           setTimeout('document.location = "' + link.href + '"', 200)
       else
           setTimeout('document.location = "' + link + '"', 200)

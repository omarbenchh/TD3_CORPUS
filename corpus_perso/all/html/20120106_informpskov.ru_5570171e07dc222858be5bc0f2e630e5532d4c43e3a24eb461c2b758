                });


                $("#jquery_jplayer_1").bind($.jPlayer.event.pause, function() {
                  if (!stopPlayer){
                    $.cookie("nebo7player", "pause", { path: "/" });
                  }
                  else stopPlayer = 0;

                  playerActive = 0;
                });

                testStopPlayer();
            });
            $(window).resize(function(){
                    $("#jp_container_1").css("margin-left", (jQuery(window).width()-$("#jp_container_1").width())/2);
            });
            window.onbeforeunload = function() {
                exitFromPage=1;
                $.removeCookie("nebo7player_stop", { path: "/" });
            }
        
});
/*]]>*/
</script>
</body>
</html> 
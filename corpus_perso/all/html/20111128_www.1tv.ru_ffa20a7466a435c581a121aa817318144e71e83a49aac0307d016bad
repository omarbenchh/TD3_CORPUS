             	$(function(){
             	 setPageUrls();	
                 function setPageUrls() {
                   $("#list_abc_search div.all_pagination a").click(function() {
                     $.get($(this).attr("href"), function(data){
                       $("#list_abc_search").empty().prepend(data);
             //          window.scroll(0,190);
                       setPageUrls();
                     });
                     return false;
                   });
                 }
                  }
                  );
             </script>
<script type="text/javascript">
$(document).ready(function() {
                $("#vote_shed").ajaxForm( {beforeSubmit:vote_shedValidate, target:  "#vote_rez", success: hide_vote} );});
                function getRadioGroupValue(radioGroupObj)
                {
                  for (var i=0; i < radioGroupObj.length; i++)
                    if (radioGroupObj[i].checked) return radioGroupObj[i].value;
                
                  return null;
                }
                function vote_shedValidate(formData, jqForm, options) {
                  var form = jqForm[0];
                  if (!getRadioGroupValue(form.radio01)) {
                    alert("Выберите ответ.");
                    return false;
                  }
                  return true;
                }
                function hide_vote(){
                  $("#vote_div").hide();
                  $("#vote_rez").show();
                }
                </script>

<script type='text/javascript'>
googletag.cmd.push(function() {
	googletag.pubads().enableAsyncRendering(); // enable async loading
	googletag.pubads().collapseEmptyDivs(); // if no ads loaded, kill their containers
	googletag.enableServices();
});

// check viewport for resize events and reload ads (1000ms delay)
var resizeTimer;
var width = window.innerWidth;

function resizer() {
	//Check to see if the width is changed 
	var newWidth = window.innerWidth;

	if (width != newWidth ) {
		googletag.pubads().refresh([window.MNT_Leaderboard_Top]);
		googletag.pubads().refresh([window.MNT_MREC_Right]);
		googletag.pubads().refresh([window.MNT_600xFlex_Bottom]);
		googletag.pubads().refresh([window.MNT_Flex_Right]);
		
		//Last , assign new width to width
		width = newWidth;
	}
}

window.addEventListener("resize", function(){ 
	clearTimeout(resizeTimer);
	resizeTimer = setTimeout(resizer, 1000);
});
</script>

	

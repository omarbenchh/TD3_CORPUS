			qst.hide();
			sensor.hide();
		})

		closeQst.click(function(){
			qst.hide();
			sensor.hide();
		})
	})

	function showQst(id) {
		id.blur();

		qst.css({
			left: "50%",
			margin: "0 0 0 -160px",
			top: "305px",
			display: "block"
		}); 

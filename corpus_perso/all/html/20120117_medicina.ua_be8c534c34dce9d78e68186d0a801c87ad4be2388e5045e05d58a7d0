		function valid_form_login(){
			var err = true;
			if($('#login').val() == ''){
				err = false;
				$('#err_login').show();
			}
			else{
				$('#err_login').hide();
			}
				
			if($('#pass').val() == ''){
				err = false;
				$('#err_pass').show();
			}
			else{
				$('#err_pass').hide();
			}
				
			if(err)
				document.form_login.submit();
		}
	var auth;
	var sensor;
		
	$(document).ready(function(){
		auth = $('#authDiv');
		sensor = $('#sensorShadow');
		closeAuth = $('#close');
		sensor.click(function(){
			auth.hide();
			sensor.hide();
		})
		closeAuth.click(function(){
			auth.hide();
			sensor.hide();
		})
		
	})

	function showAuth(id) {
		id.blur();
		auth.center().css({
			display: "block"
		}); 
		sensor.css({
			display: "block",
			height: $(document).height() + "px",
			opacity: 0.5
		});
	}

	var qst;
	var sensor;
	$(document).ready(function(){

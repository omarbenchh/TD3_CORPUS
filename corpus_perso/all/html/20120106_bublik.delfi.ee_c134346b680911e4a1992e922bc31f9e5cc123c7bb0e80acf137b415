<script type="text/javascript">
function formatOnAirEvents(){
	var container = $('#tvonair');
	// Clear floating
	$('<div/>').addClass('clr').appendTo(container);
	// Remove right border from last column
	container.css('width',container.width()-2).find('.onair_channel_block:last').css({'border-right-width': '0px','width': container.find('.onair_channel_block:last').width()-2});
	var height = container.height();
	container.find('.onair_channel_block').each(function (i, block){
		// change columns height to have separator line from top to bottom
		$(block).height(height);
		// remove bottom border from last element in each column
		$(block).find('.onair_event:last').css('border', 0);
		$(block).find('.onair_title').wrapInner('<a href="http://programma.delfi.ee" />');
	});
	//set logo sizes (not viable as it is done afther document onliad)
	//$(".onair_channel img").each(function () {
	//	$(this).attr('widht', this.width);
    //    $(this).attr('height', this.height);
    //});
}
$(document).ready(function (){
	delfitvonair.language_id = 131;
	delfitvonair.service_url = "http://kava.delfi.ee/tvguide/fe/service.php";
	delfitvonair.channel_ids = [135, 136, 32, 106, 198, 199];
	delfitvonair.placeholder = 'tvonair';
	delfitvonair.limit = 4;
	delfitvonair.format_callback = formatOnAirEvents;
	delfitvonair.requestEvents();
});
</script>


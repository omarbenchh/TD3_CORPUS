<script type="text/javascript">
(function(){
var scrollObj = document.getElementById('scrollBox');
var JMark = document.getElementById('JMark');
if(!scrollObj) return;
var tempObj = JMark;
var ot = JMark.offsetTop;
function getOFTop(){
ot = JMark.offsetTop;
if(pc.browser.ie6 || pc.browser.ie7){
while(tempObj.parentNode){
tempObj = tempObj.parentNode;
if(tempObj.tagName == 'BODY') break;
ot += parseInt(tempObj.offsetTop);
}
tempObj = JMark;
}
}
window.onscroll = window.onresize = function(){
var cName = scrollObj.className;
if(!scrollObj) return true;
var st = document.documentElement.scrollTop+document.body.scrollTop;
//alert((st + document.documentElement.clientHeight) > ot)
getOFTop();
if(st > ot){
if(cName.indexOf('fixTop') == -1) scrollObj.className += ' fixTop';
}else{
scrollObj.className = scrollObj.className.replace(' fixTop','');
}
}
})()
function cmtCount(){
var HTML='';
HTML ='<a href="'+ getTopicUrl() +'" target="_blank" >(评论<em class="red">'+ getTopicTotal() +'</em>条)</a>';
document.getElementById("cmtCount").innerHTML=HTML;
}
window.onload=function(){cmtCount()}
</script>
